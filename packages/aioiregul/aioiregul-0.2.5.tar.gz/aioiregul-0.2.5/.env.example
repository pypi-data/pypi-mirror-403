# IRegul Socket Client Configuration
# Copy this file to .env and fill in your device details

# Device connection settings
IREGUL_HOST=i-regul.fr
IREGUL_PORT=443
IREGUL_DEVICE_ID=your_device_id
IREGUL_PASSWORD_V2=your_device_key
IREGUL_PASSWORD_V1=your_password_v1

# Optional: Socket client settings
IREGUL_TIMEOUT=60
