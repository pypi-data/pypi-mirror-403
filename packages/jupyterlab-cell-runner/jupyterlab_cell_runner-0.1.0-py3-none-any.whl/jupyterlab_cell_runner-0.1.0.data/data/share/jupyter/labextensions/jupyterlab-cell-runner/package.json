{
  "name": "jupyterlab-cell-runner",
  "version": "0.1.0",
  "description": "JupyterLab extension for BlocklyBot - programmatic cell writing and execution via postMessage",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "license": "MIT",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.js",
  "scripts": {
    "build": "tsc",
    "build:labextension": "jupyter labextension build .",
    "build:all": "tsc && jupyter labextension build .",
    "clean": "rimraf lib tsconfig.tsbuildinfo jupyterlab_cell_runner/labextension",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^4.0.0",
    "@jupyterlab/cells": "^4.0.0",
    "@jupyterlab/notebook": "^4.0.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.5.3",
    "rimraf": "^5.0.0",
    "typescript": "~5.2.0"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "jupyterlab_cell_runner/labextension",
    "_build": {
      "load": "static\\remoteEntry.917c52fc1b1ef18ceaef.js",
      "extension": "./extension",
      "style": "./style"
    }
  }
}
