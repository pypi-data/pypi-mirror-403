[
  {
    "type": "DbfetchRESTTool",
    "name": "dbfetch_fetch_entry",
    "description": "Fetch a single database entry by ID from various databases (UniProt, PDB, etc.) in specified format. Supports FASTA, XML, JSON, and other formats.",
    "parameter": {
      "type": "object",
      "properties": {
        "db": {
          "type": "string",
          "description": "Database name (e.g., 'uniprotkb', 'pdb', 'embl', 'ensembl')"
        },
        "id": {
          "type": "string",
          "description": "Entry ID (e.g., UniProt accession 'P05067', PDB ID '1A2B')"
        },
        "format": {
          "type": "string",
          "description": "Output format (e.g., 'fasta', 'xml', 'json', 'txt'). Default: 'fasta'",
          "default": "fasta",
          "enum": ["fasta", "xml", "json", "txt", "gff", "embl", "genbank"]
        }
      },
      "required": ["db", "id"]
    },
    "fields": {
      "endpoint": "https://www.ebi.ac.uk/Tools/dbfetch/dbfetch",
      "return_format": "TEXT"
    },
    "return_schema": {
      "type": "object",
      "properties": {
        "status": {
          "type": "string"
        },
        "data": {
          "type": "string",
          "description": "Database entry in requested format"
        },
        "content_type": {
          "type": "string"
        },
        "url": {
          "type": "string"
        }
      }
    },
    "test_examples": [
      {
        "db": "uniprotkb",
        "id": "P05067",
        "format": "fasta"
      },
      {
        "db": "pdb",
        "id": "1A2B",
        "format": "xml"
      }
    ]
  },
  {
    "type": "DbfetchRESTTool",
    "name": "dbfetch_fetch_batch",
    "description": "Fetch multiple database entries by IDs in batch. Supports comma-separated IDs or list of IDs. Returns entries in specified format.",
    "parameter": {
      "type": "object",
      "properties": {
        "db": {
          "type": "string",
          "description": "Database name"
        },
        "ids": {
          "type": ["string", "array"],
          "description": "Comma-separated IDs or list of IDs (e.g., 'P05067,P04637' or ['P05067', 'P04637'])"
        },
        "format": {
          "type": "string",
          "description": "Output format (default: 'fasta')",
          "default": "fasta"
        }
      },
      "required": ["db", "ids"]
    },
    "fields": {
      "endpoint": "https://www.ebi.ac.uk/Tools/dbfetch/dbfetch",
      "return_format": "TEXT"
    },
    "return_schema": {
      "type": "object",
      "properties": {
        "status": {
          "type": "string"
        },
        "data": {
          "type": "string",
          "description": "Batch of database entries"
        },
        "count": {
          "type": "integer",
          "description": "Number of entries retrieved"
        }
      }
    },
    "test_examples": [
      {
        "db": "uniprotkb",
        "ids": "P05067,P04637",
        "format": "fasta"
      }
    ]
  },
  {
    "type": "DbfetchRESTTool",
    "name": "dbfetch_list_databases",
