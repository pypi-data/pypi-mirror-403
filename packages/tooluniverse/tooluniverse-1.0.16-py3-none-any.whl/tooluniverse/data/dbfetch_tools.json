    ]
  },
  {
    "type": "DbfetchRESTTool",
    "name": "dbfetch_list_formats",
    "description": "List available output formats for a specific database. Returns supported formats for the database.",
    "parameter": {
      "type": "object",
      "properties": {
        "db": {
          "type": "string",
          "description": "Database name"
        }
      },
      "required": ["db"]
    },
    "fields": {
      "endpoint": "https://www.ebi.ac.uk/Tools/dbfetch/dbfetch",
      "return_format": "TEXT"
    },
    "return_schema": {
      "type": "object",
      "properties": {
        "status": {
          "type": "string"
        },
        "data": {
          "type": "string",
          "description": "List of available formats"
        }
      }
    },
    "test_examples": [
