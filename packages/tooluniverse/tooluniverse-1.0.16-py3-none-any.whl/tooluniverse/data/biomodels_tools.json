[
  {
    "name": "biomodels_search",
    "type": "BioModelsRESTTool",
    "description": "Search for computational biological models in the EBI BioModels database by keyword. Returns models with identifiers, names, formats (typically SBML), and URLs. Use this to find systems biology models related to specific biological processes (e.g., 'glycolysis', 'apoptosis', 'cell cycle'), diseases, or pathways. Find model IDs to use with other BioModels tools for detailed information and downloads.",
    "parameter": {
      "type": "object",
      "properties": {
        "query": {
          "type": "string",
          "description": "Search query for biological models"
        },
        "limit": {
          "type": "integer",
          "description": "Maximum number of results to return",
          "default": 10
        }
      },
      "required": [
        "query"
      ]
    },
    "return_schema": {
      "type": "object",
      "additionalProperties": true,
      "properties": {
        "status": {
          "type": "string",
          "enum": [
            "success",
            "error"
          ]
        },
        "data": {
          "type": "object",
          "additionalProperties": true,
          "description": "BioModels API search response",
          "properties": {
            "models": {
              "type": "array",
              "description": "Array of biological model objects",
              "items": {
                "type": "object",
                "properties": {
                  "id": {"type": "string", "description": "Model identifier (e.g., BIOMD0000000469)"},
                  "name": {"type": "string", "description": "Model name/title"},
                  "format": {"type": "string", "description": "Model format (usually SBML)"},
                  "publicationId": {"type": "string"},
                  "submissionId": {"type": "string"}
                }
              }
            },
            "matches": {"type": "integer", "description": "Total number of matching models"}
          }
        },
        "count": {
          "type": [
            "integer",
            "null"
          ],
          "description": "Number of models in the matches field"
        },
        "url": {
          "type": [
            "string",
            "null"
          ]
        },
        "error": {
          "type": [
            "string",
            "null"
          ]
        }
      }
    },
    "test_examples": [
      {
        "query": "glycolysis",
        "limit": 2
      },
      {
        "query": "BIOMD",
        "limit": 1
      }
    ],
    "fields": {
      "endpoint": "https://www.ebi.ac.uk/biomodels/search",
      "method": "GET",
      "params": {
        "format": "json"
      }
    }
  },
  {
    "type": "BioModelsRESTTool",
    "name": "BioModels_get_model",
    "description": "Get comprehensive metadata for a specific BioModels entry by its model identifier. Returns detailed information including model name, description, publication references, organism, biological processes, SBML level/version, creation/modification dates, and curator annotations. Use this to understand model details, citations, and biological context before downloading or using a model. Find model IDs using biomodels_search (e.g., 'BIOMD0000000469', 'MODEL1707110000').",
    "parameter": {
      "type": "object",
      "properties": {
        "model_id": {
          "type": "string",
          "description": "BioModels identifier (e.g., 'BIOMD0000000469', 'MODEL1707110000'). Find IDs using biomodels_search. Optionally append '.N' for a specific revision (e.g., 'BIOMD0000000469.1')."
        }
      },
      "required": [
        "model_id"
      ]
    },
    "fields": {
      "endpoint": "https://www.ebi.ac.uk/biomodels/{model_id}",
      "method": "GET",
      "params": {
        "format": "json"
      }
    },
    "return_schema": {
