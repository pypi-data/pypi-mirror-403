[
    {
        "name": "DGIdb_get_drug_gene_interactions",
        "description": "Get drug-gene interactions for a list of genes from DGIdb. Returns drugs targeting each gene, interaction types, and evidence sources.",
        "parameter": {
            "type": "object",
            "properties": {
                "genes": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "description": "List of gene symbols (e.g., ['EGFR', 'BRAF', 'KRAS'])."
                },
                "interaction_sources": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "description": "Optional filter by data sources (e.g., ['DrugBank', 'ChEMBL'])."
                },
                "interaction_types": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    },
                    "description": "Optional filter by interaction types (e.g., ['inhibitor', 'antagonist'])."
                }
            },
            "required": [
                "genes"
            ]
        },
        "fields": {
            "operation": "interactions"
        },
        "type": "DGIdbTool",
        "test_examples": [
            {
                "genes": [
                    "EGFR"
                ]
            },
            {
                "genes": [
                    "BRAF",
                    "KRAS"
                ],
                "interaction_types": [
                    "inhibitor"
                ]
            }
        ],
        "return_schema": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "object",
                    "properties": {
                        "genes": {
                            "type": "object",
                            "properties": {
                                "nodes": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "name": {
                                                "type": "string"
                                            },
                                            "longName": {
                                                "type": "string"
                                            },
                                            "interactions": {
                                                "type": "array",
                                                "items": {
                                                    "type": "object",
                                                    "properties": {
                                                        "drug": {
                                                            "type": "object"
                                                        },
                                                        "interactionTypes": {
                                                            "type": "array"
                                                        },
                                                        "sources": {
                                                            "type": "array"
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    {
