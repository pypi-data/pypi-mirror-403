[
  {
    "type": "ENABrowserRESTTool",
    "name": "ena_get_sequence_fasta",
    "description": "Get nucleotide sequence in FASTA format from ENA by accession number. Returns sequence with header information.",
    "parameter": {
      "type": "object",
      "properties": {
        "accession": {
          "type": "string",
          "description": "ENA accession number (e.g., 'U00096', 'AJ312385')"
        },
        "download": {
          "type": "boolean",
          "description": "Force download instead of inline display (default: false)",
          "default": false
        }
      },
      "required": ["accession"]
    },
    "fields": {
      "endpoint": "https://www.ebi.ac.uk/ena/browser/api/fasta/{accession}",
      "return_format": "TEXT"
    },
    "return_schema": {
      "type": "object",
      "properties": {
        "status": {
          "type": "string"
        },
        "data": {
          "type": "string",
          "description": "FASTA sequence"
        },
        "count": {
          "type": "integer",
          "description": "Number of sequences"
        },
        "content_type": {
          "type": "string"
        }
      }
    },
    "test_examples": [
      {
        "accession": "U00096"
      },
      {
        "accession": "AJ312385"
      }
    ]
  },
