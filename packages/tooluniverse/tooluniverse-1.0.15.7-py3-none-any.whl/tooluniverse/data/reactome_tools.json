  {
    "name": "Reactome_get_pathway_reactions",
    "description": "Query all reactions and subpathways contained under a Pathway using Pathway Stable ID. Returns list of events (reactions and subpathways) contained in the pathway, including their metadata and relationships.",
    "endpoint": "/data/pathway/{stId}/containedEvents",
    "method": "GET",
    "parameter": {
      "type": "object",
      "properties": {
        "stId": {
          "type": "string",
          "description": "Pathway Stable ID (e.g., 'R-HSA-73817'). To find pathway IDs, use Reactome_list_top_pathways to get top-level pathways, or Reactome_map_uniprot_to_pathways if you have a UniProt protein ID."
        }
      },
      "required": ["stId"]
    },
    "type": "ReactomeRESTTool",
    "test_examples": [
      {
        "stId": "R-HSA-73817"
      }
    ],
    "return_schema": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "dbId": {"type": "integer", "description": "Database ID"},
          "displayName": {"type": "string", "description": "Event display name"},
          "stId": {"type": "string", "description": "Stable identifier"},
          "stIdVersion": {"type": "string", "description": "Versioned stable ID"},
          "isInDisease": {"type": "boolean", "description": "Whether event is associated with disease"},
          "isInferred": {"type": "boolean", "description": "Whether event is inferred"},
          "maxDepth": {"type": "integer", "description": "Maximum depth"},
          "name": {"type": "array", "items": {"type": "string"}, "description": "Event names"},
          "releaseDate": {"type": "string", "description": "Release date"},
          "speciesName": {"type": "string", "description": "Species name"},
          "eventOf": {"type": "array", "items": {"type": "object"}, "description": "Parent events"},
          "category": {"type": "string", "description": "Event category"},
          "schemaClass": {"type": "string", "description": "Schema class (e.g., 'Reaction', 'Pathway')"},
          "className": {"type": "string", "description": "Class name"}
        },
        "description": "Event objects (reactions or subpathways) contained in the pathway"
      }
    }
  },
