        "data": {
          "type": ["object", "null"],
          "description": "Available versions for a base matrix ID.",
          "required": ["count", "results"],
          "properties": {
            "count": { "type": "integer" },
            "next": { "type": ["string", "null"] },
            "previous": { "type": ["string", "null"] },
            "results": {
              "type": "array",
              "items": {
                "type": "object",
                "required": [
                  "matrix_id",
                  "base_id",
                  "name",
                  "collection",
                  "version",
                  "url"
                ],
                "properties": {
                  "matrix_id": { "type": "string" },
                  "base_id": { "type": "string" },
                  "name": { "type": "string" },
                  "collection": { "type": "string" },
                  "version": { "type": ["string", "integer"] },
                  "url": { "type": "string" },
                  "sequence_logo": { "type": ["string", "null"] }
                },
                "additionalProperties": true
              }
            }
          },
          "additionalProperties": true
        },
        "url": { "type": ["string", "null"] },
        "error": { "type": ["string", "null"] }
      },
      "additionalProperties": true
    },
    "test_examples": [
      { "base_id": "MA0002", "page": 1, "page_size": 5 }
    ]
  },
