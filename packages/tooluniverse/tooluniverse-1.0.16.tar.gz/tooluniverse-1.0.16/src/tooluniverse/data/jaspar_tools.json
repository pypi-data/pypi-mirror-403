[
  {
    "type": "JASPARRESTTool",
    "name": "JASPAR_get_transcription_factors",
    "description": "List transcription factor binding site matrices (PFMs/PWMs metadata) from JASPAR. Use this to browse matrices and discover `matrix_id` values for downstream tools like `jaspar_get_matrix`.",
    "parameter": {
      "type": "object",
      "properties": {
        "collection": {
          "type": "string",
          "default": "CORE",
          "description": "JASPAR collection (e.g., CORE)."
        },
        "page": {
          "type": "integer",
          "default": 1,
          "description": "Page number (1-based)."
        },
        "page_size": {
          "type": "integer",
          "default": 20,
          "description": "Results per page (JASPAR `page_size`)."
        }
      }
    },
    "fields": {
      "endpoint": "https://jaspar.elixir.no/api/v1/matrix/",
      "return_format": "JSON",
      "use_params": true
    },
    "return_schema": {
      "type": "object",
      "required": ["status"],
      "properties": {
        "status": { "type": "string", "enum": ["success", "error"] },
        "data": {
          "type": ["object", "null"],
          "description": "Paginated list of JASPAR matrices (summary metadata).",
          "required": ["count", "results"],
          "properties": {
            "count": { "type": "integer" },
            "next": { "type": ["string", "null"] },
            "previous": { "type": ["string", "null"] },
            "results": {
              "type": "array",
              "items": {
                "type": "object",
                "required": [
                  "matrix_id",
                  "base_id",
                  "name",
                  "collection",
                  "version",
                  "url"
                ],
                "properties": {
                  "matrix_id": { "type": "string" },
                  "base_id": { "type": "string" },
                  "name": { "type": "string" },
                  "collection": { "type": "string" },
                  "version": { "type": ["string", "integer"] },
                  "url": { "type": "string" },
                  "sequence_logo": { "type": ["string", "null"] }
                },
                "additionalProperties": true
              }
            }
          },
          "additionalProperties": true
        },
        "url": { "type": ["string", "null"] },
        "error": { "type": ["string", "null"] }
      },
      "additionalProperties": true
    },
    "test_examples": [
      { "collection": "CORE", "page": 1, "page_size": 5 }
    ]
  },
  {
    "type": "JASPARRESTTool",
    "name": "jaspar_get_matrix",
    "description": "Get full details for a single JASPAR matrix by `matrix_id` (e.g., MA0002.2). Returns metadata plus PFM/PWM. For reproducible analyses, use `jaspar_get_matrix_versions` with the corresponding `base_id` (e.g., MA0002) to see all versions and choose/cite the exact version you used.",
    "parameter": {
      "type": "object",
      "properties": {
        "matrix_id": {
          "type": "string",
          "description": "JASPAR matrix ID (e.g., 'MA0002.2'). You can discover IDs with `JASPAR_get_transcription_factors`."
        }
      },
      "required": ["matrix_id"]
    },
    "fields": {
      "endpoint": "https://jaspar.elixir.no/api/v1/matrix/{matrix_id}",
      "return_format": "JSON"
    },
    "return_schema": {
      "type": "object",
      "required": ["status"],
      "properties": {
        "status": { "type": "string", "enum": ["success", "error"] },
        "data": {
          "type": ["object", "null"],
          "description": "Full matrix detail including motif counts (PFM) and metadata.",
          "required": ["matrix_id", "base_id", "name", "collection", "version", "pfm"],
          "properties": {
            "matrix_id": { "type": "string" },
            "base_id": { "type": "string" },
            "name": { "type": "string" },
            "collection": { "type": "string" },
            "version": { "type": ["string", "integer"] },
            "pfm": {
              "type": "object",
              "description": "Position frequency matrix: A/C/G/T arrays of counts per position.",
              "required": ["A", "C", "G", "T"],
              "properties": {
                "A": { "type": "array", "items": { "type": "number" } },
                "C": { "type": "array", "items": { "type": "number" } },
                "G": { "type": "array", "items": { "type": "number" } },
                "T": { "type": "array", "items": { "type": "number" } }
              },
              "additionalProperties": true
            },
            "tax_group": { "type": ["string", "null"] },
            "species": {
              "type": ["array", "null"],
              "items": {
                "type": "object",
                "properties": {
                  "tax_id": { "type": ["integer", "string"] },
                  "name": { "type": "string" }
                },
                "additionalProperties": true
              }
            },
            "class": { "type": ["array", "null"], "items": { "type": "string" } },
            "sequence_logo": { "type": ["string", "null"] },
            "versions_url": { "type": ["string", "null"] },
            "sites_url": { "type": ["string", "null"] }
          },
          "additionalProperties": true
        },
        "url": { "type": ["string", "null"] },
        "error": { "type": ["string", "null"] }
      },
      "additionalProperties": true
    },
    "test_examples": [
      { "matrix_id": "MA0002.2" }
    ]
  },
  {
    "type": "JASPARRESTTool",
