    "description": "List all versions available for a JASPAR matrix base ID (e.g., base_id MA0002). This is useful for reproducibility (choose a specific matrix version like MA0002.2) and for tracking updates across releases.",
    "parameter": {
      "type": "object",
      "properties": {
        "base_id": {
          "type": "string",
          "description": "JASPAR base matrix ID without version (e.g., 'MA0002'). You can get `base_id` from `jaspar_get_matrix`."
        },
        "page": { "type": "integer", "default": 1, "description": "Page number (1-based)." },
        "page_size": { "type": "integer", "default": 20, "description": "Results per page." }
      },
      "required": ["base_id"]
    },
    "fields": {
      "endpoint": "https://jaspar.elixir.no/api/v1/matrix/{base_id}/versions/",
