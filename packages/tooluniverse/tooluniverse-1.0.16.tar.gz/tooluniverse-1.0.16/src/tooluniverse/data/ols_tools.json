[
  {
    "type": "OLSTool",
    "name": "ols_search_terms",
    "description": "Search for terms in OLS (Ontology Lookup Service)",
    "parameter": {
      "type": "object",
      "properties": {
        "operation": {
          "type": "string",
          "const": "search_terms",
          "description": "The operation to perform (search_terms)"
        },
        "query": {
          "type": "string",
          "description": "The search query for terms"
        },
        "rows": {
          "type": "integer",
          "description": "Number of results to return (default: 10)",
          "default": 10
        },
        "ontology": {
          "type": "string",
          "description": "Filter by specific ontology (optional)"
        },
        "exact_match": {
          "type": "boolean",
          "description": "Search for exact matches only (default: false)",
          "default": false
        },
        "include_obsolete": {
          "type": "boolean",
          "description": "Include obsolete terms (default: false)",
          "default": false
        }
      },
      "required": [
        "operation",
        "query"
      ]
    },
    "examples": [
      {
        "description": "Search for disease terms",
        "arguments": {
          "operation": "search_terms",
          "query": "diabetes",
          "rows": 10
        }
      },
      {
        "description": "Search for exact term in specific ontology",
        "arguments": {
          "operation": "search_terms",
          "query": "cancer",
          "ontology": "efo",
          "exact_match": true
        }
      }
    ],
    "tags": [
      "ontology",
      "search",
      "ebi",
      "ols"
    ],
    "author": "EBI OLS",
    "version": "1.0.0",
    "test_examples": [
      {
        "operation": "search_terms",
        "query": "gene:TP53",
        "rows": 10,
        "ontology": "efo",
        "exact_match": false,
        "include_obsolete": false
      }
    ],
    "return_schema": {
      "type": "object",
      "properties": {
        "response": {
          "type": "object",
          "properties": {
            "docs": {
              "type": "array"
            },
            "numFound": {
              "type": "integer"
            },
            "start": {
              "type": "integer"
            }
          }
        },
        "responseHeader": {
          "type": "object",
          "properties": {
            "QTime": {
              "type": "integer"
            },
            "status": {
              "type": "integer"
            }
          }
        },
        "facet_counts": {
          "type": "object",
          "properties": {
            "facet_fields": {
              "type": "object",
              "properties": {
                "ontologyPreferredPrefix": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "isDefiningOntology": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "ontologyId": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "ontologyIri": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "isObsolete": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "type": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "query": {
          "type": "string"
        },
        "filters": {
          "type": "object",
          "properties": {
            "ontology": {
              "type": "string"
            },
            "exact_match": {
              "type": "boolean"
            },
            "include_obsolete": {
              "type": "boolean"
            }
          }
        }
      }
    }
  },
