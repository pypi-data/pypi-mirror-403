<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.hijacksecurity.intercept-agent</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/intercept-agent</string>
        <string>collect</string>
        <string>--report</string>
    </array>
    <key>StartInterval</key>
    <integer>3600</integer>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <false/>
    <key>StandardOutPath</key>
    <string>/tmp/intercept-agent.stdout.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/intercept-agent.stderr.log</string>
</dict>
</plist>
