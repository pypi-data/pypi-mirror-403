{
  "name": "@fumadocs/ui",
  "version": "16.4.8",
  "description": "The low-level primitives for Fumadocs UI.",
  "keywords": [
    "Docs",
    "Fumadocs"
  ],
  "homepage": "https://fumadocs.dev",
  "license": "MIT",
  "author": "Fuma Nama",
  "repository": "github:fuma-nama/fumadocs",
  "files": [
    "css/*",
    "dist/*"
  ],
  "type": "module",
  "exports": {
    "./css/*": "./css/*",
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "next-themes": "^0.4.6",
    "postcss-selector-parser": "^7.1.1",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@types/bun": "^1.3.6",
    "@types/lodash.merge": "^4.6.9",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "class-variance-authority": "^0.7.1",
    "lodash.merge": "^4.6.2",
    "next": "16.1.4",
    "tailwindcss": "^4.1.18",
    "tsdown": "^0.19.0",
    "@fumadocs/cli": "1.2.2",
    "eslint-config-custom": "0.0.0",
    "fumadocs-core": "16.4.8",
    "tsconfig": "0.0.0"
  },
  "peerDependencies": {
    "@types/react": "*",
    "next": "16.x.x",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.0.0",
    "fumadocs-core": "16.4.8"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "@types/react": {
      "optional": true
    },
    "tailwindcss": {
      "optional": true
    }
  },
  "scripts": {
    "build": "tsdown",
    "clean": "rimraf dist",
    "dev": "tsdown --watch",
    "lint": "eslint .",
    "types:check": "tsc --noEmit"
  }
}