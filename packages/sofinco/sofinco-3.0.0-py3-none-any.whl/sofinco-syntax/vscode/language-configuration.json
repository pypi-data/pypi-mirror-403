{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    { "open": "{", "close": "}" },
    { "open": "[", "close": "]" },
    { "open": "(", "close": ")" },
    { "open": "\"", "close": "\"" },
    { "open": "'", "close": "'" }
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "indentationRules": {
    "increaseIndentPattern": "^\\s*(nakko|nakkopa|narekko|ulangi|sedding|pangngaseng|kelas|coba|nakkosala|nakkosedding|siaganga):.*$",
    "decreaseIndentPattern": "^\\s*(narekko|nakkopa|nakkosala|nakkosedding).*$"
  },
  "folding": {
    "markers": {
      "start": "^\\s*(nakko|ulangi|sedding|pangngaseng|kelas|coba|siaganga):",
      "end": "^\\s*$"
    }
  }
}
