    a owl:DatatypeProperty ;
    rdfs:comment "The standard of measurement for a quantitative value, such as 'Million tonnes CO2e' or 'Percentage'." ;
    rdfs:domain [
        a owl:Class ;
        owl:unionOf ( ont:EnvironmentalMetric ont:Metric ont:MetricResult ont:Target )
    ] ;
    rdfs:range xsd:token .

ont:hasUpdateDate
    a owl:DatatypeProperty , owl:FunctionalProperty ;
    rdfs:comment "The date on which a policy or document was most recently revised." ;
    rdfs:domain ont:Policy ;
    rdfs:range xsd:string .

ont:hasUpdateYear
    a owl:DatatypeProperty , owl:FunctionalProperty ;
    rdfs:comment "The calendar year in which a document or policy was most recently revised." ;
    rdfs:domain ont:Policy ;
    rdfs:range xsd:gYear ;
    rdfs:subPropertyOf ont:hasYear .

ont:hasValue
    a owl:DatatypeProperty , owl:FunctionalProperty ;
    rdfs:comment "The quantitative or numerical amount of a specific measurement, metric, or target." ;
    rdfs:domain [
        a owl:Class ;
        owl:unionOf ( ont:CapitalIncrease ont:CapitalStructure ont:Dividend ont:FinancialMetric ont:Metric ont:MetricReport ont:MetricResult ont:SharePrice ont:ShareSale ont:Target )
    ] ;
    rdfs:range [
        a rdfs:Datatype ;
        owl:unionOf ( xsd:decimal xsd:integer xsd:nonNegativeInteger )
    ] .

ont:hasVirginPlasticReduction
    a owl:DatatypeProperty , owl:FunctionalProperty ;
    rdfs:comment "The percentage reduction in the use of newly manufactured plastic compared to a baseline year." ;
    rdfs:domain ont:Company ;
    rdfs:range xsd:decimal .

ont:hasVoluntaryProductRecalls
    a owl:DatatypeProperty , owl:FunctionalProperty ;
    rdfs:comment "The number of instances where a company voluntarily removed a product from the market due to safety, quality, or compliance concerns." ;
    rdfs:domain ont:Company ;
    rdfs:range xsd:nonNegativeInteger .

ont:hasVotingRightsPercentage
    a owl:DatatypeProperty , owl:FunctionalProperty ;
    rdfs:comment "The proportion of total voting power held by a shareholder, a critical metric for assessing corporate control and governance." ;
    rdfs:domain ont:Shareholding ;
    rdfs:range xsd:decimal .

ont:hasWaterConsumedInHighStressRegions
    a owl:DatatypeProperty , owl:FunctionalProperty ;
    rdfs:comment "The percentage of a company's total water consumption that occurs in regions identified as having high or extremely high baseline water stress." ;
    rdfs:domain ont:Company ;
    rdfs:range xsd:decimal .

ont:hasWaterConsumptionInWaterRiskAreas
    a owl:DatatypeProperty , owl:FunctionalProperty ;
    rdfs:comment "The total volume of water consumed by facilities located in areas identified as having significant water-related risks." ;
    rdfs:domain ont:Company ;
    rdfs:range xsd:decimal .

ont:hasWaterUseReduction
    a owl:DatatypeProperty , owl:FunctionalProperty ;
    rdfs:comment "The reduction in water withdrawal per tonne of product, expressed in million cubic meters, compared to a baseline." ;
    rdfs:domain ont:Company ;
    rdfs:range xsd:decimal .

ont:hasWaterWithdrawnInHighStressRegions
    a owl:DatatypeProperty , owl:FunctionalProperty ;
    rdfs:comment "The percentage of a company's total water withdrawal that occurs in regions identified as having high or extremely high baseline water stress." ;
    rdfs:domain ont:Company ;
    rdfs:range xsd:decimal .

ont:hasWomenInManagement
    a owl:DatatypeProperty , owl:FunctionalProperty ;
    rdfs:comment "The percentage of managerial positions within the company that are held by women." ;
    rdfs:domain ont:Company ;
    rdfs:range xsd:decimal .

ont:hasYear
    a owl:DatatypeProperty , owl:FunctionalProperty ;
    rdfs:comment "The calendar year to which a specific set of data, metrics, or events pertains." ;
    rdfs:domain [
        a owl:Class ;
        owl:unionOf ( ont:Acquisition ont:Analysis ont:Appointment ont:Dividend ont:FinancialMetric ont:KeyPerformanceIndicator ont:Metric ont:MetricResult ont:OrganizationalUnit ont:Report ont:TemporalEntity ont:TrainingProgram )
    ] ;
    rdfs:range xsd:gYear .

ont:hasYearMonth
    a owl:DatatypeProperty , owl:FunctionalProperty ;
    rdfs:comment "The specific month and year of an occurrence." ;
    rdfs:domain [
        a owl:Class ;
        owl:unionOf ( ont:Acquisition ont:Founding )
    ] ;
    rdfs:range xsd:gYearMonth .

ont:hasYouthEconomicOpportunitiesCumulative
    a owl:DatatypeProperty , owl:FunctionalProperty ;
    rdfs:comment "The cumulative number of young people who have been provided with apprenticeships, traineeships, or other job opportunities since the start of a specific initiative." ;
    rdfs:domain ont:Company ;
    rdfs:range xsd:nonNegativeInteger .

ont:hasInterviewee
    a owl:ObjectProperty ;
    rdfs:comment "Identifies a person who was formally interviewed for a report or analysis." ;
    rdfs:domain ont:Report ;
    rdfs:range ont:Person .

#################################################################
#    Classes (Corrected and Restructured)
#################################################################

ont:AcademicInstitution
    a owl:Class ;
    rdfs:comment "An institution dedicated to higher education and research, relevant in the context of providing risk modeling or sustainability expertise." ;
    rdfs:subClassOf ont:Organization .

ont:AccessibleProduct
    a owl:Class ;
    rdfs:comment "A product that is readily available and affordable to consumers, particularly those with lower incomes." ;
    rdfs:subClassOf ont:TangibleProduct .

ont:AccountingStandard
    a owl:Class ;
    rdfs:comment "A common set of principles, standards, and procedures that define the basis of financial accounting policies and practices." ;
    rdfs:subClassOf ont:Standard .

ont:Acquisition
    a owl:Class ;
    rdfs:comment "An event where one company purchases most or all of another company's shares to gain control, relevant for tracking corporate structure changes under CSRD." ;
    rdfs:subClassOf ont:Event ;
    owl:disjointWith ont:Appointment , ont:Founding , ont:Investment .

ont:ActionPlan
    a owl:Class ;
    rdfs:comment "A detailed proposal or set of steps for achieving a specific objective related to corporate social responsibility, particularly in response to identified risks or compliance issues." ;
    rdfs:subClassOf ont:Plan .

ont:AdministrativeSite
    a owl:Class ;
    rdfs:comment "A facility dedicated to administrative functions." ;
    rdfs:subClassOf ont:Facility .

ont:Advertisement
    a owl:Class ;
    rdfs:comment "A notice or announcement in a public medium promoting a product, service, or event." ;
    rdfs:subClassOf ont:MediaContent .

ont:AdvertisingSpace
    a owl:Class ;
    rdfs:comment "A physical or digital location where advertisements can be placed." ;
    rdfs:subClassOf ont:Content ;
    owl:disjointWith ont:AudiovisualContent .

ont:AdvisoryBody
    a owl:Class ;
    rdfs:comment "A group of experts or stakeholders that provides strategic advice and guidance on sustainability matters to a company's leadership." ;
    rdfs:subClassOf ont:GovernanceBody .

ont:AffectedCommunityRisk
    a owl:Class ;
    rdfs:comment "A potential adverse impact on the rights, well-being, or environment of local populations due to a company's operations." ;
    rdfs:subClassOf ont:StakeholderRisk .

ont:Agreement
    a owl:Class ;
    rdfs:comment "A formal understanding or arrangement between parties." ;
    rdfs:subClassOf ont:Document .

ont:Analysis
    a owl:Class ;
    rdfs:comment "A systematic examination of data or content, such as a Double Materiality Analysis, which is a core requirement for CSRD reporting to identify significant sustainability impacts, risks, and opportunities." ;
    rdfs:subClassOf ont:Report ;
    owl:disjointWith ont:Framework , ont:OwnershipStructure .

ont:AnimalRisk
    a owl:Class ;
    rdfs:comment "A potential adverse impact on animal welfare resulting from a company's operations or supply chain." ;
    rdfs:subClassOf ont:Risk .

ont:AnimalWelfare
    a owl:Class ;
    rdfs:comment "The physical and mental state of animals in the supply chain." ;
    rdfs:subClassOf ont:SustainabilityTopic .

ont:AnimalWelfareControversy
    a owl:Class ;
    rdfs:comment "A public dispute or disagreement concerning the ethical treatment of animals within a company's value chain." ;
    rdfs:subClassOf ont:Wrongdoing .

ont:Appointment
    a owl:Class ;
    rdfs:comment "An event marking the assignment of a person to a specific role or position, crucial for understanding corporate governance structures as required by CSRD." ;
    rdfs:subClassOf ont:Event ;
    owl:disjointWith ont:Acquisition , ont:Founding , ont:Investment , ont:Milestone , ont:Reorganization , ont:Transaction .

ont:Assessment
    a owl:Class ;
    rdfs:comment "A formal evaluation process, such as determining the material impacts, risks, and opportunities related to sustainability." ;
    rdfs:subClassOf ont:Process .

ont:AssetProtection
    a owl:Class ;
    rdfs:comment "A category of risk related to safeguarding a company's tangible and intangible assets from loss, theft, or damage." ;
    rdfs:subClassOf ont:Wrongdoing .

ont:Association
    a owl:Class ;
    rdfs:comment "A legally constituted organization of people with a common purpose, particularly in the context of environmental preservation and education." ;
    rdfs:subClassOf ont:Organization .

ont:Audit
    a owl:Class ;
    rdfs:comment "A formal, independent examination of sustainability information to provide assurance on its accuracy and adherence to reporting standards." ;
    rdfs:subClassOf ont:Event .

ont:AuditCommittee
    a owl:Class ;
    rdfs:comment "A committee of a company's board of directors which is responsible for oversight of the financial reporting process, selection of the independent auditor, and receipt of audit results." ;
    rdfs:subClassOf ont:GovernanceBody ;
    owl:disjointWith ont:EmployeeRepresentativeBody , ont:ExecutiveCommittee .

ont:Auditor
    a owl:Class ;
    rdfs:comment "An independent professional entity responsible for examining and providing an opinion on the fairness and accuracy of a company's financial and sustainability reports." ;
    rdfs:subClassOf ont:Company , ont:ProfessionalServicesFirm .

ont:AudiovisualContent
    a owl:Class ;
    rdfs:comment "Content that has both a sound and a visual component, such as films, television programs, and online videos." ;
    rdfs:subClassOf ont:Content .

ont:AudiovisualProduction
    a owl:Class ;
    rdfs:comment "The process of creating audiovisual content." ;
    rdfs:subClassOf ont:MediaContent .

ont:Award
    a owl:Class ;
    rdfs:comment "A formal recognition granted by an external body for excellence in a specific area of corporate responsibility or sustainability." ;
    rdfs:subClassOf ont:Event .

ont:Binder
    a owl:Class ;
    rdfs:comment "A substance that causes other materials to cohere." ;
    rdfs:subClassOf ont:TangibleProduct .

ont:BiodiversityRestorationInitiative
    a owl:Class ;
    rdfs:comment "A corporate program or venture specifically aimed at restoring ecosystems and biodiversity, often as part of a CSR strategy." ;
    rdfs:subClassOf ont:Initiative .

ont:Biofuel
    a owl:Class ;
    rdfs:comment "Fuel that is produced through contemporary biological processes, such as agriculture and anaerobic digestion, rather than a fuel produced by geological processes." ;
    rdfs:subClassOf ont:RenewableEnergy .

ont:BiosourcedBinder
    a owl:Class ;
    rdfs:comment "A binder derived from biological materials (biomass)." ;
    rdfs:subClassOf ont:Binder , ont:BiosourcedMaterial .

ont:BiosourcedMaterial
    a owl:Class ;
    rdfs:comment "Material derived from biomass (plants, animals, etc.)." ;
    rdfs:subClassOf ont:Material .

ont:Bitumen
    a owl:Class ;
    rdfs:comment "A black viscous mixture of hydrocarbons obtained naturally or as a residue from petroleum distillation, used for road surfacing and roofing." ;
    rdfs:subClassOf ont:Material .

ont:BoardCommittee
    a owl:Class ;
    rdfs:comment "A specialized subgroup of a Board of Directors, established to focus on specific areas of corporate governance and oversight, such as audit, compensation, or sustainability, in line with regulatory requirements." ;
    rdfs:subClassOf ont:GovernanceBody .

ont:Boutique
    a owl:Class ;
    rdfs:comment "A retail store, especially one selling fashionable clothes or accessories, relevant in the context of energy consumption and operational footprint." ;
    rdfs:subClassOf ont:Facility .

ont:Building
    a owl:Class ;
    rdfs:comment "A structure with a roof and walls, such as a house or factory." ;
    rdfs:subClassOf ont:Infrastructure .

ont:BuildingMaterial
    a owl:Class ;
    rdfs:comment "Material used for construction." ;
    rdfs:subClassOf ont:TangibleProduct .

ont:BusinessApproach
    a owl:Class ;
    rdfs:comment "A strategic methodology or philosophy that guides a company's operations and decision-making, such as Creating Shared Value." ;
    rdfs:subClassOf ont:BusinessConcept ;
    owl:disjointWith ont:BusinessModel , ont:Framework , ont:Initiative , ont:Program , ont:Target .

ont:BusinessConcept
    a owl:Class ;
    rdfs:comment "An abstract idea or plan related to the operation and strategy of a business." ;
    rdfs:subClassOf ont:AbstractConcept ;
    owl:disjointWith ont:Metric , ont:Opportunity , ont:Role .

ont:BusinessModel
    a owl:Class ;
    rdfs:comment "The fundamental plan for how a company creates, delivers, and captures value, often represented by a core concept like the Virtuous Circle." ;
    rdfs:subClassOf ont:BusinessConcept ;
    owl:disjointWith ont:Framework , ont:Initiative , ont:Program , ont:Target .

ont:BusinessSector
    a owl:Class ;
    rdfs:comment "A specific segment of economic activity." ;
    rdfs:subClassOf ont:Service ;
    owl:disjointWith ont:Engineering , ont:Maintenance .

ont:BusinessSegment
    a owl:Class ;
    rdfs:comment "A distinct, self-contained division of a company that operates as a separate entity with its own specific focus, revenue, and operational metrics. Merges concepts of Business Unit and Operating Segment." ;
    rdfs:subClassOf ont:OrganizationalUnit .

ont:CSRD
    a owl:Class ;
    rdfs:comment "The Corporate Sustainability Reporting Directive, a type of report mandated by the EU." ;
    rdfs:subClassOf ont:Report ;
    owl:disjointWith ont:UniversalRegistrationDocument .

ont:CapitalExpenditure
    a owl:Class ;
    rdfs:comment "Funds used to acquire, upgrade, and maintain physical assets." ;
    rdfs:subClassOf ont:FinancialMetric .

ont:CapitalIncrease
    a owl:Class ;
    rdfs:comment "A corporate action that results in an increase in a company's share capital, often as part of an employee share plan, impacting the overall capital structure." ;
    rdfs:subClassOf ont:Transaction ;
    owl:disjointWith ont:Dividend , ont:ShareSale , ont:Subscription .

ont:CapitalStructure
    a owl:Class ;
    rdfs:comment "The particular combination of debt and equity used by a company to finance its overall operations and growth, tracked year-over-year for compliance and analysis." ;
    rdfs:subClassOf ont:Situation ;
    owl:disjointWith ont:Shareholding .

ont:CarbonCredit
    a owl:Class ;
    rdfs:comment "A tradable certificate or permit representing the right to emit a set amount of carbon dioxide or an equivalent amount of a different greenhouse gas, used for offsetting emissions." ;
    rdfs:subClassOf ont:TangibleProduct .

ont:CarbonIntensityMetric
    a owl:Class ;
    rdfs:comment "A key performance indicator that measures greenhouse gas emissions relative to a unit of economic output, used to track decarbonization efficiency." ;
    rdfs:subClassOf ont:EnvironmentalMetric .

ont:CarbonSink
    a owl:Class ;
    rdfs:comment "A natural or artificial reservoir that accumulates and stores some carbon-containing chemical compound for an indefinite period." ;
    rdfs:subClassOf ont:Resource .

ont:Category
    a owl:Class ;
    rdfs:comment "A class or division of people or things regarded as having particular shared characteristics." ;
    rdfs:subClassOf ont:AbstractConcept ;
    owl:disjointWith ont:Goal , ont:Metric , ont:Plan , ont:Rule .

ont:Channel
    a owl:Class ;
    rdfs:comment "A medium through which a message is transmitted to its intended audience, such as print media or broadcast media." ;
    rdfs:subClassOf ont:AbstractConcept .

ont:ChemicalSubstance
    a owl:Class ;
    rdfs:comment "A form of matter having constant chemical composition and characteristic properties." ;
    rdfs:subClassOf ont:TangibleProduct .

ont:ChildLabor
    a owl:Class ;
    rdfs:comment "The employment of children in any work that deprives them of their childhood, interferes with their ability to attend regular school, and is mentally, physically, socially or morally dangerous and harmful." ;
    rdfs:subClassOf ont:HumanRightsImpact .

ont:City
    a owl:Class ;
    rdfs:comment "A major urban center that serves as the legal or operational headquarters for a corporate entity, relevant for jurisdictional and regulatory purposes." ;
    rdfs:subClassOf ont:Place .

ont:ClimateEngineering
    a owl:Class ;
    rdfs:comment "The branch of engineering dealing with large-scale intervention in the Earth's climate system, such as with cooling systems or heat pumps." ;
    rdfs:subClassOf ont:Engineering ;
    owl:disjointWith ont:DigitalAndIct , ont:ElectricalEngineering , ont:FireSafety , ont:MechanicsAndRobotics , ont:Refrigeration .

ont:ClimateOpportunity
    a owl:Class ;
    rdfs:comment "A potential positive effect or business advantage arising from climate change or the transition to a low-carbon economy." ;
    rdfs:subClassOf ont:Topic .

ont:ClimateRisk
    a owl:Class ;
    rdfs:comment "A potential negative impact on a company's operations, financial performance, or reputation resulting from climate change, categorized as either physical or transition risks." ;
    rdfs:subClassOf ont:EnvironmentalRisk .

ont:ClimateTransitionPlan
    a owl:Class ;
    rdfs:comment "A forward-looking strategic document detailing a company's plan to decarbonize its business model in line with climate science." ;
    rdfs:subClassOf ont:Document .

ont:CommercialInvestment
    a owl:Class ;
    rdfs:comment "Investment in commercial (rather than residential or industrial) property or ventures." ;
    rdfs:subClassOf ont:FinancialResource ;
    owl:disjointWith ont:StructuralCost .

ont:Commitment
    a owl:Class ;
    rdfs:comment "A formal pledge or undertaking made by an organization to achieve a specific long-term sustainability goal, such as net-zero emissions." ;
    rdfs:subClassOf ont:AbstractConcept .

ont:Company
    a owl:Class ;
    rdfs:comment "A commercial business entity, potentially part of a larger group, subject to Corporate Sustainability Reporting Directive (CSRD) regulations and analysis. It may act as a parent, subsidiary, or partner." ;
    rdfs:subClassOf ont:Organization ;
    owl:disjointWith ont:EducationalInstitution , ont:Foundation , ont:GovernanceBody , ont:Group , ont:Initiative , ont:InnovationHub , ont:OrganizationalUnit , ont:ShareholderGroup , ont:ThinkTank .

ont:ComplianceRisk
    a owl:Class ;
    rdfs:comment "Risks associated with the failure to comply with laws, regulations, or internal policies, including financial crimes and corruption." ;
    rdfs:subClassOf ont:Risk .

ont:Construction
    a owl:Class ;
    rdfs:comment "The business sector involved in the building of infrastructure." ;
    rdfs:subClassOf ont:BusinessSector ;
    owl:disjointWith ont:EnergyAndServices , ont:Media , ont:Telecom .

ont:ConstructionEquipment
    a owl:Class ;
    rdfs:comment "Heavy-duty vehicles, specially designed for executing construction tasks." ;
    rdfs:subClassOf ont:Equipment .

ont:ConstructionMaterial
    a owl:Class ;
    rdfs:subClassOf ont:PhysicalAsset ;
    owl:disjointWith ont:Building , ont:Infrastructure .

ont:ConsumerRisk
    a owl:Class ;
    rdfs:comment "A potential adverse impact on the health, safety, or rights of consumers using a company's products or services." ;
    rdfs:subClassOf ont:StakeholderRisk .

ont:ConsumptionMetric
    a owl:Class ;
    rdfs:comment "A metric that measures the use of a resource, such as energy or water." ;
    rdfs:subClassOf ont:Metric .

ont:Content
    a owl:Class ;
    rdfs:comment "Information and experiences that are directed towards an end-user or audience." ;
    rdfs:subClassOf ont:IntangibleProduct ;
    owl:disjointWith ont:Platform .

ont:CoolingSystem
    a owl:Class ;
    rdfs:comment "A system that removes heat from a space, substance, or system to lower its temperature." ;
    rdfs:subClassOf ont:ClimateEngineering , ont:Device ;
    owl:disjointWith ont:HeatPump .

ont:CorporateCulture
    a owl:Class ;
    rdfs:comment "The shared values, beliefs, standards, and behaviors that characterize an organization and guide its actions and decisions regarding business conduct and ethics." ;
    rdfs:subClassOf ont:Topic .

ont:Corruption
    a owl:Class ;
    rdfs:comment "A category of risk involving dishonest or fraudulent conduct by those in power, typically involving bribery, which is a key focus of CSRD compliance." ;
    rdfs:subClassOf ont:Wrongdoing .

ont:CostSavingsProgram
    a owl:Class ;
    rdfs:comment "A program specifically designed to reduce expenses and improve financial efficiency." ;
    rdfs:subClassOf ont:Program .

ont:CounterfeitGoods
    a owl:Class ;
    rdfs:comment "A category of risk related to the production and distribution of fraudulent imitations of a company's products, impacting brand value and intellectual property." ;
    rdfs:subClassOf ont:Wrongdoing .

ont:Country
    a owl:Class ;
    rdfs:comment "A sovereign nation-state, relevant as the legal jurisdiction for corporate reporting obligations." ;
    rdfs:subClassOf ont:GeopoliticalEntity .

ont:CreativeCircularity
    a owl:Class ;
    rdfs:comment "A corporate strategy pillar focused on extending product life and minimizing waste through innovative design, repair, and recycling." ;
    rdfs:subClassOf ont:SustainabilityTopic .

ont:Customer
    a owl:Class ;
    rdfs:comment "An individual or entity that purchases goods or services, considered a key stakeholder for environmental information and product transparency." ;
    rdfs:subClassOf ont:Stakeholder .

ont:CustomsLegislation
    a owl:Class ;
    rdfs:comment "A category of risk associated with non-compliance with laws and regulations governing the import and export of goods." ;
    rdfs:subClassOf ont:Rule .

ont:Data
    a owl:Class ;
    rdfs:comment "Facts and statistics collected together for reference or analysis." ;
    rdfs:subClassOf ont:InformationContentEntity .

ont:DataCenter
    a owl:Class ;
    rdfs:subClassOf ont:Infrastructure ;
    owl:disjointWith ont:Network , ont:PowerGenerationFacility , ont:Railway , ont:Road .

ont:DataPrivacy
    a owl:Class ;
    rdfs:comment "A category of risk concerning the improper handling, processing, or protection of personal data, relevant to governance and compliance." ;
    rdfs:subClassOf ont:Wrongdoing .

ont:Database
    a owl:Class ;
    rdfs:comment "A structured collection of data, often from a public or intergovernmental body, used as a source for environmental metrics like water availability." ;
    rdfs:subClassOf ont:InformationContentEntity .

ont:DecarbonizationOffer
    a owl:Class ;
    rdfs:comment "A service or product line offered to customers with the explicit goal of helping them reduce their carbon footprint." ;
    rdfs:subClassOf ont:Offer .

ont:DecarbonizedEnergyPlant
    a owl:Class ;
    rdfs:comment "An energy-generating facility that has been modified or designed to produce little to no carbon emissions." ;
    rdfs:subClassOf ont:Infrastructure .

ont:Deconstruction
    a owl:Class ;
    rdfs:comment "The selective dismantlement of building components, specifically for re-use, recycling, and waste management." ;
    rdfs:subClassOf ont:Process .

ont:DeforestationFreePrimarySupplyChain
    a owl:Class ;
    rdfs:comment "A supply chain for a specific commodity where sourcing activities do not cause the conversion of forests to other land uses." ;
    rdfs:subClassOf ont:Topic .

ont:Depollution
    a owl:Class ;
    rdfs:comment "The process of removing contaminants from a specific environment, such as soil, water, or air." ;
    rdfs:subClassOf ont:Process .

ont:Device
    a owl:Class ;
    rdfs:comment "A machine or piece of equipment designed to serve a specific purpose." ;
    rdfs:subClassOf ont:TangibleProduct .

ont:DigitalAndIct
    a owl:Class ;
    rdfs:comment "The branch of engineering focused on digital technologies, information systems, and communication technologies (ICT)." ;
    rdfs:subClassOf ont:Engineering ;
    owl:disjointWith ont:ClimateEngineering , ont:ElectricalEngineering , ont:FireSafety , ont:MechanicsAndRobotics .

ont:DigitalService
    a owl:Class ;
    rdfs:comment "A service that is delivered via the internet or an electronic network." ;
    rdfs:subClassOf ont:Service .

ont:Directive
    a owl:Class ;
    rdfs:comment "A legislative act, particularly from the European Union, that sets out a goal that all EU countries must achieve through their own national laws, such as the CSRD." ;
    rdfs:subClassOf ont:LegalDocument .

ont:Discrimination
    a owl:Class ;
    rdfs:comment "The unjust or prejudicial treatment of different categories of people, especially on the grounds of race, age, sex, or disability, within the workplace." ;
    rdfs:subClassOf ont:HumanRightsImpact .

ont:Dividend
    a owl:Class ;
    rdfs:comment "A distribution of a portion of a company's earnings, decided by the board of directors, to a class of its shareholders, reported as a key performance indicator." ;
    rdfs:subClassOf ont:Transaction ;
    owl:disjointWith ont:CapitalIncrease , ont:ShareSale , ont:Subscription .

ont:Document
    a owl:Class ;
    rdfs:comment "A formal written text that provides detailed information on a specific subject, such as ESG KPI methodology." ;
    rdfs:subClassOf ont:InformationContentEntity .

ont:DocumentPart
    a owl:Class ;
    rdfs:comment "A distinct section, chapter, or component of a larger document or report." ;
    rdfs:subClassOf ont:Document ;
    owl:disjointWith ont:Report .

ont:Donation
    a owl:Class ;
    rdfs:comment "A category of risk related to charitable contributions, which could be misused for influence peddling or bribery." ;
    rdfs:subClassOf ont:Transaction .

ont:EcologicalEngineering
    a owl:Class ;
    rdfs:comment "A field of engineering that designs, monitors and constructs ecosystems." ;
    rdfs:subClassOf ont:Engineering .

ont:EconomicOpportunity
    a owl:Class ;
    rdfs:comment "A chance for an individual, particularly youth, to gain employment or develop professional skills." ;
    rdfs:subClassOf ont:Opportunity ;
    owl:disjointWith ont:PartnershipOpportunity .

ont:EconomicSanctionsViolation
    a owl:Class ;
    rdfs:comment "A category of risk involving the breach of trade penalties and commercial restrictions imposed by one or more countries against a targeted state, group, or individual." ;
    rdfs:subClassOf ont:Wrongdoing .

ont:EducationalCampus
    a owl:Class ;
    rdfs:comment "A physical site dedicated to environmental awareness, training, and education for stakeholders." ;
    rdfs:subClassOf ont:Facility .

ont:EducationalInitiative
    a owl:Class ;
    rdfs:comment "A structured program designed to build environmental skills and knowledge within an organization's workforce." ;
    rdfs:subClassOf ont:Initiative .

ont:EducationalInstitution
    a owl:Class ;
    rdfs:comment "An organization dedicated to providing education." ;
    rdfs:subClassOf ont:Organization ;
    owl:disjointWith ont:Company , ont:Foundation , ont:Initiative , ont:InnovationHub , ont:RatingAgency , ont:ThinkTank .

ont:ElectricalEngineering
    a owl:Class ;
    rdfs:comment "The branch of engineering that deals with the technology of electricity." ;
    rdfs:subClassOf ont:Engineering ;
    owl:disjointWith ont:ClimateEngineering , ont:DigitalAndIct , ont:FireSafety , ont:MechanicsAndRobotics , ont:Refrigeration .

ont:Employee
    a owl:Class ;
    rdfs:comment "An individual working for the company, who is a key participant in and target of sustainability training and initiatives." ;
    rdfs:subClassOf ont:Stakeholder .

ont:EmployeeCount
    a owl:Class ;
    rdfs:comment "A key performance indicator representing the total number of individuals employed by an organization, used for workforce analysis and regulatory reporting." ;
    rdfs:subClassOf ont:Metric .

ont:EmployeeRepresentativeBody
    a owl:Class ;
    rdfs:comment "A body which represents the employees in a company, such as a works council." ;
    rdfs:subClassOf ont:GovernanceBody ;
    owl:disjointWith ont:AuditCommittee , ont:ExecutiveCommittee .

ont:EmployeeShareholderPlan
    a owl:Class ;
    rdfs:comment "A formal, company-sponsored program that gives employees the opportunity to acquire ownership interest in the company, often as part of a governance and compensation strategy." ;
    rdfs:subClassOf ont:Plan .

ont:Energy
    a owl:Class ;
    rdfs:comment "Power derived from the utilization of physical or chemical resources, especially to provide light and heat or to work machines." ;
    rdfs:subClassOf ont:TangibleProduct .

ont:EnergyAndServices
    a owl:Class ;
    rdfs:comment "A business sector focused on the generation, distribution, and management of energy, as well as related services." ;
    rdfs:subClassOf ont:BusinessSector ;
    owl:disjointWith ont:Construction , ont:Media , ont:Telecom .

ont:EnergyConsumptionMetric
    a owl:Class ;
    rdfs:comment "A quantifiable measure of the total energy used by an entity, often broken down by source (fossil, renewable, nuclear) for CSRD reporting." ;
    rdfs:subClassOf ont:ConsumptionMetric .

ont:Engineering
    a owl:Class ;
    rdfs:comment "The branch of science and technology concerned with the design, building, and use of engines, machines, and structures." ;
    rdfs:subClassOf ont:Service ;
    owl:disjointWith ont:BusinessSector , ont:Maintenance .

ont:EnvironmentalMetric
    a owl:Class ;
    rdfs:comment "A quantifiable measure used to track and assess an organization's environmental performance, such as energy consumption or low-carbon energy share, essential for CSRD compliance." ;
    rdfs:subClassOf ont:Metric ;
    owl:disjointWith ont:FinancialMetric , ont:HrMetric , ont:OwnershipMetric , ont:Rating , ont:SafetyMetric , ont:SupplyChainMetric .

ont:EnvironmentalPolicy
    a owl:Class ;
    rdfs:comment "A formal statement of an organization's principles and intentions for managing its environmental impacts." ;
    rdfs:subClassOf ont:Policy .

ont:EnvironmentalRisk
    a owl:Class ;
    rdfs:comment "A potential negative impact on a company's operations or finances arising from environmental factors such as pollution, resource depletion, or ecosystem degradation." ;
    rdfs:subClassOf ont:Risk .

ont:Equipment
    a owl:Class ;
    rdfs:comment "The necessary items for a particular purpose." ;
    rdfs:subClassOf ont:TangibleProduct .

ont:EsgKeyPerformanceIndicator
    a owl:Class ;
    rdfs:comment "A quantifiable measure used to track performance on specific environmental, social, and governance objectives, often linked to executive compensation." ;
    rdfs:subClassOf ont:KeyPerformanceIndicator .

ont:EuropeanWorksCouncil
    a owl:Class ;
    rdfs:comment "A works council for a company with operations across the European Union, established to facilitate information and consultation between management and workers." ;
    rdfs:subClassOf ont:EmployeeRepresentativeBody .

ont:Event
    a owl:Class ;
    rdfs:comment "A temporal entity representing an instantaneous or point-in-time occurrence, such as a transaction, an appointment, or the achievement of a milestone." ;
    rdfs:subClassOf ont:TemporalEntity .

ont:ExecutiveCommittee
    a owl:Class ;
    rdfs:comment "A committee consisting of the senior executives of a company, responsible for top-level operational decisions." ;
    rdfs:subClassOf ont:GovernanceBody ;
    owl:disjointWith ont:AuditCommittee , ont:EmployeeRepresentativeBody .

ont:ExpertGroup
    a owl:Class ;
    rdfs:comment "An authoritative body of scientists and specialists, such as the IPCC (GIEC), whose findings inform climate-related strategies and risk assessments." ;
    rdfs:subClassOf ont:Organization .

ont:FacilitationPayment
    a owl:Class ;
    rdfs:comment "A category of risk involving small, unofficial payments made to secure or expedite a routine government action, often considered a form of bribery." ;
    rdfs:subClassOf ont:Corruption .

ont:Facility
    a owl:Class ;
    rdfs:comment "A place, amenity, or piece of equipment provided for a particular purpose." ;
    rdfs:subClassOf ont:Place .

ont:FiberOpticNetwork
    a owl:Class ;
    rdfs:comment "A network infrastructure that uses fiber-optic cables to transmit data as pulses of light." ;
    rdfs:subClassOf ont:FixedNetwork .

ont:Fiction
    a owl:Class ;
    rdfs:comment "A literary genre consisting of stories, events, and people that are imaginary." ;
    rdfs:subClassOf ont:Genre .

ont:FinancialDocument
    a owl:Class ;
    rdfs:comment "A formal record of the financial activities of a business, person, or other entity." ;
    rdfs:subClassOf ont:Document .

ont:FinancialMetric
    a owl:Class ;
    rdfs:comment "A quantifiable measure used to track, monitor, and assess the financial health, performance, and status of an organization or its segments. It is disjoint from operational and share price metrics." ;
    rdfs:subClassOf ont:KeyPerformanceIndicator ;
    owl:disjointWith ont:EnvironmentalMetric , ont:HrMetric , ont:OperationalMetric , ont:OwnershipMetric , ont:Rating , ont:SafetyMetric , ont:ServingOfAffordableNutrition , ont:SharePrice , ont:SupplyChainMetric .

ont:FinancialReporting
    a owl:Class ;
    rdfs:comment "A structured presentation of an entity's financial performance and position, including key figures like turnover, income, and order book value." ;
    rdfs:subClassOf ont:Document .

ont:FinancialResource
    a owl:Class ;
    rdfs:comment "Assets of a business that are in the form of money or can be easily converted into money." ;
    rdfs:subClassOf ont:Resource ;
    owl:disjointWith ont:RawMaterial .

ont:FinancialRisk
    a owl:Class ;
    rdfs:comment "The potential for monetary loss or negative financial impact on an organization, as identified and managed within a risk framework." ;
    rdfs:subClassOf ont:Risk .

ont:FinancialStatement
    a owl:Class ;
    rdfs:comment "A formal, structured record of the financial activities and position of a business, such as an income statement or balance sheet, used for regulatory reporting and analysis." ;
    rdfs:subClassOf ont:FinancialDocument ;
    owl:disjointWith ont:RegulatoryFiling .

ont:FireSafety
    a owl:Class ;
    rdfs:comment "The set of practices intended to reduce the destruction caused by fire, including related engineering services." ;
    rdfs:subClassOf ont:Engineering ;
    owl:disjointWith ont:ClimateEngineering , ont:DigitalAndIct , ont:ElectricalEngineering , ont:MechanicsAndRobotics , ont:Refrigeration .

ont:FixedNetwork
    a owl:Class ;
    rdfs:comment "A telecommunications network where the connection to the end-user is through a physical line, like a cable or fiber." ;
    rdfs:subClassOf ont:Network ;
    owl:disjointWith ont:MobileNetwork .

ont:FoodSecurity
    a owl:Class ;
    rdfs:comment "The state of having reliable access to a sufficient quantity of affordable, nutritious food." ;
    rdfs:subClassOf ont:Topic .

ont:FoodWaste
    a owl:Class ;
    rdfs:comment "Food intended for human consumption that is lost or discarded along the food supply chain, from production to the consumer." ;
    rdfs:subClassOf ont:Topic .

ont:ForcedLabor
    a owl:Class ;
    rdfs:comment "Any work or service which people are forced to do against their will, under threat of punishment." ;
    rdfs:subClassOf ont:HumanRightsImpact .

ont:FossilFuel
    a owl:Class ;
    rdfs:comment "A natural fuel such as coal or gas, formed in the geological past from the remains of living organisms." ;
    rdfs:subClassOf ont:Energy .

ont:Foundation
    a owl:Class ;
    rdfs:comment "A non-profit organization established and funded by a corporate entity to support charitable activities, relevant for assessing social impact and community engagement under CSRD." ;
    rdfs:subClassOf ont:Organization ;
    owl:disjointWith ont:Company , ont:EducationalInstitution , ont:Initiative , ont:InnovationHub , ont:RatingAgency , ont:ThinkTank .

ont:Founding
    a owl:Class ;
    rdfs:comment "The event that marks the establishment of an organization, providing historical context for its governance and strategic evolution." ;
    rdfs:subClassOf ont:Event ;
    owl:disjointWith ont:Acquisition , ont:Appointment , ont:Investment .

ont:Framework
    a owl:Class ;
    rdfs:comment "A structured set of guidelines, principles, or processes used to manage and report on specific areas, such as climate-related financial disclosures or enterprise risk." ;
    rdfs:subClassOf ont:BusinessConcept ;
    owl:disjointWith ont:Analysis , ont:BusinessApproach , ont:BusinessModel , ont:Initiative , ont:Methodology , ont:OwnershipStructure , ont:Plan , ont:Program , ont:PropertyRight , ont:Report , ont:Risk , ont:Rule , ont:System , ont:Target , ont:TrainingProgram .

ont:FrameworkAgreement
    a owl:Class ;
    rdfs:comment "An agreement between one or more businesses or organizations, the purpose of which is to establish the terms governing contracts to be awarded during a given period." ;
    rdfs:subClassOf ont:Agreement .

ont:GHGEmissionsMetric
    a owl:Class ;
    rdfs:comment "A quantifiable measure of greenhouse gases emitted into the atmosphere, categorized by scope (1, 2, 3) as required for climate-related disclosures." ;
    rdfs:subClassOf ont:EnvironmentalMetric .

ont:Genre
    a owl:Class ;
    rdfs:comment "A category of artistic composition, as in music or literature, characterized by similarities in form, style, or subject matter." ;
    rdfs:subClassOf ont:AbstractConcept .

ont:GeographicArea
    a owl:Class ;
    rdfs:comment "A region or area defined by its geographical features." ;
    rdfs:subClassOf ont:Place .

ont:GeopoliticalEntity
    a owl:Class ;
    rdfs:comment "A politically defined geographical region, such as a country or a union of countries." ;
    rdfs:subClassOf ont:Place .

ont:GeosourcedMaterial
    a owl:Class ;
    rdfs:comment "Material sourced from the earth, such as minerals or aggregates." ;
    rdfs:subClassOf ont:Material .

ont:GhgEmissions
    a owl:Class ;
    rdfs:comment "The release of greenhouse gases into the atmosphere, a key metric for measuring climate impact under various scopes." ;
    rdfs:subClassOf ont:Topic .

ont:Goal
    a owl:Class ;
    rdfs:comment "An aim or desired result." ;
    rdfs:subClassOf ont:AbstractConcept ;
    owl:disjointWith ont:Category , ont:GovernanceBody , ont:Metric , ont:Plan , ont:Policy , ont:Program , ont:Role , ont:Rule , ont:StrategicPlan , ont:SustainabilityTopic .

ont:GovernanceBody
    a owl:Class ;
    rdfs:comment "A formal group or committee, such as a Board of Directors, responsible for the oversight, strategic direction, and accountability of an organization." ;
    rdfs:subClassOf ont:GovernanceEntity , ont:Organization ;
    owl:disjointWith ont:Company , ont:Goal , ont:Group , ont:Metric , ont:Plan , ont:Policy , ont:Program , ont:Role , ont:StrategicPlan , ont:SustainabilityTopic .

ont:GovernanceCommittee
    a owl:Class ;
    rdfs:comment "A formal subgroup of a board of directors tasked with specific oversight responsibilities related to corporate governance." ;
    rdfs:subClassOf ont:GovernanceEntity .

ont:GovernanceEntity
    a owl:Class ;
    rdfs:comment "A body or committee responsible for the oversight and management of an organization." ;
    rdfs:subClassOf ont:AbstractConcept .

ont:GrievanceMechanism
    a owl:Class ;
    rdfs:comment "A formal, systematic process designed to receive, investigate, and resolve complaints or concerns raised by stakeholders, such as employees or community members." ;
    rdfs:subClassOf ont:Process .

ont:Group
    a owl:Class ;
    rdfs:comment "The parent corporate entity that encompasses multiple companies, brands, and business segments. Replaces CompanyGroup." ;
    rdfs:subClassOf ont:Organization ;
    owl:disjointWith ont:Company , ont:GovernanceBody .

ont:Guidance
    a owl:Class ;
    rdfs:comment "A forward-looking statement or projection issued by a company regarding its expected future performance on specific metrics, such as sales growth or profit margins." ;
    rdfs:subClassOf ont:Plan .

ont:Harassment
    a owl:Class ;
    rdfs:comment "Unwelcome conduct that is based on protected characteristics, which becomes a condition of continued employment or creates a hostile work environment." ;
    rdfs:subClassOf ont:HumanRightsImpact .

ont:HealthAndSafetyRisk
    a owl:Class ;
    rdfs:comment "A potential adverse impact on the physical and mental well-being of individuals, particularly workers, within a company's operations or value chain." ;
    rdfs:subClassOf ont:Risk .

ont:Healthcare
    a owl:Class ;
    rdfs:comment "The organized provision of medical care to individuals or a community." ;
    rdfs:subClassOf ont:Service .

ont:HeatPump
    a owl:Class ;
    rdfs:comment "A device that transfers heat from a colder area to a hotter area by using mechanical energy, as in a refrigerator." ;
    rdfs:subClassOf ont:ClimateEngineering , ont:Device ;
    owl:disjointWith ont:CoolingSystem .

ont:Hotel
    a owl:Class ;
    rdfs:comment "A commercial establishment providing lodging, meals, and other services for travelers, relevant in the context of operational environmental footprint." ;
    rdfs:subClassOf ont:Building , ont:Facility .

ont:Housing
    a owl:Class ;
    rdfs:subClassOf ont:Building ;
    owl:disjointWith ont:Office .

ont:HrMetric
    a owl:Class ;
    rdfs:comment "A quantifiable measure used to track and assess human resources and workforce characteristics, such as employee count or diversity ratios, which are key social indicators for CSRD." ;
    rdfs:subClassOf ont:Metric ;
    owl:disjointWith ont:EnvironmentalMetric , ont:FinancialMetric , ont:OwnershipMetric , ont:Rating , ont:SafetyMetric , ont:SupplyChainMetric .

ont:HumanRightsImpact
    a owl:Class ;
    rdfs:comment "A change in the human rights enjoyment of an individual or group resulting from a company's operations, products, or business relationships." ;
    rdfs:subClassOf ont:SocialIssue .

ont:HumanRightsRisk
    a owl:Class ;
    rdfs:comment "A potential adverse impact on the fundamental rights and freedoms of individuals, as defined by international declarations, within a company's sphere of influence." ;
    rdfs:subClassOf ont:Risk .

ont:HydraulicBinder
    a owl:Class ;
    rdfs:comment "A type of binder that hardens by reacting with water (hydration)." ;
    rdfs:subClassOf ont:Binder .

ont:IfrsAccountingStandard
    a owl:Class ;
    rdfs:comment "A set of globally recognized accounting rules and principles (International Financial Reporting Standards) used for preparing and presenting financial statements to ensure consistency and transparency." ;
    rdfs:subClassOf ont:AccountingStandard .

ont:IncentivePlan
    a owl:Class ;
    rdfs:comment "A formal program designed to motivate and reward employees, particularly executives, for achieving specific performance targets, which may include ESG metrics." ;
    rdfs:subClassOf ont:AbstractConcept .

ont:Index
    a owl:Class ;
    rdfs:comment "A curated list or benchmark, compiled by a third party, that rates or ranks companies based on their ESG or sustainability performance." ;
    rdfs:subClassOf ont:AbstractConcept .

ont:IndigenousPeoplesLandRight
    a owl:Class ;
    rdfs:comment "The rights of Indigenous peoples to own, use, and control the lands, territories, and resources that they have traditionally occupied or otherwise used." ;
    rdfs:subClassOf ont:Topic .

ont:Industry
    a owl:Class ;
    rdfs:comment "A group of companies or organizations that are related based on their primary business activities." ;
    rdfs:subClassOf ont:Organization .

ont:InfluencePeddling
    a owl:Class ;
    rdfs:comment "A category of risk involving the illegal practice of using one's influence in government or connections with persons in authority to obtain favors or preferential treatment for another, usually in return for payment." ;
    rdfs:subClassOf ont:Corruption .

ont:InformationBearingObject
    a owl:Class ;
    rdfs:comment "An object, physical or digital, whose primary purpose is to contain and convey information." ;
    rdfs:subClassOf ont:InformationContentEntity .

ont:InformationContentEntity
    a owl:Class ;
    rdfs:comment "A class for concepts that primarily exist to contain and convey information, whether in physical or digital form." ;
    rdfs:subClassOf ont:AbstractConcept .

ont:InformationSystem
    a owl:Class ;
    rdfs:comment "A software platform or digital tool used to collect, manage, and analyze specific performance data, such as greenhouse gas emissions." ;
    rdfs:subClassOf ont:System .

ont:Infrastructure
    a owl:Class ;
    rdfs:comment "The basic physical and organizational structures and facilities needed for the operation of a society or enterprise." ;
    rdfs:subClassOf ont:ConcreteEntity ;
    owl:disjointWith ont:ConstructionMaterial .

ont:Initiative
    a owl:Class ;
    rdfs:comment "A specific project, program, or formal third-party framework (like SBTi) undertaken to achieve a particular social or environmental goal." ;
    rdfs:subClassOf ont:Organization ;
    owl:disjointWith ont:BusinessApproach , ont:BusinessModel , ont:Company , ont:EducationalInstitution , ont:Foundation , ont:Framework , ont:InnovationHub , ont:Program , ont:RatingAgency , ont:Target , ont:ThinkTank .

ont:InnovationHub
    a owl:Class ;
    rdfs:comment "A collaborative entity or space designed to foster innovation, often through partnerships, which can be a source of sustainable solutions relevant to CSRD reporting." ;
    rdfs:subClassOf ont:Organization ;
    owl:disjointWith ont:Company , ont:EducationalInstitution , ont:Foundation , ont:Initiative , ont:RatingAgency , ont:ThinkTank .

ont:InnovativeProduct
    a owl:Class ;
    rdfs:comment "A product that introduces new ideas or methods." ;
    rdfs:subClassOf ont:Solution .

ont:InsiderTrading
    a owl:Class ;
    rdfs:comment "A category of risk related to the illegal practice of trading on the stock exchange to one's own advantage through having access to confidential information." ;
    rdfs:subClassOf ont:Wrongdoing .

ont:IntangibleProduct
    a owl:Class ;
    rdfs:comment "Products that are not physical in nature, such as services, content, and digital platforms." ;
    rdfs:subClassOf ont:AbstractConcept .

ont:IntellectualProperty
    a owl:Class ;
    rdfs:comment "Creations of the mind, such as inventions, literary and artistic works, designs, and symbols, names and images used in commerce." ;
    rdfs:subClassOf ont:AbstractConcept .

ont:IntellectualPropertyRight
    a owl:Class ;
    rdfs:comment "A category of risk concerning the infringement or protection of creations of the mind, such as inventions, literary and artistic works, designs, and symbols." ;
    rdfs:subClassOf ont:PropertyRight .

ont:IntensiveFarming
    a owl:Class ;
    rdfs:comment "An agricultural production system characterized by high inputs of capital, labor, or heavy use of technologies like pesticides and chemical fertilizers relative to land area." ;
    rdfs:subClassOf ont:Topic .

ont:InternalControlFramework
    a owl:Class ;
    rdfs:comment "A systematic process and set of controls implemented by a company to manage risks, ensure the reliability of reporting, and comply with laws and regulations." ;
    rdfs:subClassOf ont:Framework .

ont:InternationalAgreement
    a owl:Class ;
    rdfs:comment "A formal treaty or accord between nations, such as the Paris Agreement, which sets goals that influence corporate climate strategy." ;
    rdfs:subClassOf ont:Agreement .

ont:InternetBox
    a owl:Class ;
    rdfs:comment "A device that provides internet connectivity to other devices in a local network." ;
    rdfs:subClassOf ont:Device .

ont:InvasiveSpecies
    a owl:Class ;
    rdfs:comment "A species that is not native to a specific location, and that has a tendency to spread to a degree believed to cause damage to the environment, economy, or human health." ;
    rdfs:subClassOf ont:Species .

ont:Investment
    a owl:Class ;
    rdfs:comment "The strategic allocation of capital into an asset or company, relevant for understanding corporate influence and structure." ;
    rdfs:subClassOf ont:Event ;
    owl:disjointWith ont:Acquisition , ont:Appointment , ont:Founding .

ont:KeyPerformanceIndicator
    a owl:Class ;
    rdfs:comment "A measurable value that demonstrates how effectively a company is achieving key business and sustainability objectives." ;
    rdfs:subClassOf ont:Metric ;
    owl:disjointWith ont:MetricResult , ont:Topic .

ont:LaborUnion
    a owl:Class ;
    rdfs:comment "An organized association of workers formed to protect and further their rights and interests, with whom a company may have formal agreements." ;
    rdfs:subClassOf ont:OrganizationalUnit .

ont:LegalDocument
    a owl:Class ;
    rdfs:comment "A document that states some contractual relationship or grants some right." ;
    rdfs:subClassOf ont:Document .

ont:LegalProceeding
    a owl:Class ;
    rdfs:comment "A formal action or process conducted in a court of law, such as a judicial administration, relevant to a company's compliance and legal risks." ;
    rdfs:subClassOf ont:Process ;
    owl:disjointWith ont:Transaction .

ont:Legislation
    a owl:Class ;
    rdfs:comment "A law or set of laws enacted by a government that establishes legal requirements for corporate reporting and conduct, such as on non-financial or climate matters." ;
    rdfs:subClassOf ont:Document .

ont:LightVehicle
    a owl:Class ;
    rdfs:comment "A motor vehicle with a gross vehicle weight rating of under 10,000 pounds (4,500 kg)." ;
    rdfs:subClassOf ont:Vehicle .

ont:LivingWage
    a owl:Class ;
    rdfs:comment "Remuneration received for a standard work week by a worker in a particular place sufficient to afford a decent standard of living for the worker and her or his family." ;
    rdfs:subClassOf ont:Topic .

ont:LogisticsCenter
    a owl:Class ;
    rdfs:comment "A facility where the distribution and transportation of goods are managed, representing a key point in the supply chain for emissions and efficiency." ;
    rdfs:subClassOf ont:Facility .

ont:LowCarbonCommercialSolution
    a owl:Class ;
    rdfs:comment "A commercial solution designed to have a lower carbon footprint than traditional alternatives." ;
    rdfs:subClassOf ont:Solution .

ont:LowCarbonConcrete
    a owl:Class ;
    rdfs:comment "Concrete produced with a lower carbon footprint than traditional concrete, typically through the use of supplementary cementitious materials or carbon capture." ;
    rdfs:subClassOf ont:BuildingMaterial .

ont:LowCarbonSolution
    a owl:Class ;
    rdfs:comment "A product, technology, or process specifically developed to have a lower carbon footprint than conventional alternatives." ;
    rdfs:subClassOf ont:Solution .

ont:LowCarbonSteel
    a owl:Class ;
    rdfs:comment "Steel manufactured using methods that significantly reduce greenhouse gas emissions compared to conventional blast furnace production." ;
    rdfs:subClassOf ont:BuildingMaterial .

ont:Maintenance
    a owl:Class ;
    rdfs:comment "The process of preserving a condition or situation or the state of being preserved." ;
    rdfs:subClassOf ont:Service ;
    owl:disjointWith ont:BusinessSector , ont:Engineering .

ont:Maison
    a owl:Class ;
    rdfs:comment "A specific brand or house within a larger luxury group, often with its own distinct heritage and operational footprint." ;
    rdfs:subClassOf ont:Company .

ont:ManagementPosition
    a owl:Class ;
    rdfs:comment "A role within a company's organizational structure that holds leadership and decision-making responsibilities." ;
    rdfs:subClassOf ont:Role .

ont:ManagementSystem
    a owl:Class ;
    rdfs:comment "A set of policies, processes, and procedures used by an organization to ensure it can fulfill all tasks required to achieve its objectives, such as compliance." ;
    rdfs:subClassOf ont:AbstractConcept .

ont:Material
    a owl:Class ;
    rdfs:comment "The matter from which a thing is or can be made." ;
    rdfs:subClassOf ont:Resource .

ont:MechanicsAndRobotics
    a owl:Class ;
    rdfs:comment "The branch of engineering dealing with the design, construction, operation, and use of robots, as well as the computer systems for their control." ;
    rdfs:subClassOf ont:Engineering ;
    owl:disjointWith ont:ClimateEngineering , ont:DigitalAndIct , ont:ElectricalEngineering , ont:FireSafety , ont:Refrigeration .

ont:Media
    a owl:Class ;
    rdfs:comment "The main means of mass communication (broadcasting, publishing, and the internet) regarded collectively." ;
    rdfs:subClassOf ont:BusinessSector ;
    owl:disjointWith ont:Construction , ont:EnergyAndServices , ont:Telecom .

ont:MediaContent
    a owl:Class ;
    rdfs:comment "Any content, such as text, audio, video, and graphics, that is published on any media platform." ;
    rdfs:subClassOf ont:IntangibleProduct .

ont:MediaProgram
    a owl:Class ;
    rdfs:comment "A scheduled broadcast or a piece of recorded media content." ;
    rdfs:subClassOf ont:MediaContent .

ont:Methodology
    a owl:Class ;
    rdfs:comment "A standardized system of methods and principles used for measurement and reporting, such as the GHG Protocol for emissions accounting or risk assessment." ;
    rdfs:subClassOf ont:AbstractConcept ;
    owl:disjointWith ont:Framework , ont:Plan , ont:Program , ont:PropertyRight , ont:Risk , ont:Rule , ont:System , ont:TrainingProgram .

ont:Metric
    a owl:Class ;
    rdfs:comment "A standard of measurement or a quantitative assessment of a particular characteristic." ;
    rdfs:subClassOf ont:AbstractConcept ;
    owl:disjointWith ont:BusinessConcept , ont:Category , ont:Goal , ont:GovernanceBody , ont:Opportunity , ont:Plan , ont:Policy , ont:Program , ont:Role , ont:Rule .

ont:MetricReport
    a owl:Class ;
    rdfs:comment "A specific, quantifiable data point or key performance indicator (KPI) reported for a given period, used to measure progress against sustainability and compliance targets." ;
    rdfs:subClassOf ont:Report .

ont:MetricResult
    a owl:Class ;
    rdfs:comment "The specific, recorded quantitative or qualitative value of a Key Performance Indicator for a given reporting period." ;
    rdfs:subClassOf ont:Metric ;
    owl:disjointWith ont:KeyPerformanceIndicator .

ont:Milestone
    a owl:Class ;
    rdfs:comment "A significant point or achievement in the progress toward a long-term goal, such as meeting an SBTi-validated target." ;
    rdfs:subClassOf ont:Event ;
    owl:disjointWith ont:Appointment , ont:Reorganization .

ont:MobileNetwork
    a owl:Class ;
    rdfs:comment "A wireless network distributed over land areas called cells, each served by at least one fixed-location transceiver." ;
    rdfs:subClassOf ont:Network ;
    owl:disjointWith ont:FixedNetwork .

ont:MobilePhone
    a owl:Class ;
    rdfs:comment "A portable telephone that can make and receive calls over a radio frequency link while the user is moving within a telephone service area." ;
    rdfs:subClassOf ont:Device .

ont:MoneyLaundering
    a owl:Class ;
    rdfs:comment "A category of risk involving the concealment of the origins of illegally obtained money, a critical compliance area under CSRD." ;
    rdfs:subClassOf ont:Wrongdoing .

ont:MultiStakeholderInitiative
    a owl:Class ;
    rdfs:comment "A collaborative effort involving various groups, such as corporations, governments, and civil society, to address complex sustainability challenges and develop standards." ;
    rdfs:subClassOf ont:Organization ;
    owl:disjointWith ont:OrganizationalUnit .

ont:NaturalResource
    a owl:Class ;
    rdfs:comment "Materials or substances such as minerals, forests, water, and fertile land that occur in nature and can be used for economic gain." ;
    rdfs:subClassOf ont:Resource .

ont:Neighborhood
    a owl:Class ;
    rdfs:comment "A district, especially one forming a community within a town or city." ;
    rdfs:subClassOf ont:Building .

ont:Network
    a owl:Class ;
    rdfs:comment "A system of interconnected lines, channels, or systems." ;
    rdfs:subClassOf ont:Infrastructure ;
    owl:disjointWith ont:DataCenter , ont:PowerGenerationFacility , ont:Railway , ont:Road .

ont:NonFinancialMatter
    a owl:Class ;
    rdfs:comment "A topic related to environmental, social, employee, human rights, anti-corruption, or bribery issues that is subject to mandatory corporate disclosure." ;
    rdfs:subClassOf ont:Topic .

ont:NonIfrsFinancialPerformanceMeasure
    a owl:Class ;
    rdfs:comment "An alternative performance measure (APM) that adjusts official IFRS figures to provide a different perspective on a company's performance, such as 'Underlying Trading Operating Profit'." ;
    rdfs:subClassOf ont:Metric .

ont:NuclearEnergy
    a owl:Class ;
    rdfs:comment "Energy released during nuclear fission or fusion, especially when used to generate electricity." ;
    rdfs:subClassOf ont:Energy .

ont:Offer
    a owl:Class ;
    rdfs:comment "A product or service made available for purchase or use." ;
    rdfs:subClassOf ont:IntangibleProduct .

ont:Office
    a owl:Class ;
    rdfs:comment "A room, set of rooms, or building used as a place for commercial, professional, or bureaucratic work." ;
    rdfs:subClassOf ont:Building ;
    owl:disjointWith ont:Housing .

ont:OfficeBuilding
    a owl:Class ;
    rdfs:comment "A building containing offices." ;
    rdfs:subClassOf ont:Building .

ont:OffshoreWind
    a owl:Class ;
    rdfs:comment "Wind power generated from turbines located in bodies of water, usually the ocean." ;
    rdfs:subClassOf ont:RenewableEnergy .

ont:OperationalMetric
    a owl:Class ;
    rdfs:comment "A quantifiable measure used to track non-financial aspects of a company's operations, such as the number of physical locations." ;
    rdfs:subClassOf ont:Metric ;
    owl:disjointWith ont:FinancialMetric .

ont:Opportunity
    a owl:Class ;
    rdfs:comment "A set of circumstances that makes it possible to do something, offering potential positive impacts or advantages." ;
    rdfs:subClassOf ont:Topic ;
    owl:disjointWith ont:BusinessConcept , ont:Metric , ont:Role .

ont:Organization
    a owl:Class ;
    rdfs:comment "A formally constituted group or institution, such as a company, standard-setter, non-profit, or intergovernmental body, that plays a role in the corporate sustainability ecosystem." ;
    rdfs:subClassOf ont:Agent ;
    owl:disjointWith ont:Person , ont:Species .

ont:OrganizationalUnit
    a owl:Class ;
    rdfs:comment "A specific department, division, or functional team within a company that has defined responsibilities related to sustainability, ethics, compliance, or operations." ;
    rdfs:subClassOf ont:Organization ;
    owl:disjointWith ont:Company , ont:GovernanceBody , ont:MultiStakeholderInitiative , ont:ResearchCenter , ont:Supplier .

ont:OwnershipMetric
    a owl:Class ;
    rdfs:comment "A quantifiable measure describing the distribution of a company's equity, such as the percentage held by employees or major shareholders." ;
    rdfs:subClassOf ont:Metric ;
    owl:disjointWith ont:EnvironmentalMetric , ont:FinancialMetric , ont:HrMetric , ont:Rating , ont:SafetyMetric , ont:SupplyChainMetric .

ont:OwnershipStructure
    a owl:Class ;
    rdfs:comment "The formal arrangement of shareholdings that defines the ownership of a company, a key element of corporate governance reporting." ;
    rdfs:subClassOf ont:AbstractConcept ;
    owl:disjointWith ont:Analysis , ont:Framework .

ont:Packaging
    a owl:Class ;
    rdfs:comment "Materials used to wrap or protect goods, which is a key area of focus for environmental impact reduction and circular economy strategies." ;
    rdfs:subClassOf ont:TangibleProduct .

ont:ParallelDistribution
    a owl:Class ;
    rdfs:comment "A category of risk related to the unauthorized trade of non-counterfeit products outside of official distribution channels." ;
    rdfs:subClassOf ont:Wrongdoing .

ont:Park
    a owl:Class ;
    rdfs:comment "A designated area of land, often for conservation or recreation, which can be the subject of a company's biodiversity or community support initiatives." ;
    rdfs:subClassOf ont:Place .

ont:PartnershipOpportunity
    a owl:Class ;
    rdfs:comment "An opportunity arising from collaboration with other entities." ;
    rdfs:subClassOf ont:Opportunity ;
    owl:disjointWith ont:EconomicOpportunity .

ont:Person
    a owl:Class ;
    rdfs:comment "An individual, typically holding a key position of responsibility or oversight within a company's governance structure related to sustainability and compliance." ;
    rdfs:subClassOf ont:Agent ;
    owl:disjointWith ont:Organization , ont:Species .

ont:PersonalData
    a owl:Class ;
    rdfs:comment "Any information that relates to an identified or identifiable individual, the protection of which is a key corporate responsibility." ;
    rdfs:subClassOf ont:Data .

ont:Pesticide
    a owl:Class ;
    rdfs:comment "A substance used for destroying insects or other organisms harmful to cultivated plants or to animals, the use of which is a material sustainability topic in agriculture." ;
    rdfs:subClassOf ont:ChemicalSubstance .

ont:PhotovoltaicCyclePath
    a owl:Class ;
    rdfs:comment "A road or path for cyclists that incorporates photovoltaic (solar) panels to generate electricity." ;
    rdfs:subClassOf ont:Road .

ont:PhysicalAsset
    a owl:Class ;
    rdfs:comment "A tangible object or structure, such as a building, infrastructure, or material." ;
    rdfs:subClassOf ont:ConcreteEntity .

ont:PhysicalRisk
    a owl:Class ;
    rdfs:comment "A risk to a company's operations, supply chain, or assets resulting from the physical impacts of climate change, such as extreme weather events." ;
    rdfs:subClassOf ont:Risk .

ont:Place
    a owl:Class ;
    rdfs:comment "A particular position, point, or area in space; a location." ;
    rdfs:subClassOf ont:ConcreteEntity .

ont:Plan
    a owl:Class ;
    rdfs:comment "A detailed proposal for doing or achieving something, such as a Vigilance Plan, which outlines due diligence processes." ;
    rdfs:subClassOf ont:AbstractConcept ;
    owl:disjointWith ont:Category , ont:Framework , ont:Goal , ont:GovernanceBody , ont:Methodology , ont:Metric , ont:Policy , ont:Program , ont:PropertyRight , ont:Risk , ont:Role , ont:Rule , ont:System , ont:TrainingProgram .

ont:Platform
    a owl:Class ;
    rdfs:comment "A digital service or system that facilitates interactions or provides data, such as a sustainability rating platform (EcoVadis) or a media streaming service (TF1+)." ;
    rdfs:subClassOf ont:IntangibleProduct ;
    owl:disjointWith ont:Content .

ont:Policy
    a owl:Class ;
    rdfs:comment "A formal statement of principles, rules, and guidelines adopted by an organization to guide its decisions and achieve specific outcomes in areas like sustainability, human rights, or business conduct." ;
    rdfs:subClassOf ont:Document , ont:Rule ;
    owl:disjointWith ont:Goal , ont:GovernanceBody , ont:Metric , ont:Plan , ont:Program , ont:Role , ont:Standard , ont:StrategicPlan , ont:SustainabilityTopic .

ont:PoliticalEntity
    a owl:Class ;
    rdfs:comment "A geopolitical unit, such as the European Union, that has the authority to create and enforce laws and directives impacting corporate reporting." ;
    rdfs:subClassOf ont:GeopoliticalEntity .


ont:Position
    a owl:Class ;
    rdfs:comment "A specific job or role within an organization's hierarchy, such as the Global Head of Human Resources." ;
    rdfs:subClassOf ont:Role , ont:Situation .

ont:PowerGenerationFacility
    a owl:Class ;
    rdfs:comment "A facility designed to produce electrical energy from other forms of energy." ;
    rdfs:subClassOf ont:Infrastructure ;
    owl:disjointWith ont:DataCenter , ont:Network , ont:Railway , ont:Road .

ont:PowerPurchaseAgreement
    a owl:Class ;
    rdfs:comment "A contract between two parties, one which generates electricity (the seller) and one which is looking to purchase electricity (the buyer)." ;
    rdfs:subClassOf ont:Agreement .

ont:PrimarySupplyChain
    a owl:Class ;
    rdfs:comment "The network of suppliers providing the main raw materials essential for a company's production processes." ;
    rdfs:subClassOf ont:SupplyChain .

ont:Process
    a owl:Class ;
    rdfs:comment "A temporal entity representing a series of actions or steps that unfold over a duration, such as a manufacturing process, an assessment, or a legal proceeding." ;
    rdfs:subClassOf ont:TemporalEntity .

ont:Procurement
    a owl:Class ;
    rdfs:subClassOf ont:Process ;
    owl:disjointWith ont:SupplyChain .

ont:ProductCategory
    a owl:Class ;
    rdfs:comment "A classification for a particular type of product." ;
    rdfs:subClassOf ont:Category .

ont:ProductionSite
    a owl:Class ;
    rdfs:comment "A facility where goods are manufactured or processed, representing a primary source of direct environmental impacts like emissions and water use." ;
    rdfs:subClassOf ont:Facility .

ont:ProfessionalServicesFirm
    a owl:Class ;
    rdfs:comment "A company that provides professional services to other businesses or individuals, such as auditing, consulting, or legal advice." ;
    rdfs:subClassOf ont:Company .

ont:Program
    a owl:Class ;
    rdfs:comment "A structured initiative or plan of action designed to achieve a long-term sustainability or compliance goal, often involving multiple projects and stakeholders." ;
    rdfs:subClassOf ont:Plan ;
    owl:disjointWith ont:BusinessApproach , ont:BusinessModel , ont:Framework , ont:Goal , ont:GovernanceBody , ont:Initiative , ont:Methodology , ont:Metric , ont:Plan , ont:Policy , ont:PropertyRight , ont:Risk , ont:Role , ont:Rule , ont:StrategicPlan , ont:SustainabilityTopic , ont:System , ont:Target , ont:TrainingProgram .

ont:PropertyRight
    a owl:Class ;
    rdfs:comment "A legally recognized and protected interest in a thing, tangible or intangible." ;
    rdfs:subClassOf ont:AbstractConcept ;
    owl:disjointWith ont:Framework , ont:Methodology , ont:Plan , ont:Program , ont:Risk , ont:Rule , ont:System , ont:TrainingProgram .

ont:PublicWorks
    a owl:Class ;
    rdfs:comment "The work of building such things as roads, schools, and reservoirs, carried out by the government for the community." ;
    rdfs:subClassOf ont:Process .

ont:QualityProduct
    a owl:Class ;
    rdfs:comment "A product that meets or exceeds established standards for safety, performance, and consumer expectations." ;
    rdfs:subClassOf ont:TangibleProduct .

ont:Quarry
    a owl:Class ;
    rdfs:comment "A place, typically a large, deep pit, from which stone or other materials are or have been extracted." ;
    rdfs:subClassOf ont:Site .

ont:Railway
    a owl:Class ;
    rdfs:comment "A line of track providing a runway for wheels, forming a railroad." ;
    rdfs:subClassOf ont:Infrastructure ;
    owl:disjointWith ont:DataCenter , ont:Network , ont:PowerGenerationFacility , ont:Road .

ont:Rating
    a owl:Class ;
    rdfs:comment "An external assessment or score, such as a CDP Climate Score, that evaluates a company's performance on specific ESG criteria." ;
    rdfs:subClassOf ont:Metric ;
    owl:disjointWith ont:EnvironmentalMetric , ont:FinancialMetric , ont:HrMetric , ont:OwnershipMetric , ont:SafetyMetric , ont:SupplyChainMetric .

ont:RatingAgency
    a owl:Class ;
    rdfs:comment "A company that assesses and scores other companies on their ESG performance, risk management, and sustainability practices." ;
    rdfs:subClassOf ont:Company .

ont:RawMaterial
    a owl:Class ;
    rdfs:subClassOf ont:Resource ;
    owl:disjointWith ont:FinancialResource .

ont:RecycledMaterial
    a owl:Class ;
    rdfs:comment "Waste materials that have been converted into reusable material." ;
    rdfs:subClassOf ont:Material .

ont:RecycledSteel
    a owl:Class ;
    rdfs:comment "Steel produced from recycled ferrous scrap, rather than from iron ore." ;
    rdfs:subClassOf ont:RecycledMaterial .

ont:RecyclingMetric
    a owl:Class ;
    rdfs:comment "A key performance indicator that measures the extent to which waste materials are reprocessed into new products, such as the rate of recycled asphalt aggregates." ;
    rdfs:subClassOf ont:EnvironmentalMetric .

ont:ReductionTarget
    a owl:Class ;
    rdfs:comment "A specific, quantified goal to decrease a particular metric, such as GHG emissions, by a certain amount within a defined timeframe." ;
    rdfs:subClassOf ont:Target .

ont:Refrigeration
    a owl:Class ;
    rdfs:comment "The process of removing heat from an enclosed space, or from a substance, to lower its temperature." ;
    rdfs:subClassOf ont:Engineering ;
    owl:disjointWith ont:ClimateEngineering , ont:DigitalAndIct , ont:ElectricalEngineering , ont:FireSafety , ont:MechanicsAndRobotics .

ont:RegenerativeAgriculture
    a owl:Class ;
    rdfs:comment "A system of farming principles and practices that seeks to rehabilitate and enhance the entire ecosystem of the farm, a key strategy for biodiversity and soil health." ;
    rdfs:subClassOf ont:Topic .

ont:RegenerativeAgriculturePractice
    a owl:Class ;
    rdfs:comment "A farming method that aims to improve soil health, biodiversity, and ecosystem services, contributing to climate resilience and carbon sequestration." ;
    rdfs:subClassOf ont:Process .

ont:RegenerativeFoodSystem
    a owl:Class ;
    rdfs:comment "A holistic approach to food production that aims to restore and enhance environmental resources, support farmer livelihoods, and ensure community well-being." ;
    rdfs:subClassOf ont:System .

ont:Regulation
    a owl:Class ;
    rdfs:comment "A rule or directive made and maintained by an authority, such as the CSRD or EU Taxonomy, that mandates specific corporate sustainability disclosures." ;
    rdfs:subClassOf ont:AbstractConcept .

ont:RegulatoryFiling
    a owl:Class ;
    rdfs:comment "An official document, such as a Universal Registration Document, submitted by a company to a regulatory authority to comply with public disclosure requirements." ;
    rdfs:subClassOf ont:FinancialDocument ;
    owl:disjointWith ont:FinancialStatement .

ont:RehabilitatedHousing
    a owl:Class ;
    rdfs:comment "Housing units that have been renovated or restored to a good condition." ;
    rdfs:subClassOf ont:Housing .

ont:Remedy
    a owl:Class ;
    rdfs:comment "The means by which the remediation or resolution of an adverse impact, such as a human rights violation, is achieved." ;
    rdfs:subClassOf ont:Process .

ont:RenewableElectricity
    a owl:Class ;
    rdfs:comment "Electricity generated from sources that are naturally replenished, such as wind, solar, hydro, and geothermal power." ;
    rdfs:subClassOf ont:Topic .

ont:RenewableEnergy
    a owl:Class ;
    rdfs:comment "Energy from a source that is not depleted when used, such as wind or solar power." ;
    rdfs:subClassOf ont:Energy .

ont:Reorganization
    a owl:Class ;
    rdfs:comment "A significant, planned change in the structure, operations, or focus of a business unit or the company as a whole." ;
    rdfs:subClassOf ont:Event ;
    owl:disjointWith ont:Appointment , ont:Milestone .

ont:Report
    a owl:Class ;
    rdfs:comment "A formal document, such as an Integrated Report or Non-Financial Statement, that presents an organization's sustainability-related information, performance, and governance." ;
    rdfs:subClassOf ont:Document ;
    owl:disjointWith ont:Framework , ont:OwnershipStructure .

ont:ReportSection
    a owl:Class ;
    rdfs:comment "A distinct, labeled part or appendix within a sustainability report that organizes specific types of information, such as KPIs, policy lists, or assurance statements." ;
    rdfs:subClassOf ont:DocumentPart .

ont:ReportingStandard
    a owl:Class ;
    rdfs:comment "A prescribed set of rules and guidelines that define the content and format of sustainability disclosures to ensure consistency and comparability." ;
    rdfs:subClassOf ont:Standard .

ont:ResearchCenter
    a owl:Class ;
    rdfs:comment "An institution dedicated to scientific or academic research, often supported by a company to advance knowledge in a specific sustainability field like animal welfare." ;
    rdfs:subClassOf ont:Organization ;
    owl:disjointWith ont:OrganizationalUnit .

ont:ResearchFirm
    a owl:Class ;
    rdfs:comment "A company that conducts in-depth analysis and provides data and ratings on corporate ESG performance to investors and other stakeholders." ;
    rdfs:subClassOf ont:Company .

ont:Researcher
    a owl:Class ;
    rdfs:comment "An individual engaged in systematic investigation to establish facts or principles, often providing data for sustainability assessments." ;
    rdfs:subClassOf ont:Stakeholder .

ont:Resource
    a owl:Class ;
    rdfs:comment "A stock or supply of money, materials, staff, and other assets that can be drawn on by a person or organization in order to function effectively." ;
    rdfs:subClassOf ont:ConcreteEntity .

ont:ResponsibleMarketing
    a owl:Class ;
    rdfs:comment "The practice of marketing products and services in a manner that is truthful, ethical, and does not target vulnerable populations inappropriately." ;
    rdfs:subClassOf ont:Topic .

ont:ResponsibleProduct
    a owl:Class ;
    rdfs:comment "A product designed, produced, and marketed with consideration for its environmental, social, and ethical impacts throughout its lifecycle." ;
    rdfs:subClassOf ont:Solution .

ont:Risk
    a owl:Class ;
    rdfs:comment "A potential event or condition, whether financial, environmental, or social, that could negatively impact an organization's ability to achieve its objectives, as identified for analysis and mitigation." ;
    rdfs:subClassOf ont:Situation ;
    owl:disjointWith ont:Framework , ont:Methodology , ont:Plan , ont:Program , ont:PropertyRight , ont:Rule , ont:System , ont:TrainingProgram .

ont:RiskCategory
    a owl:Class ;
    rdfs:comment "A classification for potential adverse impacts or uncertainties related to sustainability matters, such as climate change, that could affect the organization." ;
    rdfs:subClassOf ont:Category .

ont:Road
    a owl:Class ;
    rdfs:subClassOf ont:Infrastructure ;
    owl:disjointWith ont:DataCenter , ont:Network , ont:PowerGenerationFacility , ont:Railway .

ont:Role
    a owl:Class ;
    rdfs:comment "A specific function or title within a corporate structure, such as 'CEO' or 'Chairman', held by a person, defining their responsibilities and authority." ;
    rdfs:subClassOf ont:AbstractConcept ;
    owl:disjointWith ont:BusinessConcept , ont:Goal , ont:GovernanceBody , ont:Metric , ont:Opportunity , ont:Plan , ont:Policy , ont:Program , ont:StrategicPlan , ont:SustainabilityTopic .

ont:Rule
    a owl:Class ;
    rdfs:comment "An explicit or understood regulation or principle governing conduct or procedure within a particular area of activity." ;
    rdfs:subClassOf ont:AbstractConcept ;
    owl:disjointWith ont:Category , ont:Framework , ont:Goal , ont:Methodology , ont:Metric , ont:Plan , ont:Program , ont:PropertyRight , ont:Risk , ont:System , ont:TrainingProgram .

ont:SafeProduct
    a owl:Class ;
    rdfs:comment "A product that does not pose an unacceptable risk of harm to consumers when used as intended or in a reasonably foreseeable manner." ;
    rdfs:subClassOf ont:TangibleProduct .

ont:SafetyMetric
    a owl:Class ;
    rdfs:comment "A quantifiable measure used to track and assess workplace health and safety performance, such as accident frequency rates, a critical social indicator for CSRD." ;
    rdfs:subClassOf ont:Metric ;
    owl:disjointWith ont:EnvironmentalMetric , ont:FinancialMetric , ont:HrMetric , ont:OwnershipMetric , ont:Rating , ont:SupplyChainMetric .

ont:Service
    a owl:Class ;
    rdfs:comment "A system supplying a public need such as transport, communications, or utilities, or a commercial enterprise providing work or help." ;
    rdfs:subClassOf ont:IntangibleProduct .

ont:ServiceVehicle
    a owl:Class ;
    rdfs:comment "A vehicle equipped to provide a service, such as repairs or maintenance." ;
    rdfs:subClassOf ont:Vehicle .

ont:ServingOfAffordableNutrition
    a owl:Class ;
    rdfs:comment "A single portion of a food product that is both economically accessible and enhanced with essential micronutrients." ;
    rdfs:subClassOf ont:KeyPerformanceIndicator ;
    owl:disjointWith ont:FinancialMetric .

ont:SharePrice
    a owl:Class ;
    rdfs:comment "The market price of a single share of a company's stock at a specific point in time, used as a key performance indicator for investors and analysts." ;
    rdfs:subClassOf ont:Metric ;
    owl:disjointWith ont:FinancialMetric .

ont:ShareSale
    a owl:Class ;
    rdfs:comment "The act of selling existing shares, such as treasury shares, often as part of a structured offering like an employee shareholder plan." ;
    rdfs:subClassOf ont:Transaction ;
    owl:disjointWith ont:CapitalIncrease , ont:Dividend , ont:Subscription .

ont:Shareholder
    a owl:Class ;
    rdfs:comment "An owner of shares in a company, considered a key stakeholder for whom CSRD and financial-sustainability reporting is prepared." ;
    rdfs:subClassOf ont:Stakeholder .

ont:ShareholderGroup
    a owl:Class ;
    rdfs:comment "A collection of individuals or entities, such as a family, that acts as a single unit in its ownership and control of a company." ;
    rdfs:subClassOf ont:Organization ;
    owl:disjointWith ont:Company .

ont:Shareholding
    a owl:Class ;
    rdfs:comment "The ownership of shares in a company by an individual or group, detailing the percentage of capital and voting rights held, which is a key aspect of corporate governance." ;
    rdfs:subClassOf ont:Situation ;
    owl:disjointWith ont:CapitalStructure .

ont:Site
    a owl:Class ;
    rdfs:comment "A specific area or location where something is, was, or will be built or situated." ;
    rdfs:subClassOf ont:Place .

ont:Situation
    a owl:Class ;
    rdfs:comment "A set of circumstances or a state of affairs, often involving relationships between entities over a period of time." ;
    rdfs:subClassOf ont:AbstractConcept .

ont:SocialIssue
    a owl:Class ;
    rdfs:comment "A problem that influences a considerable number of the individuals within a society." ;
    rdfs:subClassOf ont:Situation .

ont:SolarFarm
    a owl:Class ;
    rdfs:comment "A large-scale photovoltaic power station, designed for the supply of merchant power into the electricity grid." ;
    rdfs:subClassOf ont:PowerGenerationFacility .

ont:Solution
    a owl:Class ;
    rdfs:comment "A means of solving a problem or dealing with a difficult situation, often packaged as a product or service." ;
    rdfs:subClassOf ont:IntangibleProduct .

ont:Species
    a owl:Class ;
    rdfs:comment "A group of living organisms consisting of similar individuals capable of exchanging genes or interbreeding." ;
    rdfs:subClassOf ont:Agent ;
    owl:disjointWith ont:Organization , ont:Person .

ont:Sponsorship
    a owl:Class ;
    rdfs:comment "A category of risk related to financial or in-kind support for an event or organization, which could be perceived as a means to gain improper influence." ;
    rdfs:subClassOf ont:Transaction .

ont:Stakeholder
    a owl:Class ;
    rdfs:comment "Any group or individual who can affect or is affected by the achievement of the organization's objectives, central to the CSRD's dual materiality concept." ;
    rdfs:subClassOf ont:AbstractConcept .

ont:StakeholderRisk
    a owl:Class ;
    rdfs:comment "Risks related to adverse impacts on key stakeholder groups, such as workers, consumers, and local communities." ;
    rdfs:subClassOf ont:Risk .

ont:Standard
    a owl:Class ;
    rdfs:comment "A recognized framework or set of guidelines, such as GRI or SASB, that provides a structured approach for reporting sustainability information, or a required level of quality." ;
    rdfs:subClassOf ont:Document , ont:Rule ;
    owl:disjointWith ont:Policy .

ont:StandardizedOffer
    a owl:Class ;
    rdfs:comment "A product or service offering that is consistent and uniform, without customization." ;
    rdfs:subClassOf ont:Offer .

ont:StockExchange
    a owl:Class ;
    rdfs:comment "A marketplace where securities like company stocks are bought and sold, which may have listing requirements related to sustainability reporting." ;
    rdfs:subClassOf ont:Organization .

ont:StrategicPillar
    a owl:Class ;
    rdfs:comment "A fundamental thematic area of focus within a broader strategic program, guiding specific targets and actions." ;
    rdfs:subClassOf ont:StrategicPlan ;
    owl:disjointWith ont:StrategicProgram .

ont:StrategicPlan
    a owl:Class ;
    rdfs:comment "A high-level document, roadmap, or abstract strategy outlining an organization's long-term approach to a specific challenge, such as its climate transition." ;
    rdfs:subClassOf ont:Plan ;
    owl:disjointWith ont:Goal , ont:GovernanceBody , ont:Metric , ont:Policy , ont:Program , ont:Role , ont:SustainabilityTopic , ont:Target .

ont:StrategicProgram
    a owl:Class ;
    rdfs:comment "A comprehensive, multi-year corporate initiative that sets the overarching vision, targets, and action pillars for sustainability." ;
    rdfs:subClassOf ont:Program , ont:StrategicPlan ;
    owl:disjointWith ont:StrategicPillar .

ont:StrategicRawMaterial
    a owl:Class ;
    rdfs:comment "Raw materials that are critical to the company's production and supply chain security." ;
    rdfs:subClassOf ont:AbstractConcept .

ont:StreamingPlatform
    a owl:Class ;
    rdfs:comment "An on-demand online entertainment source for TV shows, movies, and other streaming media." ;
    rdfs:subClassOf ont:Platform .

ont:StructuralCost
    a owl:Class ;
    rdfs:comment "Costs that are a fixed part of the operating budget, not directly related to the volume of production or sales." ;
    rdfs:subClassOf ont:FinancialResource ;
    owl:disjointWith ont:CommercialInvestment .

ont:Subscription
    a owl:Class ;
    rdfs:comment "An event where investors, typically employees, commit to purchasing newly offered shares at a predetermined price and time." ;
    rdfs:subClassOf ont:Transaction ;
    owl:disjointWith ont:CapitalIncrease , ont:Dividend , ont:ShareSale .

ont:Supplier
    a owl:Class ;
    rdfs:comment "A type of third-party entity that provides goods or services and is subject to a company's due diligence and risk management processes under CSRD." ;
    rdfs:subClassOf ont:Organization ;
    owl:disjointWith ont:OrganizationalUnit .

ont:SupplyChain
    a owl:Class ;
    rdfs:comment "The sequence of processes involved in the production and distribution of a commodity." ;
    rdfs:subClassOf ont:Process ;
    owl:disjointWith ont:Procurement .

ont:SupplyChainMetric
    a owl:Class ;
    rdfs:comment "A quantifiable measure used to track and assess the performance and due diligence of an organization's supply chain, such as the number of suppliers evaluated for CSR risks." ;
    rdfs:subClassOf ont:Metric ;
    owl:disjointWith ont:EnvironmentalMetric , ont:FinancialMetric , ont:HrMetric , ont:OwnershipMetric , ont:Rating , ont:SafetyMetric .

ont:SustainabilityFactor
    a owl:Class ;
    rdfs:comment "An environmental, social, or governance matter that can affect the value, risk, or performance of an investment or a company." ;
    rdfs:subClassOf ont:Topic .

ont:SustainabilityTopic
    a owl:Class ;
    rdfs:comment "A specific subject area (e.g., Climate Change, Business Conduct) identified as material through a double materiality analysis for CSRD reporting." ;
    rdfs:subClassOf ont:Topic ;
    owl:disjointWith ont:Goal , ont:GovernanceBody , ont:Metric , ont:Plan , ont:Policy , ont:Program , ont:Role , ont:StrategicPlan .

ont:SustainableDevelopmentGoal
    a owl:Class ;
    rdfs:subClassOf ont:Target .

ont:System
    a owl:Class ;
    rdfs:comment "A set of interacting or interdependent components forming an integrated whole." ;
    rdfs:subClassOf ont:AbstractConcept ;
    owl:disjointWith ont:Framework , ont:Methodology , ont:Plan , ont:PropertyRight , ont:Risk , ont:Rule , ont:TrainingProgram .

ont:TangibleProduct
    a owl:Class ;
    rdfs:comment "Products that are physical in nature, such as devices, materials, and equipment." ;
    rdfs:subClassOf ont:ConcreteEntity .

ont:Target
    a owl:Class ;
    rdfs:comment "A specific, measurable objective with a defined timeline that the organization commits to achieving as part of its strategic objectives." ;
    rdfs:subClassOf ont:Goal ;
    owl:disjointWith ont:BusinessApproach , ont:BusinessModel , ont:Framework , ont:Initiative , ont:Program , ont:StrategicPlan .

ont:Team
    a owl:Class ;
    rdfs:comment "A group of individuals within an organization working together on a specific task, such as managing data quality for ESG reporting." ;
    rdfs:subClassOf ont:OrganizationalUnit .

ont:Telecom
    a owl:Class ;
    rdfs:comment "The business sector related to telecommunications." ;
    rdfs:subClassOf ont:BusinessSector ;
    owl:disjointWith ont:Construction , ont:EnergyAndServices , ont:Media .

ont:TelecomNetwork
    a owl:Class ;
    rdfs:comment "The infrastructure used for transmitting information in the form of voice, data, and video." ;
    rdfs:subClassOf ont:Infrastructure .

ont:ThinkTank
    a owl:Class ;
    rdfs:comment "An organization that performs research and advocacy on specific topics, such as AI's impact, contributing to the broader context of a company's strategic positioning." ;
    rdfs:subClassOf ont:Organization ;
    owl:disjointWith ont:Company , ont:EducationalInstitution , ont:Foundation , ont:Initiative , ont:InnovationHub , ont:RatingAgency .

ont:Tool
    a owl:Class ;
    rdfs:comment "A specific software application, model, or methodology used to assess and quantify environmental risks, such as water stress." ;
    rdfs:subClassOf ont:TangibleProduct .

ont:Topic
    a owl:Class ;
    rdfs:comment "A thematic category used to group related sustainability metrics and disclosures, aligned with regulatory frameworks like CSRD." ;
    rdfs:subClassOf ont:AbstractConcept ;
    owl:disjointWith ont:KeyPerformanceIndicator .

ont:Traceability
    a owl:Class ;
    rdfs:comment "The ability to track the history, application, or location of an object or activity by means of recorded identification." ;
    rdfs:subClassOf ont:SustainabilityTopic .

ont:TrainingProgram
    a owl:Class ;
    rdfs:comment "A structured educational course or module designed to build employee competency and awareness regarding corporate policies, ethics, and compliance obligations." ;
    rdfs:subClassOf ont:AbstractConcept ;
    owl:disjointWith ont:Framework , ont:Methodology , ont:Plan , ont:Program , ont:PropertyRight , ont:Risk , ont:Rule , ont:System .

ont:Transaction
    a owl:Class ;
    rdfs:comment "An instance of buying, selling, or exchanging something; a business deal or financial event." ;
    rdfs:subClassOf ont:Event ;
    owl:disjointWith ont:LegalProceeding .

ont:TransitionRisk
    a owl:Class ;
    rdfs:comment "A risk to a company arising from the process of adjusting toward a lower-carbon economy, including policy, legal, technology, and market changes." ;
    rdfs:subClassOf ont:Risk .

ont:Transparency
    a owl:Class ;
    rdfs:comment "The quality of operating in such a way that it is easy for others to see what actions are performed." ;
    rdfs:subClassOf ont:SustainabilityTopic .

ont:TransportInfrastructure
    a owl:Class ;
    rdfs:comment "The framework of transportation systems, including roads, railways, and ports." ;
    rdfs:subClassOf ont:Infrastructure .

ont:UnhealthyDiet
    a owl:Class ;
    rdfs:comment "A pattern of food consumption characterized by high intake of energy, saturated fats, trans fats, free sugars, or salt, linked to negative health outcomes." ;
    rdfs:subClassOf ont:Topic .

ont:UnitedNationsGlobalCompact
    a owl:Class ;
    rdfs:subClassOf ont:AbstractConcept .

ont:UniversalRegistrationDocument
    a owl:Class ;
    rdfs:subClassOf ont:Report ;
    owl:disjointWith ont:CSRD .

ont:University
    a owl:Class ;
    rdfs:comment "An institution of higher education and research, often engaged as a partner for innovation and talent development relevant to a company's long-term strategy." ;
    rdfs:subClassOf ont:EducationalInstitution .

ont:Vehicle
    a owl:Class ;
    rdfs:comment "A thing used for transporting people or goods, especially on land." ;
    rdfs:subClassOf ont:TangibleProduct .

ont:VideoContent
    a owl:Class ;
    rdfs:comment "Audiovisual content presented in a video format." ;
    rdfs:subClassOf ont:AudiovisualContent .

ont:VigilancePlan
    a owl:Class ;
    rdfs:comment "A mandatory due diligence plan under French law for large companies to identify and prevent severe impacts on human rights and the environment." ;
    rdfs:subClassOf ont:Document .

ont:Violence
    a owl:Class ;
    rdfs:comment "Behavior involving physical force intended to hurt, damage, or kill someone or something, which is strictly prohibited in a corporate workplace policy." ;
    rdfs:subClassOf ont:HumanRightsImpact .

ont:Whistleblower
    a owl:Class ;
    rdfs:comment "A person, usually an employee, who exposes information or activity within a private, public, or government organization that is deemed illegal, illicit, unsafe, or a waste, fraud, or abuse of taxpayer funds." ;
    rdfs:subClassOf ont:Person .

ont:WhistleblowerRightsViolation
    a owl:Class ;
    rdfs:comment "A category of risk involving retaliation against or failure to protect an individual who reports misconduct, which is a key governance aspect under CSRD." ;
    rdfs:subClassOf ont:Wrongdoing .

ont:WhistleblowingSystem
    a owl:Class ;
    rdfs:comment "A formal, confidential channel established by a company for employees and external stakeholders to report concerns about unethical or illegal activities." ;
    rdfs:subClassOf ont:System .

ont:WoodConstruction
    a owl:Class ;
    rdfs:comment "The use of wood as a primary building material in construction projects." ;
    rdfs:subClassOf ont:BuildingMaterial .

ont:WorkIncident
    a owl:Class ;
    rdfs:comment "An unplanned event that results in or has the potential to result in injury, illness, damage, or other loss at the workplace." ;
    rdfs:subClassOf ont:Event .

ont:WorkerRisk
    a owl:Class ;
    rdfs:comment "A potential adverse impact on the rights, safety, and well-being of employees and other workers throughout a company's value chain." ;
    rdfs:subClassOf ont:StakeholderRisk .

ont:WorksCouncil
    a owl:Class ;
    rdfs:comment "A body representing the employees of a company at the European level, facilitating information and consultation between management and workers on transnational issues." ;
    rdfs:subClassOf ont:OrganizationalUnit .

ont:Wrongdoing
    a owl:Class ;
    rdfs:comment "An illegal or dishonest act." ;
    rdfs:subClassOf ont:Situation .

