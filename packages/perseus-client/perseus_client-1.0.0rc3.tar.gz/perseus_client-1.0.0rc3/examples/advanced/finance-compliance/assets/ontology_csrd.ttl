
@base <http://example.org/ontology> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix ont: <#> .

#################################################################
#    Ontology Metadata
#################################################################

ont:
    a owl:Ontology ;
    rdfs:comment "Ontology for the use case: Organize CSRD reports into structured data (metrics, yearly results, governance, risks, targets, KPI's, etc.) to support expert analysis and regulatory compliance." .

#################################################################
#    Annotation Properties
#################################################################

rdfs:comment a owl:AnnotationProperty .

#################################################################
#    Top-Level Classes (Corrected)
#################################################################

ont:AbstractConcept
    a owl:Class ;
    rdfs:label "Abstract Concept" ;
    rdfs:comment "Non-physical, conceptual entities such as ideas, theories, plans, information, qualities, attributes, processes, and situations." .

ont:Agent
    a owl:Class ;
    rdfs:label "Agent" ;
    rdfs:comment "Entities capable of acting or exercising power, such as people, organizations, software agents, or intelligent systems." .

ont:ConcreteEntity
    a owl:Class ;
    rdfs:label "Concrete Entity" ;
    rdfs:comment "Physical, tangible objects, including artifacts, natural objects, locations, and materials." .

ont:TemporalEntity
    a owl:Class ;
    rdfs:label "Temporal Entity" ;
    rdfs:comment "Entities that happen in time and have temporal parts, such as events, processes, states, or periods. Also known as perdurants or occurrents." .

#################################################################
#    Object Properties (Corrected and Refactored)
#################################################################

ont:adheresTo
    a owl:ObjectProperty ;
    rdfs:comment "Indicates that an agent's behavior is governed by or follows a specific rule, standard, or policy." ;
    rdfs:domain ont:Agent ;
    rdfs:range ont:AbstractConcept ;
    owl:inverseOf ont:isAdheredToBy .

ont:adoptsStandard
    a owl:ObjectProperty ;
    rdfs:comment "Indicates that an organization officially adopts or uses a specific external standard for its operations or reporting." ;
    rdfs:domain ont:Organization ;
    rdfs:range ont:Standard .

ont:advises
    a owl:ObjectProperty ;
    rdfs:comment "Represents the act of providing expert recommendations or guidance to an organization on its strategy or policies." ;
    rdfs:domain ont:GovernanceBody ;
    rdfs:range [
        a owl:Class ;
        owl:unionOf ( ont:Company ont:GovernanceBody )
    ] ;
    owl:propertyDisjointWith ont:oversees .

ont:addressesIssue
    a owl:ObjectProperty ;
    rdfs:comment "Indicates that a specific plan or action is designed to mitigate or manage a particular environmental or sustainability topic." ;
    rdfs:domain ont:ActionPlan ;
    rdfs:range ont:SustainabilityTopic .

ont:affects
    a owl:ObjectProperty ;
    rdfs:comment "Indicates that a corporate action or reorganization has a direct impact on a specific business area." ;
    rdfs:domain ont:Reorganization ;
    rdfs:range ont:BusinessSegment .

ont:approves
    a owl:ObjectProperty ;
    rdfs:comment "Signifies the act of giving official sanction or formal consent to a document, plan, or body." ;
    rdfs:domain ont:GovernanceBody ;
    rdfs:range [
        a owl:Class ;
        owl:unionOf ( ont:GovernanceBody ont:Report )
    ] .

ont:authored
    a owl:ObjectProperty ;
    rdfs:comment "The inverse of isAuthoredBy." ;
    rdfs:domain ont:Agent ;
    rdfs:range ont:AbstractConcept ;
    owl:inverseOf ont:isAuthoredBy .

ont:collaboratesWith
    a owl:ObjectProperty , owl:SymmetricProperty ;
    rdfs:comment "Indicates a working partnership between distinct internal departments to achieve shared compliance and sustainability objectives." ;
    rdfs:domain ont:OrganizationalUnit ;
    rdfs:range ont:OrganizationalUnit .

ont:conductsAnalysis
    a owl:ObjectProperty ;
    rdfs:comment "Indicates the action of performing a systematic examination of a subject, such as a materiality assessment." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:Analysis .

ont:conductsTraining
    a owl:ObjectProperty ;
    rdfs:comment "Represents the act of delivering or administering an educational program to enhance employee knowledge on sustainability and compliance topics." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:TrainingProgram .

ont:consolidates
    a owl:ObjectProperty ;
    rdfs:comment "Represents the accounting practice where a parent company combines the financial statements of a subsidiary for reporting purposes. Inverse of isConsolidatedBy." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:Company ;
    owl:inverseOf ont:isConsolidatedBy .

ont:definesAndExecutes
    a owl:ObjectProperty ;
    rdfs:comment "Represents the dual responsibility of creating the plan for and carrying out the activities of a strategic initiative or governing body." ;
    rdfs:domain ont:GovernanceBody ;
    rdfs:range ont:GovernanceBody .

ont:deploys
    a owl:ObjectProperty ;
    rdfs:comment "Signifies the act of implementing or rolling out a strategic program or policy across an organization." ;
    rdfs:domain ont:OrganizationalUnit ;
    rdfs:range ont:StrategicProgram .

ont:deploysTraining
    a owl:ObjectProperty ;
    rdfs:comment "Represents the act of making an educational module or program available to employees." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:TrainingProgram ;
    rdfs:subPropertyOf ont:conductsTraining .

ont:designates
    a owl:ObjectProperty , owl:AsymmetricProperty , owl:IrreflexiveProperty ;
    rdfs:comment "Represents the formal act of establishing or appointing a subcommittee or group from a parent governing body." ;
    rdfs:domain ont:GovernanceBody ;
    rdfs:range ont:GovernanceBody .

ont:develops
    a owl:ObjectProperty , owl:AsymmetricProperty , owl:IrreflexiveProperty ;
    rdfs:comment "Signifies the creation or formulation of a specific standard, methodology, or framework." ;
    rdfs:domain ont:Agent ;
    rdfs:range [
        a owl:Class ;
        owl:unionOf ( ont:AbstractConcept ont:Initiative ont:IntangibleProduct ont:TangibleProduct )
    ] .

ont:employs
    a owl:ObjectProperty ;
    rdfs:comment "Signifies the relationship where an organization has an individual working for it." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:Person ;
    owl:inverseOf ont:isEmployedBy .

ont:hasAcquiredEntity
    a owl:ObjectProperty , owl:FunctionalProperty ;
    rdfs:comment "Identifies the company or asset that was purchased in an acquisition event." ;
    rdfs:domain ont:Acquisition ;
    rdfs:range ont:Company .

ont:hasAcquirer
    a owl:ObjectProperty , owl:FunctionalProperty ;
    rdfs:comment "Identifies the company that performed the act of purchasing another entity." ;
    rdfs:domain ont:Acquisition ;
    rdfs:range ont:Company .

ont:hasAcquisitionEvent
    a owl:ObjectProperty ;
    rdfs:comment "A fact stating the specific transaction through which one company took control of another." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:Acquisition .

ont:hasActionPlan
    a owl:ObjectProperty ;
    rdfs:comment "Indicates that a strategic program is composed of several detailed plans focused on specific environmental pillars." ;
    rdfs:domain ont:StrategicProgram ;
    rdfs:range ont:ActionPlan .

ont:hasAppointee
    a owl:ObjectProperty , owl:FunctionalProperty ;
    rdfs:comment "Identifies the individual who is the subject of an appointment to a role." ;
    rdfs:domain ont:Appointment ;
    rdfs:range ont:Person .

ont:hasAppointment
    a owl:ObjectProperty ;
    rdfs:comment "A fact stating the formal engagement of an entity for a specific corporate governance role." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:Appointment .

ont:hasAssuranceFrom
    a owl:ObjectProperty ;
    rdfs:comment "Indicates that an external, independent entity has provided a formal opinion on the reliability and accuracy of reported information." ;
    rdfs:domain ont:Report ;
    rdfs:range ont:Company .

ont:hasAuditor
    a owl:ObjectProperty ;
    rdfs:comment "Indicates the formal engagement of an external entity to perform an audit." ;
    rdfs:domain ont:Group ;
    rdfs:range ont:Auditor .

ont:hasAuthor
    a owl:ObjectProperty , owl:AsymmetricProperty , owl:IrreflexiveProperty ;
    rdfs:comment "Identifies the organization responsible for creating, preparing, and publishing a report. A report may have multiple authors." ;
    rdfs:domain ont:Report ;
    rdfs:range ont:Company .

ont:hasBusinessSegment
    a owl:ObjectProperty ;
    rdfs:comment "Denotes the inclusion of a distinct operational division within a larger corporate structure." ;
    rdfs:domain ont:Group ;
    rdfs:range ont:BusinessSegment .

ont:hasCapitalStructure
    a owl:ObjectProperty , owl:FunctionalProperty ;
    rdfs:comment "The association of a specific equity and debt configuration with an entity at a point in time." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:CapitalStructure .

ont:hasCeo
    a owl:ObjectProperty , owl:FunctionalProperty ;
    rdfs:comment "Identifies the individual holding the Chief Executive Officer position." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:Person ;
    rdfs:subPropertyOf ont:employs .

ont:hasChairman
    a owl:ObjectProperty , owl:FunctionalProperty ;
    rdfs:comment "Identifies the individual holding the Chairman of the Board position." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:Person ;
    rdfs:subPropertyOf ont:employs .

ont:hasCommittee
    a owl:ObjectProperty ;
    rdfs:comment "Indicates the existence of a specialized subgroup established to handle specific governance tasks." ;
    rdfs:domain ont:GovernanceBody ;
    rdfs:range [
        a owl:Class ;
        owl:unionOf ( ont:BoardCommittee ont:GovernanceCommittee )
    ] .

ont:hasCommitment
    a owl:ObjectProperty ;
    rdfs:comment "Signifies that an organization has formally adopted a specific pledge or undertaking related to sustainability." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:Commitment .

ont:hasComponent
    a owl:ObjectProperty , owl:IrreflexiveProperty , owl:TransitiveProperty ;
    rdfs:comment "Represents the inclusion of a sub-part within a larger, composite measure." ;
    rdfs:domain ont:Metric ;
    rdfs:range ont:Metric .

ont:hasDepartment
    a owl:ObjectProperty ;
    rdfs:comment "Indicates the existence of a specific operational unit within an organization's structure." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:OrganizationalUnit .

ont:hasDeputyCeo
    a owl:ObjectProperty ;
    rdfs:comment "Identifies an individual holding a Deputy Chief Executive Officer position." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:Person ;
    rdfs:subPropertyOf ont:employs .

ont:hasDividend
    a owl:ObjectProperty ;
    rdfs:comment "The association of a specific per-share profit distribution with a company for a given period." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:Dividend .

ont:hasExecutive
    a owl:ObjectProperty ;
    rdfs:comment "Identifies a person holding a key management position within an organization." ;
    rdfs:domain ont:Organization ;
    rdfs:range ont:Person .

ont:hasFinancialReporting
    a owl:ObjectProperty , owl:FunctionalProperty ;
    rdfs:comment "Associates an entity with its formal statement of financial performance and position." ;
    rdfs:domain ont:Organization ;
    rdfs:range ont:FinancialReporting .

ont:hasFinancialStatement
    a owl:ObjectProperty ;
    rdfs:comment "Indicates the existence of a formal record of financial activities for a specific reporting period." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:FinancialStatement .

ont:hasFoundation
    a owl:ObjectProperty ;
    rdfs:comment "Indicates that an entity has established or is associated with a specific philanthropic foundation." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:Foundation .

ont:hasFounder
    a owl:ObjectProperty ;
    rdfs:comment "Identifies the individual who established an organization." ;
    rdfs:domain ont:Founding ;
    rdfs:range ont:Person .

ont:hasFoundingEvent
    a owl:ObjectProperty , owl:FunctionalProperty ;
    rdfs:comment "A fact stating the specific event that created an organization." ;
    rdfs:domain [
        a owl:Class ;
        owl:unionOf ( ont:Company ont:Group )
    ] ;
    rdfs:range ont:Founding .

ont:hasFramework
    a owl:ObjectProperty ;
    rdfs:comment "Indicates the adoption, use, or implementation of a specific structured set of guidelines or rules." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:Framework .

ont:hasGovernanceBody
    a owl:ObjectProperty , owl:FunctionalProperty ;
    rdfs:comment "Associates an organization with a formal committee or group responsible for oversight." ;
    rdfs:domain [
        a owl:Class ;
        owl:unionOf ( ont:Company ont:Group ont:Organization )
    ] ;
    rdfs:range ont:GovernanceBody .

ont:hasGuidance
    a owl:ObjectProperty ;
    rdfs:comment "The issuance of a forward-looking statement about expected future performance." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:Guidance .

ont:hasHeadquartersIn
    a owl:ObjectProperty , owl:FunctionalProperty ;
    rdfs:comment "Specifies the primary legal and administrative location of an organization." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:City .

ont:hasHolder
    a owl:ObjectProperty , owl:FunctionalProperty ;
    rdfs:comment "Identifies the individual currently occupying a formal position." ;
    rdfs:domain ont:Position ;
    rdfs:range ont:Person .

ont:hasInitiative
    a owl:ObjectProperty ;
    rdfs:comment "Indicates that an organization has established a specific program to meet a goal." ;
    rdfs:domain [
        a owl:Class ;
        owl:unionOf ( ont:Company ont:Organization )
    ] ;
    rdfs:range ont:Initiative .

ont:hasInvestee
    a owl:ObjectProperty , owl:FunctionalProperty ;
    rdfs:comment "Identifies the entity that received capital in an investment event." ;
    rdfs:domain ont:Investment ;
    rdfs:range ont:Company .

ont:hasInvestmentEvent
    a owl:ObjectProperty ;
    rdfs:comment "A fact stating the specific event of allocating capital into an entity." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:Investment .

ont:hasInvestor
    a owl:ObjectProperty ;
    rdfs:comment "Identifies the entity that provided capital in an investment event." ;
    rdfs:domain ont:Investment ;
    rdfs:range ont:Company .

ont:hasManagementSystem
    a owl:ObjectProperty ;
    rdfs:comment "The existence of a formal set of processes and procedures used by an organization to achieve its objectives in a specific area, like compliance." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:ManagementSystem .

ont:hasMeasurement
    a owl:ObjectProperty ;
    rdfs:comment "Associates a specific recorded result or data point with its corresponding performance indicator." ;
    rdfs:domain ont:KeyPerformanceIndicator ;
    rdfs:range ont:MetricResult .

ont:hasMember
    a owl:ObjectProperty ;
    rdfs:comment "Signifies an individual's participation in a formal group or committee." ;
    rdfs:domain [
        a owl:Class ;
        owl:unionOf ( ont:GovernanceBody ont:GovernanceEntity )
    ] ;
    rdfs:range ont:Person ;
    owl:inverseOf ont:isMemberOf .

ont:hasMetric
    a owl:ObjectProperty ;
    rdfs:comment "Associates an organization or initiative with a specific, reported performance indicator (KPI), financial or non-financial." ;
    rdfs:domain ont:Organization ;
    rdfs:range [
        a owl:Class ;
        owl:unionOf ( ont:Metric ont:MetricReport ont:KeyPerformanceIndicator )
    ] .

ont:hasOffer
    a owl:ObjectProperty ;
    rdfs:comment "Indicates that a company provides a specific product or service to the market." ;
    rdfs:domain ont:Organization ;
    rdfs:range ont:Offer .

ont:hasOffice
    a owl:ObjectProperty ;
    rdfs:comment "Indicates the existence of a regional or specialized operational unit." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:Company .

ont:hasOrganizationalUnit
    a owl:ObjectProperty ;
    rdfs:comment "Associates an organization with a specific internal department or functional team." ;
    rdfs:domain ont:Organization ;
    rdfs:range ont:OrganizationalUnit .

ont:hasOwnershipStructure
    a owl:ObjectProperty , owl:FunctionalProperty ;
    rdfs:comment "A fact stating the formal arrangement of shareholdings for an entity." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:OwnershipStructure .

ont:hasParentCompany
    a owl:ObjectProperty , owl:AsymmetricProperty , owl:FunctionalProperty , owl:IrreflexiveProperty ;
    rdfs:comment "Identifies the controlling entity in a corporate hierarchy." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:Company ;
    owl:inverseOf ont:hasSubsidiary .

ont:hasParticipant
    a owl:ObjectProperty ;
    rdfs:comment "The inverse of participatesIn." ;
    rdfs:domain ont:TemporalEntity ;
    rdfs:range ont:Agent ;
    owl:inverseOf ont:participatesIn .

ont:hasPillar
    a owl:ObjectProperty ;
    rdfs:comment "Represents the thematic subdivision of a strategic program into key areas of focus." ;
    rdfs:domain ont:StrategicProgram ;
    rdfs:range ont:StrategicPillar .

ont:hasPlan
    a owl:ObjectProperty ;
    rdfs:comment "Indicates the adoption or use of a specific detailed proposal for action, such as a due diligence plan." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:Plan .

ont:hasPolicy
    a owl:ObjectProperty ;
    rdfs:comment "Associates an organization with a formal guiding principle or rule it has adopted." ;
    rdfs:domain [
        a owl:Class ;
        owl:unionOf ( ont:Company ont:Organization )
    ] ;
    rdfs:range ont:Policy .

ont:hasProgram
    a owl:ObjectProperty ;
    rdfs:comment "Associates an organization with a structured initiative or strategic program designed to meet a sustainability goal." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:Program .

ont:hasRating
    a owl:ObjectProperty ;
    rdfs:comment "Associates an entity with an external evaluation or score of its performance." ;
    rdfs:domain ont:Organization ;
    rdfs:range ont:Rating .

ont:hasReorganization
    a owl:ObjectProperty ;
    rdfs:comment "Indicates that an organization is undergoing a significant structural change." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:Reorganization .

ont:hasResult
    a owl:ObjectProperty ;
    rdfs:comment "Links a strategic target to its corresponding measured outcome for a specific reporting period." ;
    rdfs:domain ont:Target ;
    rdfs:range ont:MetricResult .

ont:hasRisk
    a owl:ObjectProperty ;
    rdfs:comment "The identification of a potential adverse event or condition relevant to an organization's objectives and compliance." ;
    rdfs:domain [
        a owl:Class ;
        owl:unionOf ( ont:Company ont:Group )
    ] ;
    rdfs:range ont:Risk .

ont:hasRole
    a owl:ObjectProperty , owl:FunctionalProperty ;
    rdfs:comment "Specifies the official capacity, position, or function performed during an event or within an organization." ;
    rdfs:domain [
        a owl:Class ;
        owl:unionOf ( ont:Agent ont:Appointment ont:Person ont:Position )
    ] ;
    rdfs:range ont:Role .

ont:hasSection
    a owl:ObjectProperty , owl:AsymmetricProperty , owl:IrreflexiveProperty ;
    rdfs:comment "Denotes the structural composition of a report, indicating its constituent parts." ;
    rdfs:domain ont:Report ;
    rdfs:range ont:ReportSection .

ont:hasShareholder
    a owl:ObjectProperty ;
    rdfs:comment "Identifies an entity or individual that owns shares in a corporation." ;
    rdfs:domain ont:OwnershipStructure ;
    rdfs:range ont:Company .

ont:hasShareholderType
    a owl:ObjectProperty ;
    rdfs:comment "Classifies a group of owners, such as employees or institutional investors." ;
    rdfs:domain ont:OwnershipStructure ;
    rdfs:range ont:EmployeeRepresentativeBody .

ont:hasShareholding
    a owl:ObjectProperty ;
    rdfs:comment "Indicates that an entity possesses an ownership stake in a company." ;
    rdfs:domain [
        a owl:Class ;
        owl:unionOf ( ont:Company ont:ShareholderGroup )
    ] ;
    rdfs:range ont:Shareholding ;
    owl:inverseOf ont:isShareholdingIn .

ont:hasSharePrice
    a owl:ObjectProperty ;
    rdfs:comment "The association of a specific market valuation per share with a company at a point in time." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:SharePrice .

ont:hasSolution
    a owl:ObjectProperty ;
    rdfs:comment "Indicates that a company has developed or offers a specific technical or commercial solution." ;
    rdfs:domain ont:Organization ;
    rdfs:range ont:Solution .

ont:hasStrategy
    a owl:ObjectProperty , owl:FunctionalProperty ;
    rdfs:comment "Associates an organization with its overarching plan for achieving long-term sustainability goals." ;
    rdfs:domain [
        a owl:Class ;
        owl:unionOf ( ont:Company ont:Organization )
    ] ;
    rdfs:range ont:StrategicPlan .

ont:hasSubKpi
    a owl:ObjectProperty , owl:AsymmetricProperty , owl:IrreflexiveProperty , owl:TransitiveProperty ;
    rdfs:comment "Signifies that a primary performance indicator is broken down into more detailed, component metrics." ;
    rdfs:domain ont:KeyPerformanceIndicator ;
    rdfs:range ont:KeyPerformanceIndicator ;
    rdfs:subPropertyOf ont:isBasedOn ;
    owl:inverseOf ont:isSubKpiOf .

ont:hasSubcommittee
    a owl:ObjectProperty , owl:AsymmetricProperty , owl:IrreflexiveProperty , owl:TransitiveProperty ;
    rdfs:comment "Indicates that a governance body has a more specialized committee reporting to it." ;
    rdfs:domain ont:GovernanceBody ;
    rdfs:range ont:GovernanceBody .

ont:hasSubscriptionEvent
    a owl:ObjectProperty ;
    rdfs:comment "Indicates that a corporate plan or program includes a specific offering for purchasing shares." ;
    rdfs:domain ont:EmployeeShareholderPlan ;
    rdfs:range ont:Subscription .

ont:hasSubsidiary
    a owl:ObjectProperty , owl:AsymmetricProperty , owl:IrreflexiveProperty ;
    rdfs:comment "Identifies a company that is controlled by a higher-level parent company." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:Company ;
    owl:inverseOf ont:hasParentCompany .

ont:hasTarget
    a owl:ObjectProperty ;
    rdfs:comment "Links a strategic plan or organization to a specific, measurable goal it aims to achieve." ;
    rdfs:domain [
        a owl:Class ;
        owl:unionOf ( ont:Company ont:Organization ont:StrategicPillar ont:StrategicProgram ont:StrategicPlan )
    ] ;
    rdfs:range ont:Target .

ont:hasWhistleblowingSystem
    a owl:ObjectProperty , owl:FunctionalProperty ;
    rdfs:comment "Associates an organization with a formal channel for reporting misconduct." ;
    rdfs:domain ont:Company ;
    rdfs:range ont:WhistleblowingSystem .

ont:heads
    a owl:ObjectProperty , owl:FunctionalProperty ;
    rdfs:comment "Indicates leadership of a specific organizational unit. Inverse of isHeadedBy." ;
    rdfs:domain ont:Role ;
    rdfs:range ont:OrganizationalUnit ;
    owl:inverseOf ont:isHeadedBy .

ont:identifiedIssue
    a owl:ObjectProperty ;
    rdfs:comment "Represents the outcome of an analysis that flagged a specific topic as being significant or material." ;
    rdfs:domain ont:Analysis ;
    rdfs:range ont:SustainabilityTopic .

ont:identifiesRisk
    a owl:ObjectProperty ;
    rdfs:comment "Indicates that a document or report section formally acknowledges a specific category of sustainability-related risk." ;
    rdfs:domain ont:ReportSection ;
    rdfs:range ont:RiskCategory .

ont:implements
    a owl:ObjectProperty ;
    rdfs:comment "Represents the act of putting a decision, plan, or strategy into effect." ;
    rdfs:domain [
        a owl:Class ;
        owl:unionOf ( ont:Company ont:GovernanceBody )
    ] ;
    rdfs:range [
        a owl:Class ;
        owl:unionOf ( ont:ActionPlan ont:StrategicPlan )
    ] .

ont:includes
    a owl:ObjectProperty , owl:AsymmetricProperty , owl:TransitiveProperty ;
    rdfs:comment "Signifies that a larger whole contains a smaller part, such as a report containing sections, policies, or metrics." ;
    rdfs:domain [
        a owl:Class ;
        owl:unionOf ( ont:FinancialStatement ont:IncentivePlan ont:Policy ont:RegulatoryFiling ont:ReportSection )
    ] ;
    rdfs:range [
        a owl:Class ;
        owl:unionOf ( ont:CapitalIncrease ont:EsgKeyPerformanceIndicator ont:FinancialStatement ont:FinancialMetric ont:Policy ont:ShareSale ont:Subscription ont:KeyPerformanceIndicator )
    ] .

ont:involves
    a owl:ObjectProperty ;
    rdfs:comment "Connects a temporal entity to a physical object that is central to it, but is not an agent." ;
    rdfs:domain ont:TemporalEntity ;
    rdfs:range ont:ConcreteEntity ;
    owl:inverseOf ont:wasInvolvedIn .

ont:isAdheredToBy
    a owl:ObjectProperty ;
    rdfs:comment "The inverse of adheresTo." ;
    rdfs:domain ont:AbstractConcept ;
    rdfs:range ont:Agent ;
    owl:inverseOf ont:adheresTo .

ont:isAlignedWith
    a owl:ObjectProperty ;
    rdfs:comment "Signifies that a corporate strategy or target conforms to the principles or recommendations of an external framework or scientific body." ;
    rdfs:domain ont:Plan ;
    rdfs:range [
        a owl:Class ;
        owl:unionOf ( ont:ExpertGroup ont:InternationalAgreement )
    ] .

ont:isAlumnusOf
    a owl:ObjectProperty ;
    rdfs:comment "Indicates that an individual has graduated from or formerly attended an educational institution." ;
    rdfs:domain ont:Person ;
    rdfs:range ont:University .

ont:isAppendixOf
    a owl:ObjectProperty , owl:AsymmetricProperty , owl:IrreflexiveProperty ;
    rdfs:comment "Signifies that a document serves as a supplementary section to a larger report." ;
    rdfs:domain ont:Document ;
    rdfs:range ont:Report ;
    rdfs:subPropertyOf ont:isPartOf .

ont:isAppointmentBy
    a owl:ObjectProperty , owl:FunctionalProperty ;
    rdfs:comment "Identifies the entity responsible for making a formal appointment." ;
    rdfs:domain ont:Appointment ;
    rdfs:range ont:Company .

ont:isAttachedTo
    a owl:ObjectProperty ;
    rdfs:comment "Represents a direct reporting or organizational link to a specific senior executive or board member." ;
    rdfs:domain ont:OrganizationalUnit ;
    rdfs:range ont:Person .

ont:isAuditOf
    a owl:ObjectProperty , owl:AsymmetricProperty , owl:FunctionalProperty , owl:IrreflexiveProperty ;
    rdfs:comment "Specifies that an assurance engagement was conducted on a particular report or set of disclosures." ;
    rdfs:domain ont:Audit ;
    rdfs:range ont:Report .

ont:isAuditedBy
    a owl:ObjectProperty ;
    rdfs:comment "Indicates the engagement of an external firm to formally verify corporate reporting." ;
    rdfs:domain [
        a owl:Class ;
