# Include model files (critical!)
include justembed/models/*.tar.gz
include justembed/models/*.onnx
include justembed/models/*.onnx.data

# Include documentation
include README.md
include LICENSE
include GETTING_STARTED.md

# Include examples
recursive-include examples *.py
recursive-include examples *.txt

# Exclude unnecessary files
global-exclude __pycache__
global-exclude *.py[co]
global-exclude .DS_Store
global-exclude *.so
global-exclude .git*
