    "issues": {
        "automatic_backup_agents_unavailable": {
            "description": "\u05de\u05d9\u05e7\u05d5\u05dd \u05d4\u05d2\u05d9\u05d1\u05d5\u05d9 `{agent_id}` \u05d0\u05d9\u05e0\u05d5 \u05d6\u05de\u05d9\u05df \u05d0\u05da \u05e2\u05d3\u05d9\u05d9\u05df \u05de\u05d5\u05d2\u05d3\u05e8 \u05e2\u05d1\u05d5\u05e8 \u05d2\u05d9\u05d1\u05d5\u05d9\u05d9\u05dd \u05d0\u05d5\u05d8\u05d5\u05de\u05d8\u05d9\u05d9\u05dd. \n \n\u05e0\u05d0 \u05dc\u05d1\u05e7\u05e8 \u05d1[\u05d3\u05e3 \u05ea\u05e6\u05d5\u05e8\u05ea \u05d4\u05d2\u05d9\u05d1\u05d5\u05d9 \u05d4\u05d0\u05d5\u05d8\u05d5\u05de\u05d8\u05d9]({backup_settings}) \u05db\u05d3\u05d9 \u05dc\u05e1\u05e7\u05d5\u05e8 \u05d5\u05dc\u05e2\u05d3\u05db\u05df \u05d0\u05ea \u05de\u05d9\u05e7\u05d5\u05de\u05d9 \u05d4\u05d2\u05d9\u05d1\u05d5\u05d9 \u05e9\u05dc\u05da. \u05dc\u05d0 \u05d9\u05d5\u05e2\u05dc\u05d5 \u05d2\u05d9\u05d1\u05d5\u05d9\u05d9\u05dd \u05dc\u05de\u05d9\u05e7\u05d5\u05de\u05d9\u05dd \u05e0\u05d1\u05d7\u05e8\u05d9\u05dd \u05e9\u05d0\u05d9\u05e0\u05dd \u05d6\u05de\u05d9\u05e0\u05d9\u05dd.",
            "title": "\u05de\u05d9\u05e7\u05d5\u05dd \u05d4\u05d2\u05d9\u05d1\u05d5\u05d9 {agent_id} \u05d0\u05d9\u05e0\u05d5 \u05d6\u05de\u05d9\u05df"
        },
        "automatic_backup_failed_addons": {
            "description": "\u05dc\u05d0 \u05e0\u05d9\u05ea\u05df \u05d4\u05d9\u05d4 \u05dc\u05db\u05dc\u05d5\u05dc \u05d0\u05ea \u05d4\u05d4\u05e8\u05d7\u05d1\u05d5\u05ea {failed_addons} \u05d1\u05d2\u05d9\u05d1\u05d5\u05d9 \u05d4\u05d0\u05d5\u05d8\u05d5\u05de\u05d8\u05d9. \u05e0\u05d0 \u05dc\u05d1\u05d3\u05d5\u05e7 \u05d0\u05ea \u05d9\u05d5\u05de\u05e0\u05d9 \u05d4\u05de\u05e4\u05e7\u05d7 \u05dc\u05e7\u05d1\u05dc\u05ea \u05de\u05d9\u05d3\u05e2 \u05e0\u05d5\u05e1\u05e3. \u05d9\u05d1\u05d5\u05e6\u05e2 \u05e0\u05d9\u05e1\u05d9\u05d5\u05df \u05e0\u05d5\u05e1\u05e3 \u05d1\u05de\u05d5\u05e2\u05d3 \u05d4\u05de\u05ea\u05d5\u05db\u05e0\u05df \u05d4\u05d1\u05d0 \u05d0\u05dd \u05d4\u05d5\u05d2\u05d3\u05e8 \u05dc\u05d5\u05d7 \u05d6\u05de\u05e0\u05d9\u05dd \u05dc\u05d2\u05d9\u05d1\u05d5\u05d9.",
            "title": "\u05dc\u05d0 \u05e0\u05d9\u05ea\u05df \u05dc\u05db\u05dc\u05d5\u05dc \u05d0\u05ea \u05db\u05dc \u05d4\u05d4\u05e8\u05d7\u05d1\u05d5\u05ea \u05d1\u05d2\u05d9\u05d1\u05d5\u05d9 \u05d0\u05d5\u05d8\u05d5\u05de\u05d8\u05d9"
        },
        "automatic_backup_failed_agents_addons_folders": {
            "description": "\u05d4\u05d2\u05d9\u05d1\u05d5\u05d9 \u05d4\u05d0\u05d5\u05d8\u05d5\u05de\u05d8\u05d9 \u05e0\u05d5\u05e6\u05e8 \u05e2\u05dd \u05e9\u05d2\u05d9\u05d0\u05d5\u05ea: \n * \u05de\u05d9\u05e7\u05d5\u05de\u05d9\u05dd \u05e9\u05d0\u05dc\u05d9\u05d4\u05dd \u05dc\u05d0 \u05e0\u05d9\u05ea\u05df \u05d4\u05d9\u05d4 \u05dc\u05d4\u05e2\u05dc\u05d5\u05ea \u05d0\u05ea \u05d4\u05d2\u05d9\u05d1\u05d5\u05d9: {failed_agents} \n * \u05d4\u05e8\u05d7\u05d1\u05d5\u05ea \u05e9\u05dc\u05d0 \u05e0\u05d9\u05ea\u05df \u05d4\u05d9\u05d4 \u05dc\u05d2\u05d1\u05d5\u05ea: {failed_addons} \n * \u05ea\u05d9\u05e7\u05d9\u05d5\u05ea \u05e9\u05dc\u05d0 \u05e0\u05d9\u05ea\u05df \u05d4\u05d9\u05d4 \u05dc\u05d2\u05d1\u05d5\u05ea: {failed_folders} \n \n \u05e0\u05d0 \u05dc\u05d1\u05d3\u05d5\u05e7 \u05d0\u05ea \u05d9\u05d5\u05de\u05e0\u05d9 \u05d4\u05dc\u05d9\u05d1\u05d4 \u05d5\u05d4\u05de\u05e0\u05d4\u05dc \u05dc\u05e7\u05d1\u05dc\u05ea \u05de\u05d9\u05d3\u05e2 \u05e0\u05d5\u05e1\u05e3. \u05d9\u05d1\u05d5\u05e6\u05e2 \u05e0\u05d9\u05e1\u05d9\u05d5\u05df \u05e0\u05d5\u05e1\u05e3 \u05d1\u05de\u05d5\u05e2\u05d3 \u05d4\u05de\u05ea\u05d5\u05db\u05e0\u05df \u05d4\u05d1\u05d0 \u05d0\u05dd \u05d4\u05d5\u05d2\u05d3\u05e8 \u05dc\u05d5\u05d7 \u05d6\u05de\u05e0\u05d9\u05dd \u05dc\u05d2\u05d9\u05d1\u05d5\u05d9.",
            "title": "\u05d2\u05d9\u05d1\u05d5\u05d9 \u05d0\u05d5\u05d8\u05d5\u05de\u05d8\u05d9 \u05e0\u05d5\u05e6\u05e8 \u05e2\u05dd \u05e9\u05d2\u05d9\u05d0\u05d5\u05ea"
        },
        "automatic_backup_failed_create": {
            "description": "\u05dc\u05d0 \u05d4\u05d9\u05ea\u05d4 \u05d0\u05e4\u05e9\u05e8\u05d5\u05ea \u05dc\u05d9\u05e6\u05d5\u05e8 \u05d0\u05ea \u05d4\u05d2\u05d9\u05d1\u05d5\u05d9 \u05d4\u05d0\u05d5\u05d8\u05d5\u05de\u05d8\u05d9. \u05e0\u05d0 \u05dc\u05d1\u05d3\u05d5\u05e7 \u05d1\u05d9\u05d5\u05de\u05e0\u05d9\u05dd \u05dc\u05e7\u05d1\u05dc\u05ea \u05de\u05d9\u05d3\u05e2 \u05e0\u05d5\u05e1\u05e3. \u05e0\u05d9\u05e1\u05d9\u05d5\u05df \u05e0\u05d5\u05e1\u05e3 \u05d9\u05d9\u05e2\u05e9\u05d4 \u05d1\u05de\u05d5\u05e2\u05d3 \u05d4\u05de\u05ea\u05d5\u05db\u05e0\u05df \u05d4\u05d1\u05d0 \u05d0\u05dd \u05d9\u05d5\u05d2\u05d3\u05e8 \u05dc\u05d5\u05d7 \u05d6\u05de\u05e0\u05d9\u05dd \u05dc\u05d2\u05d9\u05d1\u05d5\u05d9.",
            "title": "\u05dc\u05d0 \u05d4\u05d9\u05ea\u05d4 \u05d0\u05e4\u05e9\u05e8\u05d5\u05ea \u05dc\u05d9\u05e6\u05d5\u05e8 \u05d2\u05d9\u05d1\u05d5\u05d9 \u05d0\u05d5\u05d8\u05d5\u05de\u05d8\u05d9"
        },
        "automatic_backup_failed_folders": {
            "description": "\u05dc\u05d0 \u05e0\u05d9\u05ea\u05df \u05d4\u05d9\u05d4 \u05dc\u05db\u05dc\u05d5\u05dc \u05d0\u05ea \u05d4\u05ea\u05d9\u05e7\u05d9\u05d5\u05ea {failed_folders} \u05d1\u05d2\u05d9\u05d1\u05d5\u05d9 \u05d4\u05d0\u05d5\u05d8\u05d5\u05de\u05d8\u05d9. \u05e0\u05d0 \u05dc\u05d1\u05d3\u05d5\u05e7 \u05d0\u05ea \u05d9\u05d5\u05de\u05e0\u05d9 \u05d4\u05de\u05e4\u05e7\u05d7 \u05dc\u05e7\u05d1\u05dc\u05ea \u05de\u05d9\u05d3\u05e2 \u05e0\u05d5\u05e1\u05e3. \u05d9\u05d1\u05d5\u05e6\u05e2 \u05e0\u05d9\u05e1\u05d9\u05d5\u05df \u05e0\u05d5\u05e1\u05e3 \u05d1\u05de\u05d5\u05e2\u05d3 \u05d4\u05de\u05ea\u05d5\u05db\u05e0\u05df \u05d4\u05d1\u05d0 \u05d0\u05dd \u05d4\u05d5\u05d2\u05d3\u05e8 \u05dc\u05d5\u05d7 \u05d6\u05de\u05e0\u05d9\u05dd \u05dc\u05d2\u05d9\u05d1\u05d5\u05d9.",
            "title": "\u05dc\u05d0 \u05e0\u05d9\u05ea\u05df \u05d4\u05d9\u05d4 \u05dc\u05db\u05dc\u05d5\u05dc \u05d0\u05ea \u05db\u05dc \u05d4\u05ea\u05d9\u05e7\u05d9\u05d5\u05ea \u05d1\u05d2\u05d9\u05d1\u05d5\u05d9 \u05d0\u05d5\u05d8\u05d5\u05de\u05d8\u05d9"
        },
        "automatic_backup_failed_upload_agents": {
            "description": "\u05dc\u05d0 \u05d4\u05d9\u05ea\u05d4 \u05d0\u05e4\u05e9\u05e8\u05d5\u05ea \u05dc\u05d4\u05e2\u05dc\u05d5\u05ea \u05d0\u05ea \u05d4\u05d2\u05d9\u05d1\u05d5\u05d9 \u05d4\u05d0\u05d5\u05d8\u05d5\u05de\u05d8\u05d9 \u05dc\u05de\u05d9\u05e7\u05d5\u05de\u05d9\u05dd \u05e9\u05d4\u05d5\u05d2\u05d3\u05e8\u05d5 {failed_agents}. \u05e0\u05d0 \u05dc\u05d1\u05d3\u05d5\u05e7 \u05d1\u05d9\u05d5\u05de\u05e0\u05d9\u05dd \u05dc\u05e7\u05d1\u05dc\u05ea \u05de\u05d9\u05d3\u05e2 \u05e0\u05d5\u05e1\u05e3. \u05e0\u05d9\u05e1\u05d9\u05d5\u05df \u05e0\u05d5\u05e1\u05e3 \u05d9\u05d9\u05e2\u05e9\u05d4 \u05d1\u05de\u05d5\u05e2\u05d3 \u05d4\u05de\u05ea\u05d5\u05db\u05e0\u05df \u05d4\u05d1\u05d0 \u05d0\u05dd \u05d9\u05d5\u05d2\u05d3\u05e8 \u05dc\u05d5\u05d7 \u05d6\u05de\u05e0\u05d9\u05dd \u05dc\u05d2\u05d9\u05d1\u05d5\u05d9.",
            "title": "\u05dc\u05d0 \u05e0\u05d9\u05ea\u05df \u05d4\u05d9\u05d4 \u05dc\u05d4\u05e2\u05dc\u05d5\u05ea \u05d2\u05d9\u05d1\u05d5\u05d9 \u05d0\u05d5\u05d8\u05d5\u05de\u05d8\u05d9 \u05dc\u05de\u05d9\u05e7\u05d5\u05de\u05d9\u05dd \u05d4\u05de\u05d5\u05d2\u05d3\u05e8\u05d9\u05dd"
        }
    },
