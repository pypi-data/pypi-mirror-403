    "issues": {
        "issue_addon_boot_fail": {
            "fix_flow": {
                "abort": {
                    "apply_suggestion_fail": "Kan de oplossing niet toepassen. Controleer de logboeken van de supervisor voor meer details."
                },
                "step": {
                    "fix_menu": {
                        "menu_options": {
                            "addon_disable_boot": "Uitschakelen",
                            "addon_execute_start": "Starten"
                        }
                    }
                }
            }
        },
        "issue_mount_mount_failed": {
            "fix_flow": {
                "abort": {
                    "apply_suggestion_fail": "Kon de fix niet toepassen. Controleer de logboeken van Supervisor voor meer details."
                },
                "step": {
                    "fix_menu": {
                        "description": "Kan geen verbinding maken met `{reference}`. Controleer de hostlogs voor fouten van de mount-service voor meer details.\n\nGebruik reload om opnieuw te proberen te verbinden. Als je iets van `{reference}` moet bijwerken, ga dan naar [storage]({storage_url}).",
                        "menu_options": {
                            "mount_execute_reload": "Herladen",
                            "mount_execute_remove": "Verwijderen"
                        }
                    }
                }
            },
            "title": "Netwerkopslagapparaat mislukt"
        },
        "issue_system_disk_lifetime": {
            "description": "De dataschijf heeft meer dan 90% van zijn verwachte levensduur bereikt. De schijf kan binnenkort defect raken, wat kan leiden tot gegevensverlies. Vervang de schijf zo snel mogelijk en migreer je gegevens.",
            "title": "Schijflevensduur overschrijdt 90%"
        },
        "issue_system_docker_config": {
            "fix_flow": {
                "abort": {
                    "apply_suggestion_fail": "Een of meer herstarts zijn mislukt. Controleer de supervisorlogboeken voor meer details."
                }
            },
            "title": "Herstart vereist"
        },
        "issue_system_free_space": {
            "description": "De data-disk heeft nog maar {free_space}GB vrije ruimte beschikbaar. Dit kan problemen veroorzaken met de stabiliteit van het systeem en invloed hebben op functionaliteit zoals backups en updates. Ga naar [opslag]({storage_url}) om te zien wat de ruimte inneemt of bekijk [opslag opruimen]({more_info_free_space}) voor tips hoe meer ruimte kan worden vrijgemaakt.",
            "title": "De data-disk begint vol te raken"
        },
        "issue_system_multiple_data_disks": {
            "fix_flow": {
                "step": {
                    "fix_menu": {
                        "description": "Op `{reference}` hebben we nog een andere actieve datadisk (met een `hassos-data` bestandssysteem van een andere Home Assistant installatie) gevonden.\n\nJe moet besluiten wat je hiermee wilt doen, anders zou Home Assistant de verkeerde datadisk partitie kunnen kiezen wanneer het systeem opnieuw start.\n\nAls je de datadisk niet wilt gebruiken, koppel deze dan los van je systeem. Als je de disk wilt houden, kies dan \u00e9\u00e9n van de volgende opties:",
                        "menu_options": {
                            "system_adopt_data_disk": "Gebruik de gedetecteerde datadisk in plaats van het huidige systeemdisk",
                            "system_rename_data_disk": "Markeren als inactieve datadisk (hernoemen bestandssysteem)"
                        }
                    }
                }
            },
            "title": "Meerdere dataschijven gedetecteerd"
        },
