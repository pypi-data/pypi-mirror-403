CHARGE, TRP01, " CH2":  0.000
CHARGE, TRP01, " HH2":  0.000

# Atom radius, dielelctric boundary radius, VDW radius, and energy well depth
RADIUS, TRPBK, " N  ": 1.500, 1.824, 0.170
RADIUS, TRPBK, " H  ": 1.000, 0.600, 0.016
RADIUS, TRPBK, " CA ": 2.000, 1.908, 0.109
