# Conformer parameters that appear in head3.lst: ne, Em0, nH, pKa0, rxn
CONFORMER, TRP01: Em0=   0.0, pKa0=  0.00, ne= 0, nH= 0, rxn02=  0.000, rxn04=  0.000, rxn08=  0.000

# Rotatable bonds. The atoms extended in the bond direction will all be rotated.
ROTATE, TRP: " CA " - " CB ", " CB " - " CG "

# Torsion energy parameters
# KEY, CONF, ATOM0: ATOM1, ATOM2, ATOM3, (Vn/2(kcal),  n_fold,   gamma), ...
TORSION, TRP, " CG ": " CB ", " CA ", " N  ", (0.000, 6, 180.00), (1.400, 3, 0.00)
TORSION, TRP, " CD1": " CG ", " CB ", " CA ", (0.000, 6, 180.00)
TORSION, TRP, " NE1": " CD1", " CG ", " CB ", (0.000, 1, 0.00)
TORSION, TRP, " CE2": " CD2", " CG ", " CB ", (0.000, 1, 0.00)
TORSION, TRP, " CZ3": " CE3", " CD2", " CG ", (0.000, 1, 0.00)
TORSION, TRP, " CH2": " CZ2", " CE2", " CD2", (0.000, 1, 180.00)