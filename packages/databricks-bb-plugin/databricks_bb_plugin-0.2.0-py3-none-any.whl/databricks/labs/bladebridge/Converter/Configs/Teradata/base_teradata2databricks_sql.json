		{"from" : "PERIOD\(DATE\) FORMAT 'YY\/MM\/DD' AS VALIDTIME", "to" : "TIMESTAMP(0)"},
		{"from" : "PERIOD\(TIMESTAMP\(6\) WITH TIME ZONE\) NOT NULL AS TRANSACTIONTIME", "to" : "TIMESTAMP(6) default current_TIMESTAMP"},
