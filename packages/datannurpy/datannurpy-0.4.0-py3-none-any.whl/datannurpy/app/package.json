{
  "name": "datannur-app",
  "version": "0.0.3",
  "type": "module",
  "description": "datannur app",
  "author": {
    "name": "datannur",
    "url": "https://datannur.com"
  },
  "license": "MIT",
  "datannur": {
    "dbPath": "data/db",
    "schemasPath": "schemas",
    "apiVersion": "1.0.0",
    "schemasHash": "b0753f73",
    "openApiVersion": "3.1.0"
  },
  "scripts": {
    "node:ts": "node --experimental-strip-types --disable-warning=ExperimentalWarning",
    "static-make": "npm run node:ts node-scripts/static-make.ts",
    "deploy": "npm run node:ts node-scripts/deploy.ts",
    "sync-db": "npm run node:ts node-scripts/sync-db.ts",
    "validate-schemas": "npm run node:ts node-scripts/validate-schemas.ts",
    "schema:generate": "npm run node:ts node-scripts/manage-schemas.ts generate",
    "schema:update": "npm run node:ts node-scripts/manage-schemas.ts update",
    "schema:reset": "npm run node:ts node-scripts/manage-schemas.ts reset",
    "openapi:generate": "npm run node:ts node-scripts/generate-api.ts",
    "openapi:generate:dev": "NODE_ENV=development npm run node:ts node-scripts/generate-api.ts",
    "schema:build": "npm run validate-schemas && npm run openapi:generate",
    "api:dev": "npm run node:ts api/nodejs/server.ts"
  },
  "dependencies": {
    "jsonjsdb-builder": "^0.6.5",
    "svelte-fileapp": "^0.0.13"
  }
}
