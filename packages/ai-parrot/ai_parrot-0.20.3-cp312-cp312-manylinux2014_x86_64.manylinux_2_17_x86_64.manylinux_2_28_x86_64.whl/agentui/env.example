# AgentUI configuration
# Copy to .env and adjust for your environment

# Base URL for all Agent/Bot/Chat endpoints
# Defaults to http://localhost:5000 when omitted
VITE_API_URL=http://localhost:5000

# Optional override for the login endpoint. Falls back to ${VITE_API_URL}/api/v1/login
VITE_AUTH_URL=http://localhost:5000/api/v1/login

# Displayed badge + namespace suffix (useful when pointing to staging/prod)
VITE_AGENTUI_ENV=local

# Optional default credentials to prefill the login form in development
VITE_AGENTUI_USERNAME=
VITE_AGENTUI_PASSWORD=

# Optional AI Features (if needed for future integrations)
# OPENAI_API_KEY=
# ELEVENLABS_API_KEY=

