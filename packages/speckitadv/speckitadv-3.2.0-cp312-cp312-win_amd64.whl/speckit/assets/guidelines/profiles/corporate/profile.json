{
  "name": "corporate",
  "displayName": "Corporate/Enterprise",
  "description": "Guidelines for corporate and enterprise projects with proprietary code, internal tools, and commercial applications",
  "version": "3.0",
  "inherits": ["base"],
  "overrides": {
    "reactjs": "reactjs-overrides.md",
    "nodejs": "nodejs-overrides.md",
    "java": "java-overrides.md",
    "python": "python-overrides.md",
    "dotnet": "dotnet-overrides.md"
  },
  "characteristics": {
    "packageRegistry": "corporate (Artifactory, Nexus, Azure Artifacts, GitHub Packages)",
    "requiredLibraries": [
      "@YOUR_ORG/ui-components",
      "@YOUR_ORG/auth-client",
      "@YOUR_ORG/api-client",
      "@YOUR_ORG/logger",
      "@YOUR_ORG/analytics"
    ],
    "licensing": "proprietary",
    "securityLevel": "high",
    "complianceRequired": true,
    "auditTrail": true
  },
  "detectionMarkers": {
    "files": [
      ".corporate",
      "CORPORATE_LICENSE",
      "organization.json"
    ],
    "packageJsonFields": [
      "private: true",
      "@yourorg/* dependencies"
    ],
    "folderPatterns": [
      "internal/",
      "proprietary/"
    ]
  },
  "applicableTo": [
    "Internal tools",
    "Enterprise applications",
    "Commercial products",
    "Proprietary software",
    "Client deliverables",
    "Regulated industries (finance, healthcare)"
  ],
  "support": {
    "documentation": "https://wiki.yourorg.com/guidelines",
    "slack": "#guidelines-support",
    "email": "architecture@yourorg.com",
    "officeHours": "Tuesdays 2-3 PM EST"
  },
  "lastUpdated": "2025-11-16",
  "owner": "Architecture Team"
}
