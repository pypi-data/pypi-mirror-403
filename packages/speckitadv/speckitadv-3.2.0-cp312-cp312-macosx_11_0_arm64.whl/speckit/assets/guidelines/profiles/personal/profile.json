{
  "name": "personal",
  "displayName": "Personal/Public/Open Source",
  "description": "Guidelines for personal projects, open-source software, and public repositories",
  "version": "3.0",
  "inherits": ["base"],
  "overrides": {
    "reactjs": "reactjs-overrides.md",
    "nodejs": "nodejs-overrides.md",
    "java": "java-overrides.md",
    "python": "python-overrides.md",
    "dotnet": "dotnet-overrides.md"
  },
  "characteristics": {
    "packageRegistry": "public (npmjs.org, PyPI, Maven Central, NuGet)",
    "recommendedLibraries": [
      "Community-maintained packages",
      "Popular open-source libraries",
      "Free-tier services"
    ],
    "licensing": "open-source (MIT, Apache 2.0, GPL, etc.)",
    "securityLevel": "standard",
    "complianceRequired": false,
    "auditTrail": false
  },
  "detectionMarkers": {
    "files": [
      ".opensource",
      "LICENSE",
      "MIT_LICENSE",
      "CONTRIBUTING.md"
    ],
    "packageJsonFields": [
      "private: false",
      "license: MIT/Apache/GPL"
    ],
    "folderPatterns": [
      "examples/",
      "demo/"
    ]
  },
  "applicableTo": [
    "Personal projects",
    "Open-source libraries",
    "Side projects",
    "Learning projects",
    "Portfolio projects",
    "Community contributions",
    "Public tools and utilities"
  ],
  "resources": {
    "documentation": [
      "https://react.dev",
      "https://nodejs.org/docs",
      "https://www.typescriptlang.org/docs"
    ],
    "communities": [
      "https://discord.gg/react",
      "https://stackoverflow.com",
      "https://dev.to"
    ],
    "freeTierServices": {
      "hosting": ["Vercel", "Netlify", "Cloudflare Pages"],
      "databases": ["Supabase", "PlanetScale", "Neon"],
      "auth": ["Clerk", "Supabase Auth", "Auth0"],
      "monitoring": ["Sentry", "LogRocket"]
    }
  },
  "philosophy": "Developer experience, community packages, cost-effective solutions. Start simple, scale as needed.",
  "lastUpdated": "2025-11-16",
  "maintainedBy": "Open Source Community"
}
