<definitions
        xmlns:xsd="http://www.w3.org/2001/XMLSchema"
        xmlns:tns="urn:ws"
        xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
        xmlns="http://schemas.xmlsoap.org/wsdl/"
        targetNamespace="urn:ws">
    <!--  definition of datatypes  -->
    <types>
        <xsd:schema elementFormDefault="qualified" targetNamespace="urn:ws">
            <xsd:import namespace="http://schemas.xmlsoap.org/soap/encoding/"/>
            <xsd:import namespace="http://schemas.xmlsoap.org/wsdl/"/>
            <xsd:complexType name="message">
                <xsd:sequence>
                    <xsd:element name="value" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="SendFileRequestType">
                <xsd:sequence>
                    <xsd:element name="attr" type="xsd:boolean"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="FilesService.SendFilesRequestType">
                <xsd:sequence>
                    <xsd:element name="attr" type="xsd:boolean"/>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="SendTwoFilesRequestType">
                <xsd:sequence/>
            </xsd:complexType>
            <xsd:complexType name="CommonClientService.postRequestType">
                <xsd:sequence>
                    <xsd:element name="attr" type="xsd:boolean"/>
                    <xsd:element name="nested">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="attr" type="xsd:boolean"/>
                                <xsd:element name="nested">
                                    <xsd:complexType>
                                        <xsd:sequence>
                                            <xsd:element name="attr" type="xsd:boolean"/>
                                        </xsd:sequence>
                                    </xsd:complexType>
                                </xsd:element>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:complexType name="CommonClientService.postWithNameRequestType">
                <xsd:sequence>
                    <xsd:element name="attr" type="xsd:boolean"/>
                    <xsd:element name="nested">
                        <xsd:complexType>
                            <xsd:sequence>
                                <xsd:element name="attr" type="xsd:boolean"/>
                                <xsd:element name="nested">
                                    <xsd:complexType>
                                        <xsd:sequence>
                                            <xsd:element name="attr" type="xsd:boolean"/>
                                        </xsd:sequence>
                                    </xsd:complexType>
                                </xsd:element>
                            </xsd:sequence>
                        </xsd:complexType>
                    </xsd:element>
                </xsd:sequence>
            </xsd:complexType>
            <xsd:element name="SendFileRequest" type="tns:SendFileRequestType"/>
            <xsd:element name="SendTwoFilesRequest" type="tns:SendTwoFilesRequestType"/>
            <xsd:element name="FilesService.SendFilesRequest" type="tns:FilesService.SendFilesRequestType"/>
            <xsd:element name="CommonClientService.postRequest" type="tns:CommonClientService.postRequestType"/>
            <xsd:element name="CommonClientService.postWithNameRequest"
                         type="tns:CommonClientService.postWithNameRequestType"/>
        </xsd:schema>
    </types>
    <!--  response messages  -->
    <message name="returns_message">
        <part name="message" type="tns:message"/>
    </message>
    <!--  request messages  -->
    <message name="sendFile">
        <part name="parameters" element="tns:SendFileRequest"/>
    </message>
    <message name="sendTwoFiles">
        <part name="parameters" element="tns:SendTwoFilesRequest"/>
    </message>
    <message name="FilesService.sendFiles">
        <part name="parameters" element="tns:FilesService.SendFilesRequest"/>
    </message>
    <message name="CommonService.post">
        <part name="parameters" element="tns:CommonClientService.postRequest"/>
    </message>
    <message name="CommonService.postWithName">
        <part name="parameters" element="tns:CommonClientService.postWithNameRequest"/>
    </message>
    <!--  server's services  -->
    <portType name="wsPortType">
        <operation name="sendFile">
            <input message="tns:sendFile"/>
            <output message="tns:returns_message"/>
        </operation>
        <operation name="sendTwoFiles">
            <input message="tns:sendTwoFiles"/>
            <output message="tns:returns_message"/>
        </operation>
        <operation name="FilesService.sendFiles">
            <input message="tns:FilesService.sendFiles"/>
            <output message="tns:returns_message"/>
        </operation>
        <operation name="CommonService.post">
            <input message="tns:CommonService.post"/>
            <output message="tns:returns_message"/>
        </operation>
        <operation name="CommonService.postWithName">
            <input message="tns:CommonService.postWithName"/>
            <output message="tns:returns_message"/>
        </operation>
    </portType>
    <!--  server encoding  -->
    <binding name="wsBinding" type="tns:wsPortType">
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
        <operation name="sendFile">
            <soap:operation soapAction="urn:ws#sendFile"/>
            <input>
                <soap:body use="encoded" namespace="urn:ws"
                           encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
            </input>
            <output>
                <soap:body use="encoded" namespace="urn:ws"
                           encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
            </output>
        </operation>
        <operation name="sendTwoFiles">
            <soap:operation soapAction="urn:ws#sendTwoFiles"/>
            <input>
                <soap:body use="encoded" namespace="urn:ws"
                           encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
            </input>
            <output>
                <soap:body use="encoded" namespace="urn:ws"
                           encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
            </output>
        </operation>
        <operation name="FilesService.sendFiles">
            <soap:operation soapAction="urn:ws#FilesService.sendFiles"/>
            <input>
                <soap:body use="encoded" namespace="urn:ws"
                           encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
            </input>
            <output>
                <soap:body use="encoded" namespace="urn:ws"
                           encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
            </output>
        </operation>
        <operation name="CommonService.post">
            <soap:operation soapAction="urn:ws#CommonService.post"/>
            <input>
                <soap:body use="encoded" namespace="urn:ws"
                           encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
            </input>
            <output>
                <soap:body use="encoded" namespace="urn:ws"
                           encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
            </output>
        </operation>
        <operation name="CommonService.postWithName">
            <soap:operation soapAction="urn:ws#CommonService.postWithName"/>
            <input>
                <soap:body use="encoded" namespace="urn:ws"
                           encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
            </input>
            <output>
                <soap:body use="encoded" namespace="urn:ws"
                           encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
            </output>
        </operation>
    </binding>
    <!--  access to service provider  -->
    <service name="ws">
        <port name="wsPort" binding="tns:wsBinding">
            <soap:address location="http://127.0.0.1"/>
        </port>
    </service>
</definitions>
