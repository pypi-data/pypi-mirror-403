# Readwise API Token
# Get your token from: https://readwise.io/access_token
READWISE_TOKEN=your_readwise_token_here

# Optional: Rate limiting configuration
# Delay between pagination requests in seconds (default: 0.2)
# Increase if you're hitting rate limits frequently
READWISE_RATE_LIMIT_DELAY=0.2

# Optional: Maximum fetch limit for unlimited queries (default: 5000)
# This is used as a global default when fetch_all=True
READWISE_MAX_FETCH_LIMIT=5000