{% autoescape false %}
[build-system]
requires = ["setuptools>=68", "wheel"]
build-backend = "setuptools.build_meta"

[project]
name = "{{ name }}"
version = "0.1.0"
requires-python = ">=3.10"

dependencies = [
{%- for dep in python_dependencies %}
    "{{ dep }}"{% if not loop.last %},{% endif %}
{%- endfor %}
]
{%- endautoescape %}
