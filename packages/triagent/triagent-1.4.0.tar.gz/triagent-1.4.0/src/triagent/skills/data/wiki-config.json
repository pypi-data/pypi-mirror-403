{
  "organization": "symphonyvsts",
  "project": "Audit Cortex 2",
  "wikiIdentifier": "Audit-Cortex-2.wiki",
  "apiVersion": "7.0",
  "basePath": "/Omnia-Data-Wiki/Backlog-Management-Guidance/ADO-Work-Item-Guidance",
  "pages": {
    "epic": "/ADO-Work-Item-Hierarchy/Epic-Guidance",
    "feature": "/ADO-Work-Item-Hierarchy/Feature-Guidance",
    "enabler": "/ADO-Work-Item-Hierarchy/Enabler-Guidance",
    "pbi": "/ADO-Work-Item-Hierarchy/Product-Backlog-Item-Guidance",
    "defect": "/ADO-Work-Item-Hierarchy/Defect-Guidance",
    "defect-template": "/ADO-Work-Item-Hierarchy/Defect-Guidance/Defect-Template",
    "dtr": "/Deliver-Team-Request-Guidance",
    "issue": "/ADO-Work-Item-Hierarchy/Issue-Guidance",
    "lsi": "/ADO-Work-Item-Hierarchy/Live-Site-Incident-Guidance",
    "release-scope": "/Release-Scope-Approved-Field-Guidance",
    "target-release": "/Target-Release-(Cortex-Release)-Guidance",
    "work-item-types": "/Guidance-on-Work-Item-Categorization",
    "pbi-approval": "/ADO-Work-Item-Hierarchy/Product-Backlog-Item-Guidance/PBI-Product-Owner-Approval",
    "estimating": "/Estimating-Guidance/Estimating-Guidance",
    "ccb": "/Change-Control-Board-Process/Change-Control-Board-Process"
  },
  "mcpTools": {
    "primary": "mcp__azure-devops-wiki__get_page",
    "search": "mcp__azure-devops-wiki__search_wiki"
  },
  "apiEndpoints": {
    "getPage": "https://dev.azure.com/{organization}/{project}/_apis/wiki/wikis/{wikiIdentifier}/pages?path={path}&includeContent=true&api-version={apiVersion}",
    "searchWiki": "https://dev.azure.com/{organization}/{project}/_apis/wiki/wikis/{wikiIdentifier}/search?searchText={searchText}&api-version={apiVersion}"
  },
  "azCliCommands": {
    "getPage": "az devops invoke --org https://dev.azure.com/{organization} --area wiki --resource pages --route-parameters project=\"{project}\" wikiIdentifier=\"{wikiIdentifier}\" --query-parameters path=\"{basePath}{pagePath}\" includeContent=true --api-version {apiVersion}",
    "getAccessToken": "az account get-access-token --resource 499b84ac-1321-427f-aa17-267ca6975798 --query accessToken -o tsv"
  }
}
