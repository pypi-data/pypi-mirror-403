from _typeshed import Incomplete
from minjiang_host_client.utils.request import request as request
from typing import Any

class WorkerRegistry:
    workers: dict[str, dict[str, Any]]
    lock: Incomplete
    last_update: Incomplete
    def __init__(self) -> None: ...
    def register_worker(self, worker_name: str, worker_type: str, monitor_port: int): ...
    def unregister_worker(self, worker_name: str): ...
    def update_worker_status(self, worker_name: str, status: dict[str, Any]): ...
    def update_worker_metrics(self, worker_name: str, metrics: dict[str, Any]): ...
    def get_all_workers(self) -> dict[str, dict[str, Any]]: ...
    def get_worker_status(self, worker_name: str) -> dict[str, Any] | None: ...
    def get_worker_metrics(self, worker_name: str) -> dict[str, Any] | None: ...

class WorkerMonitorClient:
    registry: Incomplete
    running: bool
    monitor_thread: Incomplete
    update_interval: float
    cached_status: Incomplete
    cache_timestamp: int
    cache_ttl: float
    start_time: Incomplete
    def __init__(self, registry: WorkerRegistry) -> None: ...
    def start(self) -> None: ...
    def stop(self) -> None: ...
