from _typeshed import Incomplete
from minjiang_host_client.base.monitor import WorkerMonitor as WorkerMonitor
from typing import Any

class PullerMonitor(WorkerMonitor):
    connection_status: Incomplete
    task_queue_size: int
    transfer_queue_size: int
    direct_link_tasks: int
    cloud_tasks: int
    def __init__(self, worker_name: str, direct_link_server: str = 'localhost', direct_link_port: int = 6887) -> None: ...
    def get_status(self) -> dict[str, Any]: ...
    def get_metrics(self) -> dict[str, Any]: ...
    def update_queue_sizes(self, task_queue_size: int, transfer_queue_size: int): ...
    def increment_direct_link_task(self) -> None: ...
    def increment_cloud_task(self) -> None: ...
    def set_connection_status(self, status: bool): ...
    def record_experiment_title(self, exp_id: int = None, direct_exp_id: int = None, title: str = 'NO TITLE'): ...

class CompilerMonitor(WorkerMonitor):
