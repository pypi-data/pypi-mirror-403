msgid "AnyEncrypt - 简单易用的加密解密命令行工具\n\n支持文本和文件的加密解密操作。"
msgstr "AnyEncrypt - Outil CLI de chiffrement/déchiffrement simple et facile à utiliser\n\nPrend en charge le chiffrement et le déchiffrement de texte et de fichiers."

msgid "加密文本或文件\n\n示例：\n\b\n    # 加密文本\n    anyencrypt encrypt -t \"Hello World\"\n\n    # 加密文件\n    anyencrypt encrypt -f input.txt -o encrypted.bin"
msgstr "Chiffrer un texte ou un fichier\n\nExemples:\n\b\n    # Chiffrer un texte\n    anyencrypt encrypt -t \"Hello World\"\n\n    # Chiffrer un fichier\n    anyencrypt encrypt -f input.txt -o encrypted.bin"

msgid "解密文本或文件\n\n示例：\n\b\n    # 解密文本\n    anyencrypt decrypt -t \"gAAAAAB...\"\n\n    # 解密文件\n    anyencrypt decrypt -f encrypted.bin -o decrypted.txt"
msgstr "Déchiffrer un texte ou un fichier\n\nExemples:\n\b\n    # Déchiffrer un texte\n    anyencrypt decrypt -t \"gAAAAAB...\"\n\n    # Déchiffrer un fichier\n    anyencrypt decrypt -f encrypted.bin -o decrypted.txt"

msgid "交互式模式 - 引导用户完成加密/解密操作"
msgstr "Mode interactif - guide l'utilisateur pour le chiffrement/déchiffrement"

msgid "要加密的文本"
msgstr "Texte à chiffrer"

msgid "要加密的文件路径"
msgstr "Chemin du fichier à chiffrer"

msgid "输出文件路径（文件加密时使用）"
msgstr "Chemin du fichier de sortie (utilisé pour le chiffrement de fichiers)"

msgid "加密密码（不建议使用，会在命令历史中留下记录）"
msgstr "Mot de passe de chiffrement (non recommandé, restera dans l'historique)"

msgid "请输入加密密码: "
msgstr "Veuillez saisir le mot de passe de chiffrement: "

msgid "请再次输入密码: "
msgstr "Veuillez ressaisir le mot de passe: "

msgid "错误: 两次输入的密码不一致"
msgstr "Erreur: les deux mots de passe ne correspondent pas"

msgid "错误: 密码不能为空"
msgstr "Erreur: le mot de passe ne peut pas être vide"

msgid "\n加密结果:"
msgstr "\nRésultat du chiffrement:"

msgid "✓ 文件加密成功: {output}"
msgstr "✓ Fichier chiffré avec succès: {output}"

msgid "错误: 请指定要加密的文本 (-t) 或文件 (-f)"
msgstr "Erreur: veuillez spécifier le texte (-t) ou le fichier (-f) à chiffrer"

msgid "错误: {error}"
msgstr "Erreur: {error}"

msgid "要解密的文本"
msgstr "Texte à déchiffrer"

msgid "要解密的文件路径"
msgstr "Chemin du fichier à déchiffrer"

msgid "输出文件路径（文件解密时使用）"
msgstr "Chemin du fichier de sortie (utilisé pour le déchiffrement de fichiers)"

msgid "解密密码（不建议使用，会在命令历史中留下记录）"
msgstr "Mot de passe de déchiffrement (non recommandé, restera dans l'historique)"

msgid "请输入解密密码: "
msgstr "Veuillez saisir le mot de passe de déchiffrement: "

msgid "\n解密结果:"
msgstr "\nRésultat du déchiffrement:"

msgid "✓ 文件解密成功: {output}"
msgstr "✓ Fichier déchiffré avec succès: {output}"

msgid "错误: 请指定要解密的文本 (-t) 或文件 (-f)"
msgstr "Erreur: veuillez spécifier le texte (-t) ou le fichier (-f) à déchiffrer"

msgid "  AnyEncrypt - 加密解密工具"
msgstr "  AnyEncrypt - Outil de chiffrement/déchiffrement"

msgid "请选择操作类型:"
msgstr "Veuillez sélectionner le type d'opération:"

msgid "  [1] 加密"
msgstr "  [1] Chiffrer"

msgid "  [2] 解密"
msgstr "  [2] Déchiffrer"

msgid "请输入选项"
msgstr "Saisissez l'option"

msgid "错误: 无效的选项"
msgstr "Erreur: option invalide"

msgid "加密"
msgstr "Chiffrer"

msgid "解密"
msgstr "Déchiffrer"

msgid "请选择处理类型:"
msgstr "Veuillez sélectionner le type de traitement:"

msgid "  [1] 文本"
msgstr "  [1] Texte"

msgid "  [2] 文件"
msgstr "  [2] Fichier"

msgid "请输入{operation}密码: "
msgstr "Saisissez le mot de passe de {operation}: "

msgid "请输入要加密的文本"
msgstr "Saisissez le texte à chiffrer"

msgid "✓ 加密成功!"
msgstr "✓ Chiffrement réussi!"

msgid "加密结果:"
msgstr "Résultat du chiffrement:"

msgid "请输入要解密的密文"
msgstr "Saisissez le texte chiffré à déchiffrer"

msgid "✓ 解密成功!"
msgstr "✓ Déchiffrement réussi!"

msgid "解密结果:"
msgstr "Résultat du déchiffrement:"

msgid "请输入要加密的文件路径"
msgstr "Saisissez le chemin du fichier à chiffrer"

msgid "错误: 文件不存在: {path}"
msgstr "Erreur: fichier introuvable: {path}"

msgid "请输入输出文件路径"
msgstr "Saisissez le chemin du fichier de sortie"

msgid "✓ 文件加密成功: {path}"
msgstr "✓ Fichier chiffré avec succès: {path}"

msgid "请输入要解密的文件路径"
msgstr "Saisissez le chemin du fichier à déchiffrer"

msgid "✓ 文件解密成功: {path}"
msgstr "✓ Fichier déchiffré avec succès: {path}"

msgid "加密解密核心功能模块\n使用 Fernet (对称加密) 进行加密和解密操作"
msgstr "Module principal de chiffrement/déchiffrement\nUtilise Fernet (chiffrement symétrique) pour chiffrer et déchiffrer"

msgid "从密码派生出32字节的密钥\n使用 SHA-256 哈希算法"
msgstr "Dériver une clé de 32 octets à partir du mot de passe\nUtilise l'algorithme de hachage SHA-256"

msgid "内部辅助函数"
msgstr "Fonction d'assistance interne"

msgid "加密文本\n\nArgs:\n    text: 要加密的文本\n    password: 加密密码\n\nReturns:\n    加密后的文本（Base64编码）\n\nRaises:\n    Exception: 加密失败时抛出异常"
msgstr "Chiffrer un texte\n\nArgs:\n    text: Texte à chiffrer\n    password: Mot de passe de chiffrement\n\nReturns:\n    Texte chiffré (encodé en Base64)\n\nRaises:\n    Exception: Levée en cas d'échec du chiffrement"

msgid "解密文本\n\nArgs:\n    encrypted_text: 加密的文本\n    password: 解密密码\n\nReturns:\n    解密后的原文本\n\nRaises:\n    Exception: 解密失败时抛出异常（可能是密码错误或数据损坏）"
msgstr "Déchiffrer un texte\n\nArgs:\n    encrypted_text: Texte chiffré\n    password: Mot de passe de déchiffrement\n\nReturns:\n    Texte original déchiffré\n\nRaises:\n    Exception: Levée en cas d'échec du déchiffrement (mot de passe incorrect ou données corrompues)"

msgid "判断文件是否为可读文本文件\n\nArgs:\n    data: 文件的二进制内容\n    max_size_mb: 最大文件大小限制(MB),默认10MB\n\nReturns:\n    (是否为文本文件, 解码后的文本内容或空字符串)"
msgstr "Vérifier si un fichier est un texte lisible\n\nArgs:\n    data: Contenu binaire du fichier\n    max_size_mb: Limite maximale de taille de fichier (MB), par défaut 10MB\n\nReturns:\n    (S'il s'agit d'un fichier texte, contenu texte décodé ou chaîne vide)"

msgid "加密文件\n\nArgs:\n    input_path: 输入文件路径\n    output_path: 输出文件路径\n    password: 加密密码\n\nRaises:\n    FileNotFoundError: 输入文件不存在\n    Exception: 加密失败时抛出异常"
msgstr "Chiffrer un fichier\n\nArgs:\n    input_path: Chemin du fichier d'entrée\n    output_path: Chemin du fichier de sortie\n    password: Mot de passe de chiffrement\n\nRaises:\n    FileNotFoundError: Le fichier d'entrée n'existe pas\n    Exception: Levée en cas d'échec du chiffrement"

msgid "解密文件\n\nArgs:\n    input_path: 加密的文件路径\n    output_path: 解密后的文件路径\n    password: 解密密码\n\nRaises:\n    FileNotFoundError: 输入文件不存在\n    Exception: 解密失败时抛出异常"
msgstr "Déchiffrer un fichier\n\nArgs:\n    input_path: Chemin du fichier chiffré\n    output_path: Chemin du fichier déchiffré\n    password: Mot de passe de déchiffrement\n\nRaises:\n    FileNotFoundError: Le fichier d'entrée n'existe pas\n    Exception: Levée en cas d'échec du déchiffrement"

msgid "加密失败: {error}"
msgstr "Échec du chiffrement: {error}"

msgid "解密失败: 密码错误或数据已损坏"
msgstr "Échec du déchiffrement: mot de passe incorrect ou données corrompues"

msgid "解密失败: {error}"
msgstr "Échec du déchiffrement: {error}"

msgid "文件不存在: {path}"
msgstr "Fichier introuvable: {path}"

msgid "文件加密失败: {error}"
msgstr "Échec du chiffrement du fichier: {error}"

msgid "解密失败: 密码错误或文件已损坏"
msgstr "Échec du déchiffrement: mot de passe incorrect ou fichier corrompu"

msgid "文件解密失败: {error}"
msgstr "Échec du déchiffrement du fichier: {error}"
