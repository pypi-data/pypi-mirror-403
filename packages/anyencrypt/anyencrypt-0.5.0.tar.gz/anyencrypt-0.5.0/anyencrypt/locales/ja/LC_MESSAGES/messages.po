msgstr "エラー: 無効なオプションです"

msgid "加密"
msgstr "暗号化"

msgid "解密"
msgstr "復号化"

msgid "请选择处理类型:"
msgstr "処理タイプを選択してください:"

msgid "  [1] 文本"
msgstr "  [1] テキスト"

msgid "  [2] 文件"
msgstr "  [2] ファイル"

msgid "请输入{operation}密码: "
msgstr "{operation}パスワードを入力してください: "

msgid "请输入要加密的文本"
msgstr "暗号化するテキストを入力してください"

msgid "✓ 加密成功!"
msgstr "✓ 暗号化に成功しました!"

msgid "加密结果:"
msgstr "暗号化結果:"

