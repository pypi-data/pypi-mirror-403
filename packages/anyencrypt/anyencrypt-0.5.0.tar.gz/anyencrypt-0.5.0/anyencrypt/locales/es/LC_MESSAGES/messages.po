msgid "AnyEncrypt - 简单易用的加密解密命令行工具\n\n支持文本和文件的加密解密操作。"
msgstr "AnyEncrypt - Herramienta CLI de cifrado/descifrado simple y fácil de usar\n\nAdmite cifrado y descifrado de texto y archivos."

msgid "加密文本或文件\n\n示例：\n\b\n    # 加密文本\n    anyencrypt encrypt -t \"Hello World\"\n\n    # 加密文件\n    anyencrypt encrypt -f input.txt -o encrypted.bin"
msgstr "Cifrar texto o archivo\n\nEjemplos:\n\b\n    # Cifrar texto\n    anyencrypt encrypt -t \"Hello World\"\n\n    # Cifrar archivo\n    anyencrypt encrypt -f input.txt -o encrypted.bin"

msgid "解密文本或文件\n\n示例：\n\b\n    # 解密文本\n    anyencrypt decrypt -t \"gAAAAAB...\"\n\n    # 解密文件\n    anyencrypt decrypt -f encrypted.bin -o decrypted.txt"
msgstr "Descifrar texto o archivo\n\nEjemplos:\n\b\n    # Descifrar texto\n    anyencrypt decrypt -t \"gAAAAAB...\"\n\n    # Descifrar archivo\n    anyencrypt decrypt -f encrypted.bin -o decrypted.txt"

msgid "交互式模式 - 引导用户完成加密/解密操作"
msgstr "Modo interactivo - guía al usuario en el cifrado/descifrado"

msgid "要加密的文本"
msgstr "Texto a cifrar"

msgid "要加密的文件路径"
msgstr "Ruta del archivo a cifrar"

msgid "输出文件路径（文件加密时使用）"
msgstr "Ruta del archivo de salida (se usa al cifrar archivos)"

msgid "加密密码（不建议使用，会在命令历史中留下记录）"
msgstr "Contraseña de cifrado (no recomendado, quedará en el historial)"

msgid "请输入加密密码: "
msgstr "Introduzca la contraseña de cifrado: "

msgid "请再次输入密码: "
msgstr "Vuelva a introducir la contraseña: "

msgid "错误: 两次输入的密码不一致"
msgstr "Error: Las contraseñas no coinciden"

msgid "错误: 密码不能为空"
msgstr "Error: La contraseña no puede estar vacía"

msgid "\n加密结果:"
msgstr "\nResultado del cifrado:"

msgid "✓ 文件加密成功: {output}"
msgstr "✓ Archivo cifrado correctamente: {output}"

msgid "错误: 请指定要加密的文本 (-t) 或文件 (-f)"
msgstr "Error: Especifique texto (-t) o archivo (-f) para cifrar"

msgid "错误: {error}"
msgstr "Error: {error}"

msgid "要解密的文本"
msgstr "Texto a descifrar"

msgid "要解密的文件路径"
msgstr "Ruta del archivo a descifrar"

msgid "输出文件路径（文件解密时使用）"
msgstr "Ruta del archivo de salida (se usa al descifrar archivos)"

msgid "解密密码（不建议使用，会在命令历史中留下记录）"
msgstr "Contraseña de descifrado (no recomendado, quedará en el historial)"

msgid "请输入解密密码: "
msgstr "Introduzca la contraseña de descifrado: "

msgid "\n解密结果:"
msgstr "\nResultado del descifrado:"

msgid "✓ 文件解密成功: {output}"
msgstr "✓ Archivo descifrado correctamente: {output}"

msgid "错误: 请指定要解密的文本 (-t) 或文件 (-f)"
msgstr "Error: Especifique texto (-t) o archivo (-f) para descifrar"

msgid "  AnyEncrypt - 加密解密工具"
msgstr "  AnyEncrypt - Herramienta de cifrado/descifrado"

msgid "请选择操作类型:"
msgstr "Seleccione el tipo de operación:"

msgid "  [1] 加密"
msgstr "  [1] Cifrar"

msgid "  [2] 解密"
msgstr "  [2] Descifrar"

msgid "请输入选项"
msgstr "Introduzca la opción"

msgid "错误: 无效的选项"
msgstr "Error: Opción no válida"

msgid "加密"
msgstr "Cifrar"

msgid "解密"
msgstr "Descifrar"

msgid "请选择处理类型:"
msgstr "Seleccione el tipo de procesamiento:"

msgid "  [1] 文本"
msgstr "  [1] Texto"

msgid "  [2] 文件"
msgstr "  [2] Archivo"

msgid "请输入{operation}密码: "
msgstr "Introduzca la contraseña de {operation}: "

msgid "请输入要加密的文本"
msgstr "Introduzca el texto a cifrar"

msgid "✓ 加密成功!"
msgstr "✓ ¡Cifrado exitoso!"

msgid "加密结果:"
msgstr "Resultado del cifrado:"

msgid "请输入要解密的密文"
msgstr "Introduzca el texto cifrado a descifrar"

msgid "✓ 解密成功!"
msgstr "✓ ¡Descifrado exitoso!"

msgid "解密结果:"
msgstr "Resultado del descifrado:"

msgid "请输入要加密的文件路径"
msgstr "Introduzca la ruta del archivo a cifrar"

msgid "错误: 文件不存在: {path}"
msgstr "Error: Archivo no encontrado: {path}"

msgid "请输入输出文件路径"
msgstr "Introduzca la ruta del archivo de salida"

msgid "✓ 文件加密成功: {path}"
msgstr "✓ Archivo cifrado correctamente: {path}"

msgid "请输入要解密的文件路径"
msgstr "Introduzca la ruta del archivo a descifrar"

msgid "✓ 文件解密成功: {path}"
msgstr "✓ Archivo descifrado correctamente: {path}"

msgid "加密解密核心功能模块\n使用 Fernet (对称加密) 进行加密和解密操作"
msgstr "Módulo central de cifrado/descifrado\nUsa Fernet (cifrado simétrico) para cifrar y descifrar"

msgid "从密码派生出32字节的密钥\n使用 SHA-256 哈希算法"
msgstr "Derivar una clave de 32 bytes a partir de la contraseña\nUsa el algoritmo hash SHA-256"

msgid "内部辅助函数"
msgstr "Función auxiliar interna"

msgid "加密文本\n\nArgs:\n    text: 要加密的文本\n    password: 加密密码\n\nReturns:\n    加密后的文本（Base64编码）\n\nRaises:\n    Exception: 加密失败时抛出异常"
msgstr "Cifrar texto\n\nArgs:\n    text: Texto a cifrar\n    password: Contraseña de cifrado\n\nReturns:\n    Texto cifrado (codificado en Base64)\n\nRaises:\n    Exception: Se lanza cuando falla el cifrado"

msgid "解密文本\n\nArgs:\n    encrypted_text: 加密的文本\n    password: 解密密码\n\nReturns:\n    解密后的原文本\n\nRaises:\n    Exception: 解密失败时抛出异常（可能是密码错误或数据损坏）"
msgstr "Descifrar texto\n\nArgs:\n    encrypted_text: Texto cifrado\n    password: Contraseña de descifrado\n\nReturns:\n    Texto original descifrado\n\nRaises:\n    Exception: Se lanza cuando falla el descifrado (posible contraseña incorrecta o datos dañados)"

msgid "判断文件是否为可读文本文件\n\nArgs:\n    data: 文件的二进制内容\n    max_size_mb: 最大文件大小限制(MB),默认10MB\n\nReturns:\n    (是否为文本文件, 解码后的文本内容或空字符串)"
msgstr "Comprobar si un archivo es texto legible\n\nArgs:\n    data: Contenido binario del archivo\n    max_size_mb: Límite máximo de tamaño de archivo (MB), por defecto 10MB\n\nReturns:\n    (Si es un archivo de texto, contenido de texto decodificado o cadena vacía)"

msgid "加密文件\n\nArgs:\n    input_path: 输入文件路径\n    output_path: 输出文件路径\n    password: 加密密码\n\nRaises:\n    FileNotFoundError: 输入文件不存在\n    Exception: 加密失败时抛出异常"
msgstr "Cifrar archivo\n\nArgs:\n    input_path: Ruta del archivo de entrada\n    output_path: Ruta del archivo de salida\n    password: Contraseña de cifrado\n\nRaises:\n    FileNotFoundError: El archivo de entrada no existe\n    Exception: Se lanza cuando falla el cifrado"

msgid "解密文件\n\nArgs:\n    input_path: 加密的文件路径\n    output_path: 解密后的文件路径\n    password: 解密密码\n\nRaises:\n    FileNotFoundError: 输入文件不存在\n    Exception: 解密失败时抛出异常"
msgstr "Descifrar archivo\n\nArgs:\n    input_path: Ruta del archivo cifrado\n    output_path: Ruta del archivo descifrado\n    password: Contraseña de descifrado\n\nRaises:\n    FileNotFoundError: El archivo de entrada no existe\n    Exception: Se lanza cuando falla el descifrado"

msgid "加密失败: {error}"
msgstr "Fallo al cifrar: {error}"

msgid "解密失败: 密码错误或数据已损坏"
msgstr "Fallo al descifrar: contraseña incorrecta o datos dañados"

msgid "解密失败: {error}"
msgstr "Fallo al descifrar: {error}"

msgid "文件不存在: {path}"
msgstr "Archivo no encontrado: {path}"

msgid "文件加密失败: {error}"
msgstr "Fallo al cifrar el archivo: {error}"

msgid "解密失败: 密码错误或文件已损坏"
msgstr "Fallo al descifrar: contraseña incorrecta o archivo dañado"

msgid "文件解密失败: {error}"
msgstr "Fallo al descifrar el archivo: {error}"
