msgid "AnyEncrypt - 简单易用的加密解密命令行工具\n\n支持文本和文件的加密解密操作。"
msgstr "AnyEncrypt - Simple and easy-to-use encryption/decryption CLI tool\n\nSupports text and file encryption/decryption."

msgid "加密文本或文件\n\n示例：\n\b\n    # 加密文本\n    anyencrypt encrypt -t \"Hello World\"\n\n    # 加密文件\n    anyencrypt encrypt -f input.txt -o encrypted.bin"
msgstr "Encrypt text or file\n\nExamples:\n\b\n    # Encrypt text\n    anyencrypt encrypt -t \"Hello World\"\n\n    # Encrypt file\n    anyencrypt encrypt -f input.txt -o encrypted.bin"

msgid "解密文本或文件\n\n示例：\n\b\n    # 解密文本\n    anyencrypt decrypt -t \"gAAAAAB...\"\n\n    # 解密文件\n    anyencrypt decrypt -f encrypted.bin -o decrypted.txt"
msgstr "Decrypt text or file\n\nExamples:\n\b\n    # Decrypt text\n    anyencrypt decrypt -t \"gAAAAAB...\"\n\n    # Decrypt file\n    anyencrypt decrypt -f encrypted.bin -o decrypted.txt"

msgid "交互式模式 - 引导用户完成加密/解密操作"
msgstr "Interactive mode - guides you through encryption/decryption"

msgid "要加密的文本"
msgstr "Text to encrypt"

msgid "要加密的文件路径"
msgstr "File path to encrypt"

msgid "输出文件路径（文件加密时使用）"
msgstr "Output file path (used for file encryption)"

msgid "加密密码（不建议使用，会在命令历史中留下记录）"
msgstr "Encryption password (not recommended, will be saved in shell history)"

msgid "请输入加密密码: "
msgstr "Enter encryption password: "

msgid "请再次输入密码: "
msgstr "Please enter password again: "

msgid "错误: 两次输入的密码不一致"
msgstr "Error: Passwords do not match"

msgid "错误: 密码不能为空"
msgstr "Error: Password cannot be empty"

msgid "\n加密结果:"
msgstr "\nEncrypted result:"

msgid "✓ 文件加密成功: {output}"
msgstr "✓ File encrypted successfully: {output}"

msgid "错误: 请指定要加密的文本 (-t) 或文件 (-f)"
msgstr "Error: Please specify text (-t) or file (-f) to encrypt"

msgid "错误: {error}"
msgstr "Error: {error}"

msgid "要解密的文本"
msgstr "Text to decrypt"

msgid "要解密的文件路径"
msgstr "File path to decrypt"

msgid "输出文件路径（文件解密时使用）"
msgstr "Output file path (used for file decryption)"

msgid "解密密码（不建议使用，会在命令历史中留下记录）"
msgstr "Decryption password (not recommended, will be saved in shell history)"

msgid "请输入解密密码: "
msgstr "Enter decryption password: "

msgid "\n解密结果:"
msgstr "\nDecrypted result:"

msgid "✓ 文件解密成功: {output}"
msgstr "✓ File decrypted successfully: {output}"

msgid "错误: 请指定要解密的文本 (-t) 或文件 (-f)"
msgstr "Error: Please specify text (-t) or file (-f) to decrypt"

msgid "  AnyEncrypt - 加密解密工具"
msgstr "  AnyEncrypt - Encryption/Decryption Tool"

msgid "请选择操作类型:"
msgstr "Please select operation type:"

msgid "  [1] 加密"
msgstr "  [1] Encrypt"

msgid "  [2] 解密"
msgstr "  [2] Decrypt"

msgid "请输入选项"
msgstr "Enter option"

msgid "错误: 无效的选项"
msgstr "Error: Invalid option"

msgid "加密"
msgstr "Encrypt"

msgid "解密"
msgstr "Decrypt"

msgid "请选择处理类型:"
msgstr "Please select processing type:"

msgid "  [1] 文本"
msgstr "  [1] Text"

msgid "  [2] 文件"
msgstr "  [2] File"

msgid "请输入{operation}密码: "
msgstr "Enter {operation} password: "

msgid "请输入要加密的文本"
msgstr "Enter text to encrypt"

msgid "✓ 加密成功!"
msgstr "✓ Encryption successful!"

msgid "加密结果:"
msgstr "Encrypted result:"

msgid "请输入要解密的密文"
msgstr "Enter ciphertext to decrypt"

msgid "✓ 解密成功!"
msgstr "✓ Decryption successful!"

msgid "解密结果:"
msgstr "Decrypted result:"

msgid "请输入要加密的文件路径"
msgstr "Enter file path to encrypt"

msgid "错误: 文件不存在: {path}"
msgstr "Error: File not found: {path}"

msgid "请输入输出文件路径"
msgstr "Enter output file path"

msgid "✓ 文件加密成功: {path}"
msgstr "✓ File encrypted successfully: {path}"

msgid "请输入要解密的文件路径"
msgstr "Enter file path to decrypt"

msgid "✓ 文件解密成功: {path}"
msgstr "✓ File decrypted successfully: {path}"

msgid "加密解密核心功能模块\n使用 Fernet (对称加密) 进行加密和解密操作"
msgstr "Core encryption/decryption module\nUses Fernet (symmetric encryption) for encrypt/decrypt operations"

msgid "从密码派生出32字节的密钥\n使用 SHA-256 哈希算法"
msgstr "Derive a 32-byte key from password\nUses SHA-256 hash algorithm"

msgid "内部辅助函数"
msgstr "Internal helper function"

msgid "加密文本\n\nArgs:\n    text: 要加密的文本\n    password: 加密密码\n\nReturns:\n    加密后的文本（Base64编码）\n\nRaises:\n    Exception: 加密失败时抛出异常"
msgstr "Encrypt text\n\nArgs:\n    text: Text to encrypt\n    password: Encryption password\n\nReturns:\n    Encrypted text (Base64 encoded)\n\nRaises:\n    Exception: Raised when encryption fails"

msgid "解密文本\n\nArgs:\n    encrypted_text: 加密的文本\n    password: 解密密码\n\nReturns:\n    解密后的原文本\n\nRaises:\n    Exception: 解密失败时抛出异常（可能是密码错误或数据损坏）"
msgstr "Decrypt text\n\nArgs:\n    encrypted_text: Encrypted text\n    password: Decryption password\n\nReturns:\n    Decrypted plaintext\n\nRaises:\n    Exception: Raised when decryption fails (possible wrong password or corrupted data)"

msgid "判断文件是否为可读文本文件\n\nArgs:\n    data: 文件的二进制内容\n    max_size_mb: 最大文件大小限制(MB),默认10MB\n\nReturns:\n    (是否为文本文件, 解码后的文本内容或空字符串)"
msgstr "Check whether a file is a readable text file\n\nArgs:\n    data: Binary content of the file\n    max_size_mb: Maximum file size limit (MB), default 10MB\n\nReturns:\n    (Whether it is a text file, decoded text content or empty string)"

msgid "加密文件\n\nArgs:\n    input_path: 输入文件路径\n    output_path: 输出文件路径\n    password: 加密密码\n\nRaises:\n    FileNotFoundError: 输入文件不存在\n    Exception: 加密失败时抛出异常"
msgstr "Encrypt file\n\nArgs:\n    input_path: Input file path\n    output_path: Output file path\n    password: Encryption password\n\nRaises:\n    FileNotFoundError: Input file does not exist\n    Exception: Raised when encryption fails"

msgid "解密文件\n\nArgs:\n    input_path: 加密的文件路径\n    output_path: 解密后的文件路径\n    password: 解密密码\n\nRaises:\n    FileNotFoundError: 输入文件不存在\n    Exception: 解密失败时抛出异常"
msgstr "Decrypt file\n\nArgs:\n    input_path: Encrypted file path\n    output_path: Decrypted file path\n    password: Decryption password\n\nRaises:\n    FileNotFoundError: Input file does not exist\n    Exception: Raised when decryption fails"

msgid "加密失败: {error}"
msgstr "Encryption failed: {error}"

msgid "解密失败: 密码错误或数据已损坏"
msgstr "Decryption failed: wrong password or corrupted data"

msgid "解密失败: {error}"
msgstr "Decryption failed: {error}"

msgid "文件不存在: {path}"
msgstr "File not found: {path}"

msgid "文件加密失败: {error}"
msgstr "File encryption failed: {error}"

msgid "解密失败: 密码错误或文件已损坏"
msgstr "Decryption failed: wrong password or corrupted file"

msgid "文件解密失败: {error}"
msgstr "File decryption failed: {error}"
