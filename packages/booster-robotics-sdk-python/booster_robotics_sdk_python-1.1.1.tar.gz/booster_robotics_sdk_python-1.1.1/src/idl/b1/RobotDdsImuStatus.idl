module booster_interface {
    module msg {
        struct RobotDdsImuStatus {
            string name;            // IMU 名称，注意，不是型号名
            int32 index;            // IMU 索引，可用于排序
            boolean is_connected;   // 连接是否正常，即通信是否正常
            int32 status_code;      // 预留字段，用于获取更详细的 IMU 状态
        };
    };
};

