module booster_interface {
    module msg {
        struct RobotDdsBatteryStatus {
            string name;        // 名称，注意，不是型号名
            float temperature;  // 温度
            float soc;          // 电量，百分比
            float voltage;      // 电压
            int32 status_code;  // 预留字段，用于获取更详细的电池状态
            int32 status_level; // 当前状态级别，分为 LEVEL_NORMAL(0)-电量正常, LEVEL_WARNING(1)-低电量, LEVEL_FATAL(2)-电量过低，有关机风险
        };
    };
};

