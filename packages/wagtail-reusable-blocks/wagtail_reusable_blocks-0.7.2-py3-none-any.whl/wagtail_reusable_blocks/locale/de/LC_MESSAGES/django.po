#: models/reusable_block.py:301
#, python-format
msgid ""
"Circular reference detected: Block '%(name)s' (id=%(id)s) references itself "
"in the dependency chain."
msgstr ""
"Zirkuläre Referenz erkannt: Block '%(name)s' (id=%(id)s) verweist auf sich "
"selbst in der Abhängigkeitskette."

#: models/reusable_block.py:321
#, python-format
msgid ""
"Circular reference detected: Block '%(name)s' references block "
"'%(ref_name)s' which creates a cycle. %(error)s"
msgstr ""
"Zirkuläre Referenz erkannt: Block '%(name)s' verweist auf Block "
"'%(ref_name)s', was einen Zyklus erzeugt. %(error)s"

