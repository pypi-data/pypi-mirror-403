[workspace]
channels = [
    "conda-forge",
]
description = "A basic image with jupyterlab and server-documents"
name = "jupyter-pixi"
platforms = [
%{ if cpu_architecture == "arm64" ~}
    "linux-aarch64",
%{ else ~}
    "linux-64",
%{ endif ~}
]
version = "0.3.0"

[tasks]

[dependencies]
python = ">=3.12"

[pypi-dependencies]
jupyterlab = ">=4.4.2,<5"
jupyter-server-documents = ">=0.1.0a0"
