# AWS Configuration
# Copy this file to .env and fill in your values

AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_REGION=us-east-1

# Optional: Customize project name (default: omniparser)
# PROJECT_NAME=omniparser

# Optional: EC2 instance settings
# AWS_EC2_INSTANCE_TYPE=g6.xlarge  # L4 24GB, or g4dn.xlarge for T4 16GB
# AWS_EC2_AMI=ami-04631c7d8811d9bae  # AWS DLAMI Base Ubuntu 22.04

# Optional: Auto-shutdown after inactivity (default: 60 minutes)
# INACTIVITY_TIMEOUT_MINUTES=60
