{
  "name": "signalpilot-ai",
  "version": "0.12.0",
  "packageManager": "yarn@4.1.0",
  "description": "SignalPilot Agent - Your Jupyter Notebook Assistant",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://signalpilot.ai/",
  "license": "BSD-3-Clause",
  "author": {
    "name": "Daniel Schaffield",
    "email": "danielschaffield@gmail.com"
  },
  "files": [
    "lib/**/*.{d.ts,tsx,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf,md}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf,md}",
    "src/**/*.{ts,tsx,md}",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "scripts": {
    "build": "jlpm prebuild && jlpm build:lib && jlpm build:labextension:dev",
    "build:no_tracking": "cross-env DISABLE_TRACKING=true jlpm prebuild && cross-env DISABLE_TRACKING=true jlpm build:lib && cross-env DISABLE_TRACKING=true jlpm build:labextension:dev",
    "build:prod": "jlpm prebuild && jlpm clean && jlpm build:lib:prod && jlpm build:labextension",
    "build:labextension": "jlpm prebuild && jupyter labextension build .",
    "build:labextension:dev": "jlpm prebuild && jupyter labextension build --development True .",
    "build:lib": "jlpm prebuild && tsc --sourceMap",
    "build:lib:prod": "jlpm prebuild && tsc && jlpm build:remove-console",
    "build:remove-console": "cross-env NODE_ENV=production babel lib --out-dir lib --extensions .js",
    "prebuild": "mkdir -p lib/Config/prompts && cp src/Config/prompts/*.md lib/Config/prompts && node scripts/generate-tracking-config.js",
    "clean": "jlpm clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:lintcache": "rimraf .eslintcache .stylelintcache",
    "clean:labextension": "rimraf signalpilot_ai/labextension signalpilot_ai/_version.py",
    "clean:all": "jlpm clean:lib && jlpm clean:labextension && jlpm clean:lintcache",
    "eslint": "jlpm eslint:check --fix",
    "eslint:check": "eslint . --cache --ext .ts,.tsx",
    "install:extension": "jlpm build",
    "lint": "jlpm prettier && jlpm eslint",
    "lint:check": "jlpm prettier:check && jlpm eslint:check",
    "prettier": "jlpm prettier:base --write --list-different",
    "prettier:base": "prettier \"**/*{.ts,.tsx,.js,.jsx}\"",
    "prettier:check": "jlpm prettier:base --check",
    "stylelint": "jlpm stylelint:check --fix",
    "stylelint:check": "stylelint --cache \"style/**/*.css\"",
    "test": "jest --coverage",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w --sourceMap",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@electric-sql/pglite": "^0.3.6",
    "@google/generative-ai": "^0.24.0",
    "@jupyterlab/application": "^4.0.0",
    "@jupyterlab/docmanager": "^4.4.5",
    "@jupyterlab/extensionmanager": "^4.4.4",
    "@jupyterlab/notebook-extension": "^4.4.0",
    "@jupyterlab/settingregistry": "^4.0.0",
    "@modelcontextprotocol/sdk": "^1.9.0",
    "@redux-devtools/extension": "^3.3.0",
    "@types/hogan.js": "^3.0.5",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "best-effort-json-parser": "^1.1.3",
    "bluebird": "^3.7.2",
    "bootstrap": "^5.3.7",
    "chalk": "^4.1.2",
    "codemirror": "^6.0.2",
    "codemirror-merge-alpinex": "^6.10.5",
    "cross-env": "^10.0.0",
    "diff": "^8.0.1",
    "diff2html": "^3.4.51",
    "dompurify": "^3.2.5",
    "driver.js": "^1.3.6",
    "fuse.js": "^7.1.0",
    "highlight.js": "^11.11.1",
    "jwt-decode": "^4.0.0",
    "marked": "^15.0.11",
    "partial-json": "^0.1.7",
    "posthog-js": "^1.268.5",
    "react-bootstrap": "^2.10.10",
    "readline": "^1.3.0",
    "stripe": "^18.5.0",
    "textarea-caret": "^3.1.0",
    "uuid": "^11.1.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.0.0",
    "@jupyterlab/testutils": "^4.0.0",
    "@types/bluebird": "^3.5.42",
    "@types/jest": "^29.2.0",
    "@types/json-schema": "^7.0.11",
    "@types/react": "^18.0.26",
    "@types/react-addons-linked-state-mixin": "^0.14.22",
    "@types/textarea-caret": "^3.0.4",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "baseline-browser-mapping": "^2.9.15",
    "css-loader": "^6.7.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.2.0",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.0.0",
    "raw-loader": "^4.0.2",
    "rimraf": "^5.0.1",
    "source-map-loader": "^1.0.2",
    "style-loader": "^3.3.1",
    "stylelint": "^15.10.1",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-csstree-validator": "^3.0.0",
    "stylelint-prettier": "^4.0.0",
    "typescript": "~5.0.2",
    "yjs": "^13.5.0"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "signalpilot_ai/labextension",
    "schemaDir": "schema",
    "webpackConfig": "./webpack.config.js",
    "_build": {
      "load": "static/remoteEntry.21a06b464d99b3999f4d.js",
      "extension": "./extension",
      "style": "./style"
    }
  },
  "eslintIgnore": [
    "node_modules",
    "dist",
    "coverage",
    "**/*.d.ts",
    "tests",
    "**/__tests__",
    "ui-tests"
  ],
  "eslintConfig": {
    "extends": [
      "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {}
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid",
    "endOfLine": "auto",
    "overrides": [
      {
        "files": "package.json",
        "options": {
          "tabWidth": 4
        }
      }
    ]
  },
  "stylelint": {
    "extends": [
      "stylelint-config-recommended",
      "stylelint-config-standard",
      "stylelint-prettier/recommended"
    ],
    "plugins": [
      "stylelint-csstree-validator"
    ],
    "rules": {
      "csstree/validator": true,
      "property-no-vendor-prefix": null,
      "selector-class-pattern": "^([a-z][A-z\\d]*)(-[A-z\\d]+)*$",
      "selector-no-vendor-prefix": null,
      "value-no-vendor-prefix": null
    }
  }
}
