# Copy to .env and add your Anthropic API key
ANTHROPIC_API_KEY=your_key_here
# Optional: override the default model
ANTHROPIC_MODEL=claude-haiku-4-5

# Database URLs for schema search
# Format: DATABASE_<NAME>_URL where <NAME> is the database name (uppercased, hyphens to underscores)
# Example: database_names=["my-db"] looks for DATABASE_MY_DB_URL
DATABASE_PRODUCTION_URL=postgresql://user:pass@localhost:5432/mydb
DATABASE_ANALYTICS_URL=postgresql://user:pass@localhost:5432/analytics
# DATABASE_SNOWFLAKE_URL=snowflake://user:pass@account/database
