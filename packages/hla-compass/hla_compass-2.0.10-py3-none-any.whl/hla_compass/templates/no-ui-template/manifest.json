{
  "schemaVersion": "1.0",
  "name": "module-name",
  "version": "1.0.0",
  "type": "no-ui",
  "computeType": "fargate",
  "description": "Backend-only module for data processing",
  "execution": {
    "entrypoint": "backend.main:NoUIModule",
    "supports": [
      "interactive",
      "workflow"
    ],
    "defaultMode": "interactive"
  },
  "author": {
    "name": "Developer Name",
    "email": "developer@example.com",
    "organization": "Organization"
  },
  "inputs": {
    "type": "object",
    "title": "Module Inputs",
    "required": [
      "data_source"
    ],
    "properties": {
      "data_source": {
        "type": "array",
        "title": "Data source",
        "description": "Inline data set. Update manifest if using files or storage URIs"
      },
      "options": {
        "type": "object",
        "title": "Processing options",
        "properties": {
          "batch_size": {
            "type": "integer",
            "description": "Batch size for processing",
            "default": 100
          },
          "save_results": {
            "type": "boolean",
            "description": "Save results to storage",
            "default": false
          }
        }
      }
    }
  },
  "outputs": {
    "results": {
      "type": "object",
      "description": "Processing results with metadata"
    }
  },
  "resources": {
    "memory": 512,
    "timeout": 180,
    "environment": {
      "LOG_LEVEL": "INFO"
    }
  },
  "metadata": {
    "exposedParameters": [],
    "auditLog": true
  },
  "permissions": {
    "database": [
      "read"
    ],
    "storage": true,
    "network": []
  },
  "pricing": {
    "tiers": [
      {
        "tier": "developer",
        "model": "usage-based",
        "amountAct": 0.25
      }
    ]
  }
}
