{
  "schemaVersion": "1.0",
  "name": "module-name",
  "version": "1.0.0",
  "type": "with-ui",
  "computeType": "fargate",
  "description": "Module with user interface",
  "execution": {
    "entrypoint": "backend.main:UIModule",
    "supports": [
      "interactive"
    ],
    "defaultMode": "interactive"
  },
  "author": {
    "name": "Developer Name",
    "email": "developer@example.com",
    "organization": "Organization"
  },
  "ui": {
    "framework": "react",
    "buildCommand": "npm run build",
    "distPath": "frontend/dist"
  },
  "inputs": {
    "type": "object",
    "title": "Module Inputs",
    "required": [
      "param1"
    ],
    "properties": {
      "param1": {
        "type": "string",
        "title": "Primary parameter",
        "description": "User provided value exposed via the UI"
      },
      "param2": {
        "type": "string",
        "title": "Optional parameter",
        "description": "Defaults can be customized",
        "default": "default_value"
      }
    }
  },
  "outputs": {
    "results": {
      "type": "array",
      "description": "Processed results for UI display"
    },
    "summary": {
      "type": "object",
      "description": "Summary statistics"
    }
  },
  "resources": {
    "memory": 512,
    "timeout": 180,
    "environment": {
      "LOG_LEVEL": "INFO"
    }
  },
  "metadata": {
    "exposedParameters": [],
    "auditLog": true
  },
  "permissions": {
    "database": [
      "read"
    ],
    "storage": true,
    "network": []
  },
  "pricing": {
    "tiers": [
      {
        "tier": "developer",
        "model": "usage-based",
        "amountAct": 0.5
      }
    ]
  }
}
