# GRLC metadata
#+ summary: Subgraph matching for the 'topology' use case
#+ method: GET
#+ tags:
#+   - PyGraft-gen

PREFIX dc: <http://purl.org/dc/elements/1.1/>
PREFIX mod: <https://w3id.org/mod#>
PREFIX mykg-object: <http://example.org/pygraft-gen/object/>
PREFIX noria: <https://w3id.org/noria/ontology/>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>


SELECT ?res1 ?res2 ?res3 ?res4 ?nl_r1_r2 ?nl_r2_r3 ?nl_r3_r4 ?nl_r1_r4 ?nl_r2_r4 ?nl_r1_r3
WHERE {
    ?nl_r1_r2 a noria:NetworkLink ;
        noria:networkLinkTerminationResource ?res1 ;
        noria:networkLinkTerminationResource ?res2 .

    FILTER (?res1 != ?res2)

    ?nl_r2_r3 a noria:NetworkLink ;
        noria:networkLinkTerminationResource ?res2 ;
        noria:networkLinkTerminationResource ?res3 ;
    .

    FILTER (?res2 != ?res3)

    ?nl_r3_r4 a noria:NetworkLink ;
        noria:networkLinkTerminationResource ?res3 ;
        noria:networkLinkTerminationResource ?res4 ;
    .

    FILTER (?res3 != ?res4)

    ?nl_r1_r4 a noria:NetworkLink ;
        noria:networkLinkTerminationResource ?res1 ;
        noria:networkLinkTerminationResource ?res4 ;
    .

    FILTER (?res1 != ?res4)

    ?nl_r2_r4 a noria:NetworkLink ;
        noria:networkLinkTerminationResource ?res2 ;
        noria:networkLinkTerminationResource ?res4 ;
    .

    FILTER (?res2 != ?res4)

    ?nl_r1_r3 a noria:NetworkLink ;
        noria:networkLinkTerminationResource ?res1 ;
        noria:networkLinkTerminationResource ?res3 ;
    .

    FILTER (?res1 != ?res3)

}
# Optional : LIMIT 1M to avoid overlong requests
LIMIT 10000000