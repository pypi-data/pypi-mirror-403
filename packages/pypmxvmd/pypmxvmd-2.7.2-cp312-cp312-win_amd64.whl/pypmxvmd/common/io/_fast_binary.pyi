from __future__ import annotations

from typing import Tuple


class FastBinaryReader:
    def __init__(self, data: bytes) -> None: ...
    def get_position(self) -> int: ...
    def set_position(self, pos: int) -> None: ...
    def get_remaining(self) -> int: ...
    def skip(self, count: int) -> None: ...
    def read_byte(self) -> int: ...
    def read_sbyte(self) -> int: ...
    def read_ushort(self) -> int: ...
    def read_short(self) -> int: ...
    def read_uint(self) -> int: ...
    def read_int(self) -> int: ...
    def read_float(self) -> float: ...
    def read_float3(self) -> Tuple[float, float, float]: ...
    def read_float4(self) -> Tuple[float, float, float, float]: ...
    def read_bytes(self, count: int) -> bytes: ...
    def read_string_fixed(self, length: int, encoding: str = "shift_jis") -> str: ...
    def read_string_variable(self, encoding: str = "utf-16le") -> str: ...
    def read_index(self, size: int, signed: bool = True) -> int: ...


def quaternion_to_euler_fast(qx: float, qy: float, qz: float, qw: float) -> Tuple[float, float, float]: ...
def euler_to_quaternion_fast(
    roll_deg: float,
    pitch_deg: float,
    yaw_deg: float,
) -> Tuple[float, float, float, float]: ...
