"""{{ project_name }} 测试程序入口"""
from mc import main
from mc.types import char_ptr, Ptr
from gtest import RUN_ALL_TESTS, InitGoogleTest

{% if threads > 0 -%}
@main(threads={{ threads }}, works={{ works }})
{% else -%}
@main(threads=2, works=2)
{% endif -%}
def main_(argc: int, argv: Ptr[char_ptr]) -> int:
    InitGoogleTest(argc, argv)
    return RUN_ALL_TESTS()
