[project]
name = "{{ project_name }}"
version = "{{ version }}"

[dependencies]
# mcl_runtime 运行时库（必需）
# 使用版本范围规则确保使用最新的兼容版本
mcl_runtime = ">=0.1.0"

# 内置类型定义（来自 mclang/_packages/）
mc = "builtin"
gtest = "builtin"

[[targets]]
name = "{{ project_name }}"
type = "executable"

[[targets]]
name = "{{ project_name }}_test"
type = "test"
