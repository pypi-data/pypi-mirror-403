# mclang 工具链清单
# 自动生成，请勿手动修改

[toolchain]
version = "{{ toolchain_version }}"
host = "{{ host_platform }}"
{% if local %}
local = true
{% endif %}

[compiler]
type = "{{ compiler_type }}"
version = "{{ compiler_version }}"
{% if compiler_path %}
path = "{{ compiler_path }}"
{% endif %}

# 工具链支持的目标平台列表
# 每个平台需要对应的 mcl_runtime 包（包含 boost + gtest）
[targets]
{% for target in supported_targets -%}
{{ target }} = true
{% endfor %}
