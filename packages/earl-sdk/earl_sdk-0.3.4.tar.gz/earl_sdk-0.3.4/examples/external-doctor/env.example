# LLM Provider: "gemini" (default) or "openai"
LLM_PROVIDER=gemini

# Gemini Configuration (default provider)
GEMINI_API_KEY=your-gemini-api-key-here
GEMINI_MODEL=gemini-2.0-flash-exp

# OpenAI Configuration (if LLM_PROVIDER=openai)
OPENAI_API_KEY=sk-your-api-key-here
OPENAI_MODEL=gpt-4o-mini

# Server Configuration
DOCTOR_PORT=9000

# API Key for authentication (clients must send X-API-Key header)
# Default: "earl-test-doctor-key" - for SDK integration tests
# Set to empty to disable authentication
EXTERNAL_DOCTOR_API_KEY=earl-test-doctor-key
