{
  "bundle": {
    "name": "example-pipeline",
    "description": "Example ETL pipeline",
    "owner": "data-team"
  },
  "workflows": [
    {
      "name": "example-workflow",
      "description": "Example workflow with PySpark task",
      "schedule_interval": "",
      "start_date": null,
      "end_date": null,
      "catchup": false,
      "max_active_runs": 1,
      "max_active_tasks": 16,
      "tags": ["example"],
      "default_view": "graph",
      "is_paused_upon_creation": true,
      "dagrun_timeout": 7200,
      "notifications": [],
      "tasks": [
        {
          "id": "example_task",
          "task_display_name": "Example Task",
          "job_type": "pyspark",
          "path": "/path/to/your/script.py",
          "image_id": 1,
          "namespace": "fdp-workspace",
          "compute": "your-compute-cluster",
          "trigger_rule": "all_success",
          "execution_timeout": 3600,
          "priority_weight": 1,
          "depends_on_past": false,
          "retries": 3,
          "retry_delay": 300,
          "arguments": [],
          "position_x": 100,
          "position_y": 100,
          "config": {
            "driver": {
              "cores": 1,
              "memory": "1g"
            },
            "executor": {
              "cores": 1,
              "memory": "1g",
              "instances": 1
            },
            "spark_conf": {}
          },
          "libraries": {
            "Pypi": "",
            "Wheel": []
          },
          "volumes": null,
          "init_scripts": null,
          "env": {},
          "restart_policy": "OnFailure",
          "depends_on": [],
          "notifications": []
        }
      ]
    }
  ],
  "environments": {
    "dev": {
      "overrides": {
        "workflows.*.schedule_interval": null,
        "workflows.*.tasks.*.namespace": "fdp-workspace"
      }
    },
    "stg": {
      "overrides": {
        "workflows.*.schedule_interval": "0 4 * * *"
      }
    },
    "prod": {
      "overrides": {
        "workflows.*.schedule_interval": "0 2 * * *"
      }
    }
  }
}
