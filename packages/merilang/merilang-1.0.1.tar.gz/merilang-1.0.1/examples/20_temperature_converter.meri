// Temperature Converter

kaam celsius_to_fahrenheit(c) {
    wapas (c * 9.0 / 5.0) + 32.0
}

kaam fahrenheit_to_celsius(f) {
    wapas (f - 32.0) * 5.0 / 9.0
}

likho("Temperature Conversions:")
likho("Celsius | Fahrenheit")
likho("--------|------------")

bar_bar c in disha(0, 101, 10) {
    maan f = celsius_to_fahrenheit(dashamlav(c))
    likho(shabd(c) + "°C    | " + shabd(f) + "°F")
}
