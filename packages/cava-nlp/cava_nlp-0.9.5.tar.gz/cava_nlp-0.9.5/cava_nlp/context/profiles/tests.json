{
  "context_rules": [
    {
      "literal": "pending",
      "pattern": [
            {
                "LOWER": {"IN": ["pending", "awaiting", "await"] }
            }
        ],
      "category": "UNCONFIRMED",
      "direction": "BACKWARD"
    },
    {
      "literal": "testing for",
      "pattern": [
            {
                "LOWER": {"IN": ["sent", "workup", "test", "testing"] }
            },
            {
                "LOWER": {"IN": ["for", "to"]},
                "OP": "?"
            }
        ],
      "category": "UNCONFIRMED",
      "direction": "FORWARD"
    },
    {
      "literal": "not done",
      "pattern": [
            {
                "LOWER": {"IN": ["not"] }
            },
            {
                "LOWER": {"IN": ["done", "completed", "performed", "sent", "ordered", "complete", "tested"]}
            }
        ],
      "category": "UNCONFIRMED",
      "direction": "FORWARD"
    },
    {
      "literal": "positive",
      "category": "POSITIVE",
      "pattern": [
            {
                "LOWER": {
                  "IN": [
                    "positive", "detected", "elevated", "high", "confirmed", "present", "+", "+ve", "mutant", "mutated"
                    ] 
                  }
            }
        ],
      "direction": "BACKWARD"
    },
    {
      "literal": "positive",
      "category": "POSITIVE",
      "pattern": [
            {
                "LOWER": {"IN": ["positive", "detected", "elevated", "high", "confirmed", "present"] }
            },
            {
                "LOWER": {"IN": ["for", "to"]}
            }

        ],
      "direction": "FORWARD"
    },
    {
      "literal": "negative",
      "category": "NEGATED_EXISTENCE",
      "pattern": [
              {
                  "LOWER": {"IN": ["negative", "undetectable", "normal", "-ve"] }
              }
  
          ],
      "direction": "BACKWARD"
    },
    {
      "literal": "negative",
      "category": "NEGATED_EXISTENCE",
      "pattern": [
              {
                  "LOWER": {"IN": ["negative", "undetectable", "-ve", "neg"] }
              },
              {
                  "LOWER": {"IN": ["for", "to"]}
              }
  
          ],
      "direction": "BIDIRECTIONAL"
    },
    {
      "literal": "negative",
      "category": "NEGATED_EXISTENCE",
      "pattern": [
              {
                  "TEXT": "-"
              }
  
          ],
      "direction": "BIDIRECTIONAL"
    },
    {
      "literal": "negative",
      "category": "NEGATED_EXISTENCE",
      "pattern": [
              {
                  "LOWER": {"IN": ["no", "undetectable"] }
              }
  
          ],
      "direction": "FORWARD"
    },
    {
      "category": "NEGATED_EXISTENCE",
      "literal": "unless",
      "pattern": [
        {
          "LOWER": {
            "in": [
              "unless",
              "except",
              "as",
              "given",
              "but",
              "however",
              "\n",
              "nevertheless",
              "yet",
              "though",
              "although",
              ";"
            ]
          }
        }
      ],
      "direction": "TERMINATE",
      "max_targets": 1
    }
  ]
}