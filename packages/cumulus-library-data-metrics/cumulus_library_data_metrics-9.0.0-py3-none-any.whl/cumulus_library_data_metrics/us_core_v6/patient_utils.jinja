        FROM
            patient AS src,
            UNNEST(extension) AS p (ext),
            UNNEST(p.ext.extension) AS c (ext)
        WHERE
            p.ext.url = 'http://hl7.org/fhir/us/core/StructureDefinition/us-core-race'
            AND c.ext.url = 'ombCategory'
            AND c.ext.valuecoding.code IS NOT NULL
            AND c.ext.valuecoding.system IS NOT NULL
    )
    SELECT
        id,
        {% call utils.array_to_string() %}
          ARRAY_AGG(display)
        {% endcall %} AS display
    FROM flattened_race
    GROUP BY id
{% else %}
    SELECT id, '' AS display FROM patient WHERE 1=0  -- return an empty table
{% endif %}
)
{% endmacro %}


-- Extracts the right display codes to use (semicolon separated).
-- May contain missing rows, so you'll need to coalesce results.
-- http://hl7.org/fhir/us/core/STU6.1/StructureDefinition-us-core-tribal-affiliation.html
{% macro extract_has_tribal_affiliation(schema) -%}
(
{% set has_extension_codes = (
    schema["extension"]["url"]
    and schema["extension"]["extension"]["url"]
    and schema["extension"]["extension"]["valueCodeableConcept"]
) %}
{% if has_extension_codes %}
    WITH flattened AS (
        SELECT
            src.id,
            {{ utils.is_concept_valid('c.ext.valueCodeableConcept') }} AS valid_concept
        FROM
            patient AS src,
            UNNEST(extension) AS p (ext),
            UNNEST(p.ext.extension) AS c (ext)
        WHERE
            p.ext.url = 'http://hl7.org/fhir/us/core/StructureDefinition/us-core-tribal-affiliation'
            AND c.ext.url = 'tribalAffiliation'
            AND c.ext.valueCodeableConcept IS NOT NULL
    )
    SELECT
        id,
        BOOL_OR(valid_concept) AS has_tribal_affiliation
    FROM flattened
    GROUP BY id
{% else %}
    SELECT id, FALSE AS has_tribal_affiliation FROM patient WHERE 1=0  -- return an empty table
{% endif %}
)
{% endmacro %}


-- May contain missing rows, so you'll need to coalesce results.
-- http://hl7.org/fhir/us/core/STU6.1/StructureDefinition-us-core-birthsex.html
{% macro extract_birth_sex() -%}
(
    WITH flattened_birth_sex AS (
        SELECT
            src.id,

            CASE
            WHEN p.ext.valueCode = 'F'
            THEN 'Female'
            WHEN p.ext.valueCode = 'M'
            THEN 'Male'
            WHEN p.ext.valueCode = 'UNK'
            THEN 'Unknown'
            ELSE NULL -- this binding is required to be just the above values, ignore all else
            END AS display
        FROM
            patient AS src,
            UNNEST(extension) AS p (ext)
        WHERE
            p.ext.url = 'http://hl7.org/fhir/us/core/StructureDefinition/us-core-birthsex'
    )
    SELECT
        id,
        {% call utils.array_to_string() %}
          ARRAY_AGG(display)
        {% endcall %} AS display
    FROM flattened_birth_sex
    GROUP BY id
)
{% endmacro %}


-- May contain missing rows, so you'll need to coalesce results.
-- http://hl7.org/fhir/us/core/STU6.1/StructureDefinition-us-core-sex.html
{% macro extract_sex() -%}
(
    WITH flattened AS (
        SELECT
            src.id,

            CASE
            WHEN p.ext.valueCode = '248152002'
            THEN 'Female'
            WHEN p.ext.valueCode = '248153007'
            THEN 'Male'
            WHEN p.ext.valueCode = '33791000087105'
            THEN 'Nonbinary'
            WHEN p.ext.valueCode = 'asked-declined'
            THEN 'Asked but declined'
            ELSE NULL -- this binding is required to be just the above values, ignore all else
            END AS display
        FROM
            patient AS src,
            UNNEST(extension) AS p (ext)
        WHERE
            p.ext.url = 'http://hl7.org/fhir/us/core/StructureDefinition/us-core-sex'
    )
    SELECT
        id,
        {% call utils.array_to_string() %}
          ARRAY_AGG(display)
        {% endcall %} AS display
    FROM flattened
    GROUP BY id
)
{% endmacro %}


