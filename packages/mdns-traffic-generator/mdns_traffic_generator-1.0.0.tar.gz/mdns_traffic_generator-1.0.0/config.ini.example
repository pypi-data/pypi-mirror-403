# MDNS Traffic Generator Configuration
# This file is auto-generated. Use CLI commands to modify:
#   mdns-generator config set <section.key> <value>
#   mdns-generator config show
#   mdns-generator config reset

[service]
# Service instance name
name = test-service
# mDNS service type (e.g., _http._tcp.local.)
type = _http._tcp.local.
# Service port number (1-65535)
port = 8080

[network]
# Network interface to bind to
interface = 0.0.0.0
# Time-to-live for mDNS records in seconds
ttl = 4500

[traffic]
# Interval between service announcements in seconds
broadcast_interval = 5.0
# Number of services to register (1-100)
service_count = 1

[logging]
# Logging level: DEBUG, INFO, WARNING, ERROR, CRITICAL
level = INFO
# Optional log file path (comment out to disable file logging)
# file = mdns_generator.log
