<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>pointcloudcrafter</name>
  <version>1.2.0</version>
  <description>Point cloud crafter, modifier and analyzer tool for rosbags and local files.</description>
  <author email="markus.pielmeier@tum.de">Markus Pielmeier</author>
  <maintainer email="dominik.kulmer@tum.de">Dominik Kulmer</maintainer>
  <maintainer email="maxi.leitenstern@tum.de">Maximilian Leitenstern</maintainer>
  <license>Apache 2.0</license>

  <depend>geometry_msgs</depend>
  <depend>pcl_conversions</depend>
  <depend>rclcpp</depend>
  <depend>rosbag2_cpp</depend>
  <depend>sensor_msgs</depend>
  <depend>tf2_ros</depend>
  <depend>rosbag2_storage</depend>
  <depend>rosbag2_storage_mcap</depend>
  <depend>rosbag2_storage_default_plugins</depend>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_cmake_gtest</test_depend>

  <!--depend>autoware_cmake</depend-->

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
