pkg:
  id: "<pkg-id>"
  root: "/path/to/release/<pkg-id>"
  status: "open"

include:
  releases: []

git:
  repo_root: null  # optional override; default is git rev-parse from cwd
                  # pkgmgr.yaml 의 repo_root 값으로 자동 생성됩니다.
  # keywords are matched against commit messages; prefix is configurable in pkgmgr.yaml (git.keyword_prefix)
  keywords: []
  since: null
  until: null

collectors:
  enabled: ["checksums"]
