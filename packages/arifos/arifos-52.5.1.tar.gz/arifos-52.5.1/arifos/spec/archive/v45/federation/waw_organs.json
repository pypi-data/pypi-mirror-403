    "description": "W@W Federation configuration - 5 organs with domain veto power",
    "note": "No organ has supremacy. Psi vitality resolves conflicts (Amendment 3)."
  },
  "organs": {
    "PROMPT": {
      "id": 1,
      "symbol": "@PROMPT",
      "display": "Amanah Guardian",
      "role": "Integrity and mandate enforcement",
      "domain": {
        "primary": "Amanah (F1)",
        "secondary": [
          "Peace² (F5)"
        ]
      },
      "responsibilities": [
        "Verify all actions are reversible",
        "Enforce mandate boundaries (no credential theft, no irreversible ops)",
        "Protect system integrity",
        "Block destructive operations"
      ],
      "veto_powers": [
        {
          "type": "ABSOLUTE_VETO",
          "trigger": "Amanah = false (F1 violation)",
          "verdict": "VOID",
          "reason": "Irreversible or out-of-mandate operation detected"
        },
        {
          "type": "WARN_VETO",
          "trigger": "Peace² < 1.0 (destructive tendency)",
          "verdict": "PARTIAL",
          "reason": "Operation may cause harm or conflict"
        }
      ],
      "voting_criteria": {
        "PASS": "Amanah = true AND Peace² >= 1.0",
        "WARN": "Amanah = true AND Peace² < 1.0",
        "VETO": "Amanah = false"
      },
      "metrics": {
        "primary": "Amanah (boolean)",
        "thresholds": {
          "amanah": "LOCK (must be true)",
          "peace_squared": 1.0
        }
      }
    },
    "RIF": {
      "id": 2,
      "symbol": "@RIF",
      "display": "Reason Architect",
      "role": "Truth and clarity enforcement",
      "domain": {
        "primary": "Truth (F2)",
        "secondary": [
          "DeltaS (F4)",
          "Ω₀ (F7)"
        ]
      },
      "responsibilities": [
        "Verify factual accuracy",
        "Enforce lane-aware truth thresholds",
        "Ensure clarity gain (ΔS >= 0)",
        "Detect hallucinations and fabrications"
      ],
      "veto_powers": [
        {
          "type": "ABSOLUTE_VETO",
          "trigger": "Truth < lane_threshold",
          "verdict": "VOID",
          "reason": "Factual inaccuracy or hallucination detected",
          "lanes": {
            "PHATIC": 0.0,
            "SOFT": 0.8,
            "HARD": 0.9,
            "REFUSE": "instant VOID"
          }
        },
        {
          "type": "WARN_VETO",
          "trigger": "DeltaS < 0 (confusion increase)",
          "verdict": "PARTIAL",
          "reason": "Response increases confusion instead of clarity"
        }
      ],
      "voting_criteria": {
        "PASS": "Truth >= lane_threshold AND DeltaS >= 0",
        "WARN": "Truth >= lane_threshold AND DeltaS < 0",
        "VETO": "Truth < lane_threshold"
      },
      "metrics": {
        "primary": "Truth (lane-aware)",
        "thresholds": {
          "truth_phatic": 0.0,
          "truth_soft": 0.8,
          "truth_hard": 0.9,
          "delta_s": 0.0
        }
      }
    },
    "WELL": {
      "id": 3,
      "symbol": "@WELL",
      "display": "Care Auditor",
      "role": "Empathy and dignity protection",
      "domain": {
        "primary": "κᵣ (F6)",
        "secondary": [
          "Tri-Witness (F3)",
          "Peace² (F5)"
        ]
      },
      "responsibilities": [
        "Protect weakest stakeholder",
        "Enforce empathy thresholds (κᵣ >= 0.95)",
        "Prevent harm to vulnerable groups (children, minorities)",
        "Block doxxing and privacy violations"
      ],
      "veto_powers": [
        {
          "type": "ABSOLUTE_VETO",
          "trigger": "κᵣ < 0.95 (empathy floor fail)",
          "verdict": "VOID",
          "reason": "Response fails to serve weakest stakeholder"
        },
        {
          "type": "ABSOLUTE_VETO",
          "trigger": "child_harm pattern detected",
          "verdict": "VOID (NUCLEAR priority)",
          "reason": "Content targets vulnerable minors"
        },
        {
          "type": "WARN_VETO",
          "trigger": "Peace² < 1.0 (conflict amplification)",
          "verdict": "PARTIAL",
          "reason": "Response may escalate conflict"
        }
      ],
      "voting_criteria": {
        "PASS": "κᵣ >= 0.95 AND no child_harm",
        "WARN": "κᵣ >= 0.95 AND Peace² < 1.0",
        "VETO": "κᵣ < 0.95 OR child_harm detected"
      },
      "metrics": {
        "primary": "κᵣ (Empathy Conductance)",
        "thresholds": {
          "kappa_r": 0.95,
          "peace_squared": 1.0
        }
      },
      "red_patterns": [
        "child_harm (NUCLEAR)",
        "doxxing (SEVERE)"
      ]
    },
    "WEALTH": {
      "id": 4,
      "symbol": "@WEALTH",
      "display": "GENIUS Evaluator",
      "role": "Governed intelligence verification",
      "domain": {
        "primary": "GENIUS (F8)",
        "secondary": [
          "C_dark (F9)"
        ]
      },
      "responsibilities": [
        "Calculate G (Genius Index)",
        "Detect C_dark (Dark Cleverness)",
        "Ensure intelligence is governed (not manipulative)",
        "Block 'evil genius' patterns (high capability, low ethics)"
      ],
      "veto_powers": [
        {
          "type": "ABSOLUTE_VETO",
          "trigger": "G < 0.50 (ungoverned intelligence)",
          "verdict": "VOID",
          "reason": "Intelligence is not governed by ethics"
        },
        {
          "type": "ABSOLUTE_VETO",
          "trigger": "C_dark >= 0.60 (dark cleverness hazard)",
          "verdict": "VOID",
          "reason": "High capability without ethics (evil genius pattern)"
        },
        {
          "type": "WARN_VETO",
          "trigger": "0.50 <= G < 0.80 OR 0.30 <= C_dark < 0.60",
          "verdict": "PARTIAL",
          "reason": "Marginal governance or rising dark cleverness"
        }
      ],
      "voting_criteria": {
        "PASS": "G >= 0.80 AND C_dark < 0.30",
        "WARN": "0.50 <= G < 0.80 OR 0.30 <= C_dark < 0.60",
        "VETO": "G < 0.50 OR C_dark >= 0.60"
      },
      "metrics": {
        "primary": "G (Genius Index)",
        "thresholds": {
          "G_seal": 0.8,
          "G_void": 0.5,
          "C_dark_seal": 0.3,
          "C_dark_warn": 0.6
        }
      },
      "formulas": {
        "G": "normalize(A * P * E * X)",
        "C_dark": "normalize(A * (1-P) * (1-X) * E)"
      }
    },
    "GEOX": {
      "id": 5,
      "symbol": "@GEOX",
      "display": "Physics Enforcer",
      "role": "Session physics and resource limits",
      "domain": {
        "primary": "Session Physics",
        "secondary": [
          "Psi (Vitality)"
        ]
      },
      "responsibilities": [
        "Enforce budget limits (F1_budget)",
        "Detect burst patterns (F3_burst)",
        "Track failure streaks (F7_streak)",
        "Monitor Psi vitality (system health)"
      ],
      "veto_powers": [
        {
          "type": "ABSOLUTE_VETO",
          "trigger": "budget >= 1.00 (exhausted)",
          "verdict": "VOID",
          "reason": "Computational budget exhausted"
        },
        {
          "type": "ABSOLUTE_VETO",
          "trigger": "turn_rate > 30 OR entropy < 0.05",
          "verdict": "888_HOLD",
          "reason": "Burst or robotic pattern detected (verify human)"
        },
        {
          "type": "ABSOLUTE_VETO",
          "trigger": "failure_streak >= 3",
          "verdict": "888_HOLD",
          "reason": "3 consecutive failures - escalate to human"
        },
        {
          "type": "WARN_VETO",
          "trigger": "Psi < 0.95 (system unhealthy)",
          "verdict": "SABAR",
          "reason": "System vitality below threshold"
        }
      ],
      "voting_criteria": {
        "PASS": "budget < 0.80 AND no burst AND failure_streak < 3 AND Psi >= 1.0",
        "WARN": "0.80 <= budget < 1.00 OR Psi < 1.0",
        "VETO": "budget >= 1.00 OR burst detected OR failure_streak >= 3 OR Psi < 0.95"
      },
      "metrics": {
        "primary": "Psi (Vitality)",
        "thresholds": {
          "budget_warn": 0.8,
          "budget_hard": 1.0,
          "turn_rate_burst": 30,
          "entropy_robotic": 0.05,
          "failure_streak_max": 3,
          "psi_seal": 1.0,
          "psi_sabar": 0.95
        }
      }
    }
  },
  "voting_rules": {
    "principle": "No organ has supremacy. Each organ has veto power within its domain.",
    "vote_types": {
      "PASS": {
        "description": "Organ approves the response",
        "weight": 1,
        "contributes_to": "SEAL verdict"
      },
      "WARN": {
        "description": "Organ has concerns but doesn't block",
        "weight": 0,
        "contributes_to": "PARTIAL verdict"
      },
      "VETO": {
        "description": "Organ blocks the response",
        "weight": -1,
        "contributes_to": "VOID or 888_HOLD verdict"
      }
    },
    "aggregation": {
      "any_absolute_veto": "VOID or 888_HOLD (single veto blocks)",
      "mixed_warn_pass": "PARTIAL (concerns but not blocking)",
      "all_pass": "SEAL (unanimous approval)"
    },
    "hierarchy": {
      "note": "NO STATIC HIERARCHY (Amendment 3)",
      "conflict_resolution": "Psi vitality determines override",
      "rules": [
        "If multiple organs VETO, highest severity wins (NUCLEAR > SEVERE > MODERATE)",
        "If equal severity, Psi vitality score determines final verdict",
        "No organ can override another organ's domain veto"
      ]
    }
  },
  "amendment_3": {
    "title": "No Static Hierarchy",
    "text": "No organ shall have permanent precedence over another",
    "enforcement": {
      "forbidden": [
        "Hardcoded organ priority (e.g., @WELL always wins)",
        "Static veto override (e.g., @GEOX can override @PROMPT)",
        "Permanent tie-breaking rules"
      ],
      "allowed": [
        "Dynamic conflict resolution via Psi vitality",
        "Severity-based priority (NUCLEAR > SEVERE > MODERATE)",
        "Domain-specific veto power (within organ's jurisdiction)"
      ]
    },
    "conflict_resolution": {
      "step_1": "Check if conflict is within single organ's domain → that organ decides",
      "step_2": "If multi-domain conflict, compare verdict severities (SABAR > VOID > 888_HOLD > PARTIAL > SEAL)",
      "step_3": "If equal severity, compute Psi vitality score for each verdict",
      "step_4": "Select verdict with HIGHEST Psi vitality (healthiest system state)"
    },
    "examples": {
      "single_domain": {
        "scenario": "@PROMPT VETO (Amanah = false) vs @RIF PASS",
        "resolution": "@PROMPT wins (Amanah is its domain)",
        "verdict": "VOID"
      },
      "multi_domain": {
        "scenario": "@WELL WARN (κᵣ = 0.92) vs @WEALTH WARN (G = 0.75)",
        "resolution": "Both WARN → aggregate to PARTIAL",
        "verdict": "PARTIAL"
      },
      "severity_wins": {
        "scenario": "@WELL VETO (NUCLEAR child_harm) vs @RIF PASS",
        "resolution": "NUCLEAR severity > PASS → VETO wins",
        "verdict": "VOID (NUCLEAR priority)"
      },
      "psi_tiebreak": {
        "scenario": "@PROMPT VOID (Amanah = false) vs @GEOX 888_HOLD (burst)",
        "resolution": "Compute Psi for each verdict → higher Psi wins",
        "verdict": "Depends on Psi(VOID) vs Psi(888_HOLD)"
      }
    }
  },
