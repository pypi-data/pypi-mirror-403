# Port for listen (default: 7860)
PORT=7860
# whether to provide a web interface for downloads and cookie management in browser (default: False)
WEB_UI=True
# Mask Home Page UI - useful for deployment on HuggingFace Spaces (default: False)
HIDE_HOME=True
# Bearer token for authentication (default: None)
API_TOKEN=
# Directory for downloaded files (optional, default: None == auto)
DOWNLOADS_DIR=
# Folder with cookies.txt files (optional, default: None == auto)
COOKIES_FOLDER=
# Hours to keep files before cleanup (default: 0)
FILE_RETENTION_H=